    if(dynlen(config) == 0)
    {
         Log::error(\"loadConfiguration\", \"Empty array of configuration or values <%s>.\", dp);
         return;
    }

    this.adjustColumn(0);
    for(int i = 1; i <= dynlen(config); i++)
    {
      if(!initialized)
      {
        this.appendLine(\"Znak\", \"\", \"Objasnjenje\",\"\");
