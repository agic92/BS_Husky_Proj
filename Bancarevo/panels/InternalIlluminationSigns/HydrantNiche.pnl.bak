work(string dp, bool isTurnedOn) {
  if (isTurnedOn) {
    this.foreCol = \"red\";
    RECTANGLE1.backCol = \"red\";
    status.text = latinToCyrillic(\"STATUS: Osigura훾 oznake uklju훾en\");
  }
  else {
    this.foreCol = \"WF_Head\";
    status.text = latinToCyrillic(\"STATUS: Osigura훾 oznake isklju훾en\");
    RECTANGLE1.backCol = \"WF_Head\";
  }
}



