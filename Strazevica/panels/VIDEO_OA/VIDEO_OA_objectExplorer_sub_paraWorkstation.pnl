bool VIDEO_OA_objExpApplyParaChangesWorkstation() 
{   
  string defaultLive, defaultPlayback, exportPath, externalExportPath, playerPath;
  int    manualSpeed;
  bool   keyboard, joystick, jogShuttle, exportServer;
  dyn_string asDefinition;
 
  // check permission
  if (!VIDEO_OA_getUserPermission(\"perm_16\"))
  {
    VIDEO_OA_openNoPermission(\"perm_16\");
    return 1;
  }
  
  int transport ;
  
  switch (rb_transport.number)
  {
    case 1: transport = 1; break;
    case 2: transport = 2; break;
    case 3: transport = 5; break;
    default:  transport = 0; break;
  }
  
  defaultLive     = txt_defaultLive.text;
  defaultPlayback = txt_defaultPlayback.text;
  manualSpeed     = txt_manualSpeed.text;
  keyboard        = chk_keyboard.state(0);
  joystick        = chk_keyboard.state(1);  
  jogShuttle      = chk_keyboard.state(2);  
  exportServer    = chk_export.state(0);
  exportPath      = txt_exportPath.text;
  externalExportPath = txt_externalExportPath.text;
  playerPath = txt_playerPath.text;
  
  strreplace(exportPath, \"\\\\\", \"/\");
  strreplace(externalExportPath, \"\\\\\", \"/\");
  strreplace(playerPath, \"\\\\\", \"/\");
  
  asDefinition = makeDynString( \"type=WORKSTATION;para=1;isExportServer=\" + (int)exportServer + \";exportPath=\" + exportPath + 
                                                                                                 \";externalExportPath=\" + externalExportPath +
                                                                                                 \";playerPath=\" + playerPath +
                                                                                                 \";transport=\" + transport);
  
  strreplace(defaultLive, \"Stream \", \"\");  
  strreplace(defaultPlayback, \"Stream \", \"\");  
 
  updateLock = 1;  // lock backCol update for input fields
 
  dpSet( $OBJECT + \".widget.defaultStreamLive\",     defaultLive,
         $OBJECT + \".widget.defaultStreamPlayback\", defaultPlayback,
         $OBJECT + \".parameter.manualSpeed\",        manualSpeed,
         $OBJECT + \".keyboardAvailable\",            keyboard,
         $OBJECT + \".object.device.definition\",     asDefinition);

  // joystick and jog shuttle
  if(dpExists($OBJECT + \".joystickAvailable\"))
     dpSet( $OBJECT + \".joystickAvailable\", joystick);
            
  if(dpExists($OBJECT + \".jogShuttleAvailable\"))
     dpSet( $OBJECT + \".jogShuttleAvailable\", jogShuttle);  
  
  
  txt_defaultLive.backCol     = \"\";
  txt_defaultPlayback.backCol = \"\";
  txt_manualSpeed.backCol     = \"\";

  VIDEO_OA_objExpUnlockTree( $MAINMODULE, $MAINPANEL);  
  delay(0,100);
  updateLock = 0;  // unlock backCol update for input fields
  return 0;
}
//----------------------------------------------------------------


VIDEO_OA_objExpUpdateParaWorkstation() 
{
 int defaultLive, defaultPlayback, manualSpeed;
 bool keyboard, joystick, jogShuttle, exportServer;
 string exportPath, externalExportPath, playerPath;
 dyn_string definition;
 
 updateLock = 1;  // lock backCol update for input fields
  
  
  dpGet( $OBJECT + \".widget.defaultStreamLive\",     defaultLive,
         $OBJECT + \".widget.defaultStreamPlayback\", defaultPlayback,
         $OBJECT + \".parameter.manualSpeed\",        manualSpeed,
         $OBJECT + \".keyboardAvailable\",            keyboard,
         $OBJECT + \".object.device.definition\",     definition);
  
  
  // joystick and jog shuttle
  if(dpExists($OBJECT + \".joystickAvailable\"))
     dpGet( $OBJECT + \".joystickAvailable\", joystick);
            
  if(dpExists($OBJECT + \".jogShuttleAvailable\"))
     dpGet( $OBJECT + \".jogShuttleAvailable\", jogShuttle);
  
  
  
  VIDEO_OA_formatDeviceDefinition(definition);
  exportPath = VIDEO_OA_filterKeywordValue(definition[1], \"exportPath\");
  externalExportPath = VIDEO_OA_filterKeywordValue(definition[1], \"externalExportPath\");
  playerPath = VIDEO_OA_filterKeywordValue(definition[1], \"playerPath\"); 
  exportServer = VIDEO_OA_filterKeywordValue(definition[1], \"isExportServer\");
  
  string transport = VIDEO_OA_filterKeywordValue(definition[1], \"transport\");

  switch (transport)
  {
    case \"1\": rb_transport.number = 1; break;
    case \"2\": rb_transport.number = 2; break;
    case \"5\": rb_transport.number = 3; break;
    default: rb_transport.number = 0;
  }

  
  defaultLive     = (defaultLive == 0)      ? 1:defaultLive;
  defaultPlayback = (defaultPlayback == 0)  ? 1:defaultPlayback;
  manualSpeed     = (manualSpeed == 0)      ? 20:manualSpeed;
  
  txt_defaultLive.text     = \"Stream \" + defaultLive;
  txt_defaultPlayback.text = \"Stream \" + defaultPlayback;
  txt_manualSpeed.text     = manualSpeed ;
  
  txt_exportPath.text = exportPath;
  txt_externalExportPath.text = externalExportPath;
  txt_playerPath.text = playerPath;  
  
  txt_defaultLive.backCol     = \"\";
  txt_defaultPlayback.backCol = \"\";
  txt_manualSpeed.backCol     = \"\";
  
  chk_keyboard.state(0) = keyboard;
  chk_keyboard.state(1) = joystick;
  chk_keyboard.state(2) = jogShuttle;
  
  chk_export.state(0) = (int)exportServer;
  
  VIDEO_OA_objExpUnlockTree( $MAINMODULE, $MAINPANEL);  
  delay(0,100);
  updateLock = 0;  // unlock backCol update for input fields
}" 0
