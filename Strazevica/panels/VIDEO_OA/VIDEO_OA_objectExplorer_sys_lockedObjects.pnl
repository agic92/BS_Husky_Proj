E "VIDEO_OA_lockedObjectsUpdateTab()
{
 dyn_string asFolder;
 int i;
 
 table.deleteAllLines();
	table.tableMode=TABLE_SELECT_BROWSE ;
 table.selectByClick(TABLE_SELECT_LINE);

 dpGet($SYSTEM+\"_VIDEO_OA_MAIN.lockedObjects\",asFolder);
	
	for(i=1;i<=dynlen(asFolder);i++)
	 table.appendLine(\"Folder\",asFolder[i]);
	                     

	 table.selectLineN(0);
	 table.lineVisible(0);
  table.backCol = \"\";
  txt_element.text = \"\";
}


VIDEO_OA_lockedObjectsApply()
{
  dyn_string asZeile,asFolder,asFolder_check;
  int i;
  
    // check permission
  if (!VIDEO_OA_getUserPermission(\"perm_1\"))
  {
     VIDEO_OA_openNoPermission(\"perm_1\");
     return;     
  }
  shape table = getShape(\"table\");
  for(i = 0; i <= table.lineCount-1; i++)
  { 
     asZeile = table.getLineN(i);
     asFolder[i+1] = asZeile[1];
  }
  
  dynUnique(asFolder);
   
  if (dynlen(asFolder) == 1) // mabe only empty fields
  {
     if(asFolder[1] == \"\")
       dynClear(asFolder);
  }
  
  if(dynlen(asFolder)>=1) 
  {
     for(int i=1;i<=dynlen(asFolder);i++)
     {
        if(asFolder[i]!=\"\")
          dynAppend(asFolder_check,asFolder[i]);
     }
  }
  
  dpSet($SYSTEM+\"_VIDEO_OA_MAIN.lockedObjects\", asFolder_check);
  table.backCol = \"\";
  
  VIDEO_OA_lockedObjectsUpdateTab();
}
