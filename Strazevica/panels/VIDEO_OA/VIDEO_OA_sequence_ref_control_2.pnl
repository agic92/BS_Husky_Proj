work(string dp1, int mode, string dp2, int lastPosition, string dp3, int periodTime)
{
  txt_position.text = lastPosition;  
  cb_time.text = periodTime;

  if (mode == 1)
  {
    state_run.visible = 1;
    state_pause.visible = 0;
    state_stop.visible = 0;
    noPara.visible = 0;    
  }
  else if (mode == 0)
  {
    state_run.visible = 0;
    state_pause.visible = 1;
    state_stop.visible = 0;
    noPara.visible = 0;
  }
  else
  {
    state_run.visible = 0;
    state_pause.visible = 0;
    state_stop.visible = 1;
    noPara.visible = 1;
  }
  
  state.backCol = VIDEO_seq_getModeCol(mode);   
}" 0
