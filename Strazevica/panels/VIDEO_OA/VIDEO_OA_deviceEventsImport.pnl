"main()
{
  int i; 
  string dp, element;
  dyn_string s, asParameter, asParameterParts;
  string sResult;
  bool permAdmin = VIDEO_OA_getUserPermission(\"perm_1\");
  string sFilename = DATA_PATH + playlist.selectedText;
  int iAnswer, iLength;

  buDelete.enabled = 0;
  refresh.enabled = 0;
  this.enabled = 0;
  this.backCol = \"\";
  
  if (playlist.selectedText == \"\")
  {

    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"noFileSelected\"));
  }
  else if(permAdmin)
  {
    VIDEO_OA_openMessageInfoAnswer(getCatStr(\"voa_general\", \"reallyImportFile\"), 1, iAnswer);
  
    if(iAnswer == 1)
    {
      fileToString(sFilename, sResult);
      strreplace(sResult, \"\\t\", \"|\");         // delete all \\t
      strreplace(sResult, \"\\\"\", \"\");          // delete all \" ( maybe inside from excel)
      asParameter = strsplit(sResult, \"\\n\");  // make dyn string
      dynRemove(asParameter, 1);              // delete header
    
      iLength = dynlen(asParameter);
    
      for( int i = 1; i <= iLength; i++)
      {
        dyn_string dpPara;
        asParameterParts = strsplit(asParameter[i], \"|\");
         //DebugN(asParameterParts);
         
         for (int ii = 2; ii <= dynlen(asParameterParts); ii = ii + 2)
         {
           // create alarm para field [1]\"keyword|dpe\"..
           if(asParameterParts[ii] != \"\")
            dynAppend(dpPara, asParameterParts[ii] + \"|\" + asParameterParts[ii + 1]);
         }
        dpSet(asParameterParts[1] + \".object.parameter.alarm.para\", dpPara);
      }
      VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\", \"fileImportSuccessful\"));
    }
  }
  else
    VIDEO_OA_openNoPermission();
  
  buDelete.enabled = 1;
  refresh.enabled = 1;
  this.enabled = 1;;
}


