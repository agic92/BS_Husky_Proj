"main()
{

  string sSource = externalFolder.text;
  dyn_int aiLines;
  string sColor;
  dyn_string asValues;
  dyn_dyn_string result;
  
  this.enabled = 0;
  folderTree.enabled = 0;
  
    // check permission
  if(!VIDEO_OA_getUserPermission(\"perm_25\"))
   {
     VIDEO_OA_openNoPermission(\"perm_25\");
     folderTree.enabled = 1;
     this.enabled = 1;
     return;
   }
  
  getValue (\"exportTab2\", \"getSelectedLines\", aiLines);
  for(int i = 1; i <= dynlen(aiLines); i++)
  {
    getValue (\"exportTab2\", \"getLineN\", aiLines[i], result[i]);
  }
  
  if(dynlen(aiLines) == 0)
  {
    VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\", \"noLineSelected\"));
    this.enabled = 1;
    folderTree.enabled = 1;
    return;
  }
  else
     VIDEO_OA_setMyModuleEnabled(0);
  
  
  //==================
  
  for(int i=1;i<=dynlen(aiLines);i++)
  {    
   int iAnswer;

    VIDEO_OA_openMessageInfoAnswer(getCatStr(\"voa_general\",\"export_reallyDeleteExport\") + \"\\n\\n\" + getCatStr(\"voa_general\",\"exportName\")  + \" :\" + result[i][2], 1, iAnswer);                                                                  
                                                                     
    if(iAnswer == 1)    
     VIDEO_OA_deleteFolder(result[i][6]);
    else
    {
      VIDEO_OA_setMyModuleEnabled(1);
      this.enabled = 1;
      folderTree.enabled = 1;
      return;
    }
  }
  
  //==================
  
  delay(2);
  VIDEO_OA_exportUpdateExportFolder(\"externalFolder\") ;

  this.enabled = 1;
  folderTree.enabled = 1;
  
  VIDEO_OA_setMyModuleEnabled(1);
}

