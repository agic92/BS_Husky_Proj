  getValue (\"exportTab2\", \"getSelectedLines\", aiLines);
  for(int i = 1; i <= dynlen(aiLines); i++)
  {
    getValue (\"exportTab2\", \"getLineN\", aiLines[i], result[i]);
  }
  
  if(dynlen(aiLines)==0)
