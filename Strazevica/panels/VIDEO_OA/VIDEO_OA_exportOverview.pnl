E "VIDEO_OA_exportUpdateExportFolder(string shapeName)  
{
  shape folderText = getShape(shapeName);
  string folder = folderText.text;

  dpSet(VIDEO_OA_getWorkstationDp(myManNum()) + \".exportServer.object.command.request\", \"cmd=dirinfo;path=\" + folder);
}

VIDEO_OA_exportUpdateTab(dyn_string dirinfo, string shapeName)
{
  shape tab = getShape(shapeName);
  float sizeComplete;
  
  tab.deleteAllLines();
  tab.selectByClick(TABLE_SELECT_LINE);
  tab.tableMode(TABLE_SELECT_MULTIPLE);
  tab.sortOnClick = 0;
  
  for(int i = 2 ; i <= dynlen(dirinfo); i++)
  {  
    dyn_string exportParts = strsplit(dirinfo[i], \",\");
    string date, begin, end;
    string folderName = dirinfo[1] + \"/\" + exportParts[1];
       
    strreplace(folderName, \"\\\\\", \"/\");
    
    int iDate  = (int)exportParts[2];
    int iBegin = (int)exportParts[4];
    int iEnd   = (int)exportParts[5];
    float size = (float)exportParts[3];
    //DebugN(\"SIZE\",size);
    sizeComplete = sizeComplete + size;
    string sSize;
    size = size / 1024.0 ; // -> kByte to Mega Byte
    sprintf(sSize, \"%6.2f\", size);
    
    
    
    string sDate  = formatTime(\"%d.%m.%Y  %H:%M:%S\",(time)iDate);
    string sBegin = formatTime(\"%d.%m.%Y  %H:%M:%S\",(time)iBegin);
    string sEnd   = formatTime(\"%d.%m.%Y  %H:%M:%S\",(time)iEnd);
    
    tab.appendLine(\"#2\", exportParts[1], \"#1\", sDate, \"#3\", sBegin, \"#4\", sEnd, \"#5\", sSize + \" MB\", \"#6\", folderName, \"#7\", iDate);
    tab.cellWidgetRC(i-2, \"#8\", \"PushButton\", \">\");                    // play button
    tab.sort(\"#7\");
  }
  string sSizeComplete;
  sizeComplete = sizeComplete / 1024 ; // -> kByte to Mega Byte
  sprintf(sSizeComplete, \"%6.2f\", sizeComplete);
  setValue(shapeName + \"_size\", \"text\", sSizeComplete + \" MB\");
}


VIDEO_OA_moveFolder(string source, string destination)
{
  string sCmdLine;
  int result;
  if(_WIN32) // ok
  {
    sCmdLine=\"move \" + source + \" \" + destination;
    strreplace(sCmdLine, \"/\", \"\\\\\");
    //DebugN(sCmdLine);
    result = system(sCmdLine);
  }
  else if(_UNIX) // ?
  {
    sCmdLine = \"mv \" + source + \" \" + destination;
    strreplace(sCmdLine, \"\\\\\", \"/\");
    result = system(sCmdLine + \" -f\");
  }
  if(result != 0)
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"fileMoveError\"));
  delay(2);  
} 

VIDEO_OA_copyFolder(string source, string destination)
{ 
  string sCmdLine;
  int result;

  if(_WIN32) // ok
  {
    strreplace(source, \"/\", \"\\\\\");
    strreplace(destination, \"/\", \"\\\\\");
    result = copyAllFilesRecursive(source, destination);
  }
  else if(_UNIX) // ?
  {
    strreplace(sCmdLine, \"\\\\\", \"/\");
    result = copyAllFilesRecursive(source, destination);

  }
  if(result != 1)
    VIDEO_openMessageWarning(getCatStr(\"video\",\"fileCopyError\"));
  delay(2);          
}

VIDEO_OA_deleteFolder(string source)
{ 
  string sCmdLine;
  int result;
  if(_WIN32) // ok
  {
    strreplace(source, \"/\", \"\\\\\");
    sCmdLine= \"rd /s /q \" + source;

    result = system(sCmdLine);
  }
  else if(_UNIX) // ?
  {   
    strreplace(sCmdLine, \"\\\\\", \"/\");
    sCmdLine=\"rm -rf \" + source;
    
    result = system(sCmdLine);
  }
  if(result != 0)
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"fileRemoveError\"));
  delay(2);
}
   
    
       
    
  




    
