"main()
{
  string answer;
  dyn_dyn_string tab;
  dyn_string names;
  
  if (!VIDEO_OA_getUserPermission(\"perm_3\"))
  {
    VIDEO_OA_openNoPermission(\"perm_3\");
    return;
  }
  
  VIDEO_OA_openMessageInput(getCatStr(\"voa_general\",\"paraList_configName\"),\"%s\",\"\",answer);
  
  tab = VIDEO_OA_paraList_getAll(\"cam_list\");
  
  for (int i=2; i<=dynlen(tab); i++)
  {
    dynAppend(names, tab[i][2]);        
  }

  if( dynContains(names, answer) > 0 )
    VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"paraList_configAlreadyExists\"));
  else if(answer != \"\")
  {
    string dp = VIDEO_OA_paraList_getNextName();
    dpCreate(dp, \"_VIDEO_OA_PARAMETER_LIST\");  
    dpSet( dp + \".parameter\", makeDynString(\"\"),
           //dp + \".list\", makeDynString(\"\"),
           dp + \".type\", \"cam_list\",
           dp + \".name\", answer);
    
    VIDEO_OA_paraList_updateList (\"cam_list\", \"txt_configs\");
    

    dyn_string items = txt_configs.items;
    
    int actPos;
    for (int i = 1; i <= dynlen(items); i++)
    {
      if ( items[i] == answer )
        actPos = i;
    }
    
    txt_configs.selectedPos = actPos;
 
    cover_config.visible = 0;
    cover_config_text.visible = 0;
    
    VIDEO_OA_paraList_updateSequenceConfig();
  }
  else
    VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"paraList_configEmptyName\")); 
}


