"main()
{
  dyn_string number     = seq_list.getColumnN(0);
  dyn_string systemName = seq_list.getColumnN(1); 
  dyn_string camNum     = seq_list.getColumnN(2);
  dyn_string items      = seq_list.getColumnN(3);
  dyn_string dpName     = seq_list.getColumnN(4);
  dyn_string pos        = seq_list.getColumnN(5);
  

  dyn_string systemNameNew = systemName;
  dyn_string camNumNew     = camNum;
  dyn_string itemsNew      = items;
  dyn_string dpNameNew     = dpName;
  dyn_string posNew        = pos;
  
  dyn_int selectedLines = seq_list.getSelectedLines();
  int selectedPos;
    
  if (dynlen(selectedLines) > 0)
  {
    selectedPos = selectedLines[1] +1;
    if (selectedPos > 1)
    {
      itemsNew[selectedPos - 1] = items [selectedPos    ];
      itemsNew[selectedPos    ] = items [selectedPos - 1];
      
      posNew[selectedPos - 1] = pos [selectedPos    ];
      posNew[selectedPos    ] = pos [selectedPos - 1];
      
      systemNameNew[selectedPos - 1] = systemName [selectedPos    ];
      systemNameNew[selectedPos    ] = systemName [selectedPos - 1];
      
      camNumNew[selectedPos - 1] = camNum [selectedPos    ];
      camNumNew[selectedPos    ] = camNum [selectedPos - 1];
      
      dpNameNew[selectedPos - 1] = dpName [selectedPos    ];
      dpNameNew[selectedPos    ] = dpName [selectedPos - 1];
         
      seq_list.deleteAllLines();
      
      for (int i = 1; i <= dynlen(itemsNew); i++)
      {
        seq_list.appendLine(\"#5\", seq_list.lineCount + 1 , \"#1\", itemsNew[i], \"#4\", posNew[i], \"#6\", systemNameNew[i], \"#7\",camNumNew[i], \"#8\", dpNameNew[i] );
        seq_list.cellWidgetRC((seq_list.lineCount) - 1, \"#4\", \"ComboBox\", VIDEO_OA_paraList_createPresetComboBox(itemsNew[i]));
      } 

      seq_list.selectLineN() = selectedPos -2;       
      
      setInputFocus(myModuleName(), myPanelName(),\"seq_list\");      
    }
  }
}
