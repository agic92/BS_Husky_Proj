"main()
{
  dyn_string names;
  dyn_uint ids;  
  string ownSystem = getSystemName();
  strreplace(ownSystem, \":\", \"\");
  getSystemNames(names, ids);
  
  dynRemove(names, dynContains(names,ownSystem));
  this.registerName(0, ownSystem);
  this.registerHeader(0, ownSystem);
  
  this.registerPanel(0, \"VIDEO_OA/VIDEO_OA_cameraListForSystem.pnl\", makeDynString(\"$PANELNAME:\" + myPanelName(), \"$SYSTEM:\" + ownSystem + \":\",\"$TABLEMODE:\" + TABLE_SELECT_MULTIPLE ));
  
  for(int i = 1; i <= dynlen(names); i++)
  {
    this.insertRegister(1);
    this.registerName(1, names[i]);
    this.registerHeader(1, names[i]);
    this.registerPanel(1, \"VIDEO_OA/VIDEO_OA_cameraListForSystem.pnl\", makeDynString(\"$PANELNAME:\" + myPanelName(), \"$SYSTEM:\" + names[i] + \":\",\"$TABLEMODE:\" + TABLE_SELECT_MULTIPLE ));
  }
  
