//----------------------------------------------------------------
/*
  VIDEO_OA_objExpFillFunctionTab(string object, string element = \".object.device.link\")
  function to fill function table for Type VIDEO_OA_HOST
  @author MN

  @param object     : object Name
  @param element    : element name (default:.object.device.alarm) 
*/
VIDEO_OA_objExpFillFunctionTab(string object, string element = \".object.device.link\")
{
  dpConnect(\"VIDEO_OA_objExpFillFunctionTab_work\", object+element);
}
//----------------------------------------------------------------



//----------------------------------------------------------------
/*
  VIDEO_OA_objExpFillFunctionTab_work(string dp1, string sAlarm)
  work function for VIDEO_OA_objExp_fillFunctionTab()
  @author MN

*/
VIDEO_OA_objExpFillFunctionTab_work(string dp1, dyn_string asLink)
{
  VIDEO_OA_formatDeviceDefinition(asLink);
  dyn_dyn_string result = VIDEO_OA_objExpObjectInfoToDynString(asLink);
  dyn_string showResultKey,showResultValue;
  dyn_errClass err;
  
  
  for(int i = 1 ; i <= dynlen(result); i ++)
  {
    if (strpos(result[i][1],\"is\",0) >= 0)
    {  
      string sAlarmText = getCatStr(\"voa_general\", \"function_txt_\" + result[i][1]);
      err = getLastError();
      if (dynlen(err) == 0) //no error-> cat string found
      {
        result[i][1] = sAlarmText; 
        dynAppend(showResultKey, result[i][1]);
        dynAppend(showResultValue, result[i][2]); // append only if cat string found
      }
       else
        result[i][1] = result[i][1];
    }  
  }
            
            
  tab_function.deleteAllLines();
  tab_function.columnFormat(\"#1\", \"[0s,,,ALIGNMENT_BEGINNING]\");
  tab_function.columnFormat(\"#2\", \"[0s,,,ALIGNMENT_CENTER]\");
  tab_function.appendLines(dynlen(showResultKey), \"#1\", showResultKey, \"#2\", showResultValue);

}
