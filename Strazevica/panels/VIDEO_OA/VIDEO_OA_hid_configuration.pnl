E "#uses \"VIDEO_OA_hidProduct.ctl\"

mapping g_mJoystickVendors;
mapping g_mJoystickProducts;
mapping g_mJogshuttleVendors;
mapping g_mJogshuttleProducts;

dyn_string actualPlaybackCams;
bool bFirstTimelineUpdate = true;
string g_timelineStartTS;

int g_LiveCam     = 3;
int g_PlaybackCam = 3;

void InitHidCtrlExtension()
{
  SELECTION_LIST_JOYSTICK.deleteAllItems();
  SELECTION_LIST_JOGSHUTTLE.deleteAllItems();
    
  VIDEO_OA_HID_init();
  VIDEO_OA_HID_closeAll();
  VIDEO_OA_HID_getDevices();
}

 VIDEO_OA_HID_deviceCB(int iVendor, int iProduct, string sDeviceName, int iDeviceType)
{
  //DebugTN(\"VIDEO_OA_HID_deviceCB\");
  
  switch(iDeviceType)
  {
    case HID_DEVICE_TYPE_JOYSTICK:
    {
      SELECTION_LIST_JOYSTICK.appendItem(sDeviceName);
      g_mJoystickVendors[sDeviceName] = iVendor;
      g_mJoystickProducts[sDeviceName] = iProduct;
      break;
    }
    case HID_DEVICE_TYPE_JOGSHUTTLE:
    {
      SELECTION_LIST_JOGSHUTTLE.appendItem(sDeviceName);
      g_mJogshuttleVendors[sDeviceName] = iVendor;
      g_mJogshuttleProducts[sDeviceName] = iProduct;
      break;
    }
  }
}


VIDEO_OA_HID_valueCB(int iValue, int iDevice, string sParam)
{
  //DebugTN(\"VIDEO_OA_HID_valueCB\");

  switch(iDevice)
  {
    // the joystick is always device 1
    case HID_DEVICE_TYPE_JOYSTICK:  
    { 
      if(sParam==\"X\" || sParam==\"Y\" || sParam==\"Z\" || sParam==\"Rx\" || sParam==\"Ry\" || sParam==\"Rz\")
      {
        setValue(\"JOYSTICK_Axis_\" + sParam, \"text\", iValue);
        setValue(\"JOYSTICK_Axis_\" + sParam, \"backCol\", ((iValue != 0) ? \"Video_state_active\" : \"Video_state_idle\"));
      }
      else if(sParam==\"Slider\")
      {
        setValue(\"JOYSTICK_Slider_1\", \"text\", iValue);
        setValue(\"JOYSTICK_Slider_1\", \"backCol\", ((iValue != 0) ? \"Video_state_active\" : \"Video_state_idle\"));
      }
      else if(sParam==\"Hat Switch\")
      {
        setValue(\"JOYSTICK_HatSwitch_1\", \"text\", iValue);
        setValue(\"JOYSTICK_HatSwitch_1\", \"backCol\", ((iValue != 0) ? \"Video_state_active\" : \"Video_state_idle\"));
      }
      else if(strpos(sParam, \"Button\") >= 0)
      {
        setValue(\"JOYSTICK_\" + sParam, \"backCol\", ((iValue == 1) ? \"Video_state_active\" : \"Video_state_idle\"));
      }
      break;
    }
      
    // the jogShuttle is always device 2
    case HID_DEVICE_TYPE_JOGSHUTTLE:
    {
      if(sParam==\"Dial\" || sParam==\"Wheel\")
      {
        setValue(\"JOGSHUTTLE_\" + sParam, \"text\", iValue);
        setValue(\"JOGSHUTTLE_\" + sParam, \"backCol\", ((iValue != 0) ? \"Video_state_active\" : \"Video_state_idle\"));
      }
      else if(strpos(sParam, \"Button\") >= 0)
      {
        setValue(\"JOGSHUTTLE_\" + sParam, \"backCol\", ((iValue == 1) ? \"Video_state_active\" : \"Video_state_idle\"));
      }
      break;
    }
      
    default:
    {
        break;
    }
  }
  
