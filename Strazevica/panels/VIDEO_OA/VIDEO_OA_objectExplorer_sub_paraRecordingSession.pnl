bool VIDEO_OA_objExpApplyParaChangesRecordingSession() 
{   
  
  dyn_string asDefinition = makeDynString( \"type=RECORDING_SESSION;para=1\" );
  string sLink;
 
    // check permission
  if (!VIDEO_OA_getUserPermission(\"perm_16\"))
  {
    VIDEO_OA_openNoPermission(\"perm_16\");
    return 1;
  }
  
  int recordingMode = box_recordingMode.number;
  int redundancyMode = box_redundancyMode.number;
  
  if(txt_key_path1.text == \"\")
  {
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_path1\"));
    return 1;
  }
  
  if ( (txt_key_path2.text == \"\") && (redundancyMode != 0) )
  {
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_path2\"));
    return 1;
  }
  
  if ( (txt_key_path1.text == txt_key_path2.text) && (redundancyMode != 0) && (txt_key_recordingServer1.text == txt_key_recordingServer2.text) )
  {
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_path3\"));
    return 1;
  }
  
  string multipleRecordingSession;
  multipleRecordingSession = VIDEO_OA_objExpCheckMultipleRecordingSettings( txt_key_recordingServer1.text,  txt_key_path1.text, $OBJECT);
  
  if (multipleRecordingSession != \"\")
  {
    string recordingSessionErrorText = getCatStr(\"voa_general\", \"para_error_recordingSession\");
    
    strreplace(recordingSessionErrorText, \"$SESSION\", multipleRecordingSession);
    strreplace(recordingSessionErrorText, \"$NUMBER\", \"1\");
    VIDEO_OA_openMessageWarning(recordingSessionErrorText);
    return 1;  
  }
  
  multipleRecordingSession = VIDEO_OA_objExpCheckMultipleRecordingSettings( txt_key_recordingServer2.text,  txt_key_path2.text, $OBJECT);
  
  if (multipleRecordingSession != \"\")
  {
    string recordingSessionErrorText = getCatStr(\"voa_general\", \"para_error_recordingSession\");
    
    strreplace(recordingSessionErrorText, \"$SESSION\", multipleRecordingSession);
    strreplace(recordingSessionErrorText, \"$NUMBER\", \"2\");
    VIDEO_OA_openMessageWarning(recordingSessionErrorText);
    return 1;  
  }
  
  string path1 = txt_key_path1.text;
  string path2 = txt_key_path2.text;
  strreplace(path1, \"\\\\\", \"/\");
  strreplace(path2, \"\\\\\", \"/\"); 
   
  dyn_int parts_prealarm;
  dyn_int parts_postalarm;
  dyn_int parts_autoDelete;
  dyn_int parts_ringSize;
  
  parts_autoDelete[4] = alarm_day.text;
  parts_autoDelete[3] = alarm_hour.text;
  parts_autoDelete[2] = alarm_minute.text;
  parts_autoDelete[1] = 0; 
                        
  parts_ringSize[4] = ring_day.text;
  parts_ringSize[3] = ring_hour.text; 
  parts_ringSize[2] = ring_minute.text; 
  parts_ringSize[1] = 0;
      
  parts_prealarm[4] = 0;
  parts_prealarm[3] = before_hour.text;
  parts_prealarm[2] = before_minute.text;
  parts_prealarm[1] = before_second.text;
    
  parts_postalarm[4] = 0; 
  parts_postalarm[3] = after_hour.text;
  parts_postalarm[2] = after_minute.text;
  parts_postalarm[1] = after_second.text; 

  string prealarm = VIDEO_OA_objExpPartsToTime(parts_prealarm);
  string postalarm = VIDEO_OA_objExpPartsToTime(parts_postalarm);
  string autoDelete = VIDEO_OA_objExpPartsToTime(parts_autoDelete);
  string ringSize = VIDEO_OA_objExpPartsToTime(parts_ringSize); 
  
  string singleLink = \";recordingServer1=\" + txt_key_recordingServer1.text + \";path1=\" + path1 + \";\";
  string reduLink = singleLink + \"recordingServer2=\" + txt_key_recordingServer2.text + \";path2=\" + path2 + \";\";
  
  string alarmLink = \";prealarm=\" + prealarm + \";postalarm=\" + postalarm + \";autoDelete=\" + autoDelete + \";\";
  string ringLink = \";ringSize=\" + ringSize;
  
  switch(recordingMode)
  {
    case 0: sLink = sLink + \";\" + \"recordingMode=RING\" + ringLink; break;
    case 1: sLink = sLink + \";\" + \"recordingMode=ALARM\" + alarmLink; break;
    case 2: sLink = sLink + \";\" + \"recordingMode=LINEAR\" ; break;
    default: sLink = sLink + \";\" + \"recordingMode=RING\" + ringLink;
  }
 
  switch(redundancyMode)
  {
    case 0: sLink = sLink + \";\" + \"redundancyMode=NONE\" + singleLink; break;
    case 1: sLink = sLink + \";\" + \"redundancyMode=LOAD_BALANCING\" + reduLink; break;
    case 2: sLink = sLink + \";\" + \"redundancyMode=FULL_REDUNDANCY\" + reduLink; break;
    default: sLink = sLink + \";\" + \"redundancyMode=NONE\";
  }
 
  updateLock = 1;  // lock backCol update for input fields
   
  sLink = sLink + \";\" + VIDEO_OA_objExpGetLinkInfosFromCheckBox(\"chk_1\", makeDynString(\"mode\"));
  
  sLink = sLink + \";\" + \"function=SERVER\" + \";\";
  
  strreplace(sLink,\";;\",\";\");
  
  dpSet( $OBJECT + \".object.device.link\",       makeDynString(sLink, sLink, sLink),
         $OBJECT + \".object.device.definition\", asDefinition,
         $OBJECT + \".object.device.controller\", makeDynString(\"\", \"\", \"\"));
  

  txt_definition.backCol = \"\";  
  VIDEO_OA_objExpUnlockTree( $MAINMODULE, $MAINPANEL);  
  delay(0, 200);
  updateLock = 0;  // unlock backCol update for input fields
  return 0;
}
