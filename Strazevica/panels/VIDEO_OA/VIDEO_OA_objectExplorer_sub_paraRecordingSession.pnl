VIDEO_OA_objExpUpdateParaRecordingSession()
{
  dyn_string asLink, asDefinition, asController;
  
  updateLock = 1;  // lock backCol update for input fields
  
  dpGet( $OBJECT + \".object.device.link\", asLink,
         $OBJECT + \".object.device.definition\", asDefinition,
         $OBJECT + \".object.device.controller\", asController);
  
  VIDEO_OA_formatDeviceDefinition(asDefinition);  
  VIDEO_OA_formatDeviceLink(asLink);   
  VIDEO_OA_formatControllerLink(asController);
  

   
  string recordingMode =  VIDEO_OA_filterKeywordValue( asLink[1], \"recordingMode\");
  string redundancyMode = VIDEO_OA_filterKeywordValue( asLink[1], \"redundancyMode\");
  
  string prealarm =   VIDEO_OA_filterKeywordValue( asLink[1], \"prealarm\");
  string postalarm =  VIDEO_OA_filterKeywordValue( asLink[1], \"postalarm\");
  string autoDelete = VIDEO_OA_filterKeywordValue( asLink[1], \"autoDelete\");
  string ringSize =   VIDEO_OA_filterKeywordValue( asLink[1], \"ringSize\");
 
  prealarm   = (prealarm == \"\")   ? 30 : prealarm; // default if empty
  postalarm  = (postalarm == \"\")  ? 30 : postalarm; // default if empty
  autoDelete = (autoDelete == \"\") ? 7200 : autoDelete; // default if empty
  ringSize   = (ringSize == \"\")   ? 7200 : ringSize; // default if empty
  
  dyn_int parts_prealarm = VIDEO_OA_objExpTimeToParts(prealarm);
  dyn_int parts_postalarm = VIDEO_OA_objExpTimeToParts(postalarm);
  dyn_int parts_autoDelete = VIDEO_OA_objExpTimeToParts(autoDelete);
  dyn_int parts_ringSize = VIDEO_OA_objExpTimeToParts(ringSize);
  
  alarm_day.text = parts_autoDelete[4];
  alarm_hour.text = parts_autoDelete[3];
  alarm_minute.text = parts_autoDelete[2];
    
  ring_day.text = parts_ringSize[4];
  ring_hour.text = parts_ringSize[3]; 
  ring_minute.text = parts_ringSize[2];
  
  before_hour.text = parts_prealarm[3];
  before_minute.text = parts_prealarm[2];
  before_second.text = parts_prealarm[1];
    
  after_hour.text = parts_postalarm[3];
  after_minute.text = parts_postalarm[2];
  after_second.text = parts_postalarm[1];    
        
  // recordingMode
  if(recordingMode == \"ALARM\")
  {
    box_recordingMode.number = 1;
    cover_ring_recording.visible = 0;
    cover_alarm_recording.visible = 0;
    
    alarm_text.visible = 1;
    alarm_day.visible = 1;
    alarm_hour.visible = 1;
    alarm_minute.visible = 1;
    
    ring_text.visible = 0;
    ring_day.visible = 0;
    ring_hour.visible = 0;
    ring_minute.visible = 0;
  }
  else if (recordingMode == \"LINEAR\")
  {
    box_recordingMode.number = 2;
    cover_ring_recording.visible = 1;
    cover_alarm_recording.visible = 1;
  }
  else // (recordingMode == \"RING\")
  {
    box_recordingMode.number = 0;
    cover_ring_recording.visible = 0;
    cover_alarm_recording.visible = 1;
    
    alarm_text.visible = 0;
    alarm_day.visible = 0;
    alarm_hour.visible = 0;
    alarm_minute.visible = 0;
    
    ring_text.visible = 1;
    ring_day.visible = 1;
    ring_hour.visible = 1;
    ring_minute.visible = 1;
  }
  
  // redundancyMode
  if (redundancyMode == \"LOAD_BALANCING\")
  {
    cover_server_2.visible = 0;
    box_redundancyMode.number = 1;
  }
  else if (redundancyMode == \"FULL_REDUNDANCY\")
  {
    cover_server_2.visible = 0;
    box_redundancyMode.number = 2;
  }
  else
  {
    cover_server_2.visible = 1;
    box_redundancyMode.number = 0;
  }
  
  mapping objectInfo =  VIDEO_OA_objExpObjectInfoToMapping(asLink[1]);

  // text fields
  VIDEO_OA_objExpFillKeyValueFields(VIDEO_OA_objExpGetKeyValueFields(), objectInfo);

  
  // check box
  VIDEO_OA_objExpFillCheckBox(\"chk_1\", objectInfo, makeDynString(\"mode\"));
  
 // txt_key_recordingServer1
   //   txt_key_recordingServer2
  dyn_string serverNames = eventHost();
  string controllerSystem = dpSubStr($OBJECT, DPSUB_SYS);
  string controller1 = txt_key_recordingServer1.text; 
  string controller2 = txt_key_recordingServer2.text; 
  string path1 = txt_key_path1.text; 
  string path2 = txt_key_path2.text; 
  
  path1 = (path1 == \"\") ? \"C:/vimacc/export\" : path1; // set default if emty
  path2 = (path2 == \"\") ? \"C:/vimacc/export\" : path2; // set default if emty 

  txt_key_path1.text = path1; 
  txt_key_path2.text = path2; 
  
  if (dpExists(controllerSystem + controller1) && (controller1 != \"\"))
  {
    dyn_string controllerLink;
    dpGet(controllerSystem + controller1 + \".object.device.link\", controllerLink);
    VIDEO_OA_formatControllerLink(controllerLink); 

    string hostname = VIDEO_OA_filterKeywordValue(controllerLink[1], \"hostname\");
    hostname = (hostname == \"localhost\") ? getHostname() : hostname;
    

    if(dynContains(serverNames, hostname) > 0)
    {
      folderButton1.visible = 1;
    }
    else
      folderButton1.visible = 0;
  }
//   else
//     folderButton1.visible = 0;  
  

  
  if (dpExists(controllerSystem + controller2) && (controller2 != \"\"))
  {
    dyn_string controllerLink;
    dpGet(controllerSystem + controller2 + \".object.device.link\", controllerLink);
    VIDEO_OA_formatControllerLink(controllerLink); 

    string hostname = VIDEO_OA_filterKeywordValue(controllerLink[1], \"hostname\");
    hostname = (hostname == \"localhost\") ? getHostname() : hostname;
    

    if(dynContains(serverNames, hostname) > 0)
    {
      folderButton2.visible = 1;
    }
    else
      folderButton2.visible = 0;
  }
//   else
//     folderButton2.visible = 0; 
  

  VIDEO_OA_objExpUnlockTree( $MAINMODULE, $MAINPANEL);  
  delay(0, 200);
  updateLock = 0;  // unlock backCol update for input fields
}



dyn_int VIDEO_OA_objExpTimeToParts(string timeValue, int limit = 0)
{
  dyn_int result;
  int iTime = (int) timeValue;
  
  
  if (limit != 0)
  {
    iTime = (iTime > limit) ? limit : iTime; 
  } 
  
  result[4]  = floor((float)timeValue / 86400.0 );
  result[3]  = floor(((float)timeValue - (float)result[4] * 86400.0) / 3600);
  result[2]  = floor(((float)timeValue - (float)result[4] * 86400.0 - (float)result[3] * 3600.0) / 60);
  result[1]  = floor(((float)timeValue - (float)result[4] * 86400.0 - (float)result[3] * 3600.0 - (float)result[2] * 60.0));
   
  
  return result;

}

int VIDEO_OA_objExpPartsToTime(dyn_int timeParts)
{
  int result;
  if(dynlen(timeParts) == 4)
  {
    result = (timeParts[4] * 86400) + (timeParts[3] * 3600) + (timeParts[2] * 60) + (timeParts[1]) ;
  }
  else 
    result = -1;
  
  return result;
}
