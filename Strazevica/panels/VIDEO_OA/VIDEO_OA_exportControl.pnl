"main()
{
  this.selectByClick(TABLE_SELECT_NOTHING);
  this.tableMode    (TABLE_SELECT_NOTHING);
  this.sortOnClick(0);
  VIDEO_OA_alternatingRowColorsForTable();
  
  dpConnect(\"work\",VIDEO_OA_getWorkstationDp() + \".exportServer.log.log\",
                   VIDEO_OA_getWorkstationDp() + \".exportServer.log.logUser\");
}

work(string dp1, dyn_string log1, string dp2, dyn_string log2)
{
  dynAppend(log1, log2);
  dyn_dyn_string tab;
  dynSortAsc(log1);
  dyn_errClass err;
 
  logTab.deleteAllLines();
  for(int i = 1; i <= dynlen(log1); i++)
  {
    tab[i]= strsplit(log1[i], \"|\"); 
  }
  
  for(int i = 1; i <= dynlen(tab); i++)
  {
   time logDate = (int)tab[i][5];
   string pathInfo;
   if(tab[i][2] != \"\")
    VIDEO_OA_exportSepartePathinfo(tab[i][2], pathInfo);  
   
    string msgText = getCatStr(\"voa_general\",tab[i][1]); // message text
    err = getLastError();
    if (dynlen(err) == 0) //no error-> cat string found
      tab[i][1] = msgText;  
 
    string fctText = getCatStr(\"voa_general\", tab[i][4]); // function text
    err = getLastError();
    if (dynlen(err) == 0) //no error-> cat string found
      tab[i][4] = fctText; 
   
   logTab.updateLine(1,\"colSortDate\",tab[i][5],\"colSortDate\", tab[i][5],
                                               \"colDate\", formatTime(\"%c\", logDate),
                                               \"colAction\", tab[i][1],
                                               \"colName\", tab[i][2],
                                               \"colUser\", tab[i][3],
                                               \"colFunction\",tab[i][4],
                                               \"colPath\", pathInfo);
  }
  logTab.sort(\"colSortDate\");
}

