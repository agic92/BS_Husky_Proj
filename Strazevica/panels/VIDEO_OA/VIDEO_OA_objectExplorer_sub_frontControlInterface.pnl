VIDEO_OA_showCtrlState(string dp1, bool state1,
                       string dp2, bool state2,
                       string dp3, bool state3,
                       string dp4, bool state4,
                       string dp5, bool state5,
                       string dp6, bool state6,
                       string dp7, bool state7,
                       string dp8, bool state8,
                       string dp7, bool state9,
                       string dp8, bool state10)
{  
  ctrl_state_general.backCol = (state1) ? \"Video_state_connection_ok\" : \"Video_state_connection_offline\";
  api_state_general.backCol = (state2) ? \"Video_state_connection_ok\" : \"Video_state_connection_offline\";
  ctrl_state_alarm_1.backCol = (state3) ? \"Video_state_connection_ok\" : \"Video_state_connection_offline\";
  ctrl_state_alarm_2.backCol = (state4) ? \"Video_state_connection_ok\" : \"Video_state_connection_offline\";
  ctrl_state_main_1.backCol = (state5) ? \"Video_state_connection_ok\" : \"Video_state_connection_offline\";
  ctrl_state_main_2.backCol = (state6) ? \"Video_state_connection_ok\" : \"Video_state_connection_offline\";
  ctrl_state_sequence_1.backCol = (state7) ? \"Video_state_connection_ok\" : \"Video_state_connection_offline\";
  ctrl_state_sequence_2.backCol = (state8) ? \"Video_state_connection_ok\" : \"Video_state_connection_offline\";
  ctrl_state_extAlarm_1.backCol = (state9) ? \"Video_state_connection_ok\" : \"Video_state_connection_offline\";
  ctrl_state_extAlarm_2.backCol = (state10) ? \"Video_state_connection_ok\" : \"Video_state_connection_offline\";
  
  encryption_state_general.backCol = (VIDEO_OA_getEncryptionCompatibilityInfo()) ? \"Video_state_connection_ok\" : \"Video_state_connection_offline\";
}
