"main()
{
  dpConnect(\"VIDEO_OA_showSystemErrors\", $SYSTEM + \"_VIDEO_OA_MAIN.driver.state\");
  dpConnect(\"VIDEO_OA_showSystemLicence\", $SYSTEM + \"_VIDEO_OA_MAIN.driver.licence\");
}

VIDEO_OA_showSystemErrors(string dp1, string state)
{
  dyn_string checkedSources = makeDynString(\"hosts\",\"devices\",\"displays\",\"licence\",\"services\");
  string source = VIDEO_OA_filterKeywordValue(state, \"source\");
  dyn_string sourceParts= strsplit(source, \",\");
 
  for (int i = 1; i <= dynlen(checkedSources); i++)
  {
    if (state == \"\")
      setValue(\"state_\" + checkedSources[i], \"backCol\", \"Video_state_disconnected\");
    else if (dynContains(sourceParts, checkedSources[i]) > 0)
      setValue(\"state_\" + checkedSources[i], \"backCol\", \"Video_state_connection_offline\");
    else
      setValue(\"state_\" + checkedSources[i], \"backCol\", \"Video_state_connection_ok\");
  }
   
} 

VIDEO_OA_showSystemLicence(string dp1, string licence)
{
  string MaxInputChannels  = VIDEO_OA_filterKeywordValue(licence, \"MaxInputChannels\");
  string MaxOutputChannels = VIDEO_OA_filterKeywordValue(licence, \"MaxOutputChannels\");
  string MaxRecordingChannels = VIDEO_OA_filterKeywordValue(licence, \"MaxRecordingChannels\");
  
  txt_RemainingUpTime.text = VIDEO_OA_filterKeywordValue(licence, \"RemainingUpTime\");
  txt_MaxUpTime.text       = VIDEO_OA_filterKeywordValue(licence, \"MaxUpTime\");
  
  txt_MaxInputChannels.text     = (MaxInputChannels == -1) ? \"DEMO\" : MaxInputChannels;
  txt_NeededInputChannels.text  = VIDEO_OA_filterKeywordValue(licence, \"NeededInputChannels\");
  txt_MaxOutputChannels.text    = (MaxOutputChannels == -1) ? \"DEMO\" : MaxOutputChannels;
  txt_NeededOutputChannels.text = VIDEO_OA_filterKeywordValue(licence, \"NeededOutputChannels\");
  txt_MaxRecordingChannels.text    = (MaxRecordingChannels == -1) ? \"DEMO\" : MaxRecordingChannels;
  txt_NeededRecordingChannels.text = VIDEO_OA_filterKeywordValue(licence, \"NeededRecordingChannels\");
  
  if (txt_RemainingUpTime.text != \"\")
    cover_demo.visible = 0;
  else
    cover_demo.visible = 1;  
}
