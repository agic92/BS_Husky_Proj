dyn_string getBatchFiles()
{
  string sStartServ = \"\\\\bin\\\\vimaccStartServices.bat\";
  string sStopServ  = \"\\\\bin\\\\vimaccStopServices.bat\";
  
  dyn_string dsBatchFiles;
  dyn_string ds;
  
  if ( _WIN32 )
  {
    string sOut, sErr, sCmd = \"REG QUERY HKLM\\\\SOFTWARE\\\\Accellence\\\\vimacc\\\\General /v InstallationDirectory\";
    system(sCmd, sOut, sErr);
    ds = strsplit(sOut, \" \");
  
    for ( int i = dynlen(ds); i >= 1; i-- )
      if ( ds[i] == \"\" )
        dynRemove(ds, i);
  
    sOut = strltrim(strrtrim(ds[dynlen(ds)]));
  
    dsBatchFiles[1] = sOut + sStartServ;
    dsBatchFiles[2] = sOut + sStopServ;
  }
  
  return dsBatchFiles;
}







