VIDEO_OA_showDriverState (string dp1, string manNum1, 
                          string dp2, string manNum2, 
                          string dp3, dyn_int conn1,
                          string dp4, dyn_int conn2,
                          
                          string dp5, bool evStatus1,
                          string dp6, bool evStatus2,
                          string dp7, dyn_string hostNames1,
                          string dp8, dyn_string hostNames2,
                          
                          string dp9,  string connection1, 
                          string dp10, string connection2,
                          
                          string dp11, bool link0_1,
                          string dp12, int peerError_1,
                          string dp13, bool link0_2,
                          string dp14, int peerError_2)
{
  // Control Interface Connections
  string conn_1_1 = VIDEO_OA_filterKeywordValue(connection1, \"Control_Interface_00001\");
  string conn_1_2 = VIDEO_OA_filterKeywordValue(connection1, \"Control_Interface_00002\");
  string conn_2_1 = VIDEO_OA_filterKeywordValue(connection2, \"Control_Interface_00001\");
  string conn_2_2 = VIDEO_OA_filterKeywordValue(connection2, \"Control_Interface_00002\");
  
  VIDEO_OA_setConnectionCol(\"con_1_1\",conn_1_1);
  VIDEO_OA_setConnectionCol(\"con_1_2\",conn_1_2);
  VIDEO_OA_setConnectionCol(\"con_2_1\",conn_2_1);
  VIDEO_OA_setConnectionCol(\"con_2_2\",conn_2_2);
  
  bool running_1;
  bool running_2;
  
  bool host_on_2 = ((link0_1 == 0) && (peerError_1 == -1)) ? 0 : 1;
  bool host_on_1 = ((link0_2 == 0) && (peerError_2 == -1)) ? 0 : 1; 

  int iAnswer;  
  
  isRemoteSystemRedundant(iAnswer, $SYSTEM);
  
  if( iAnswer != 1 )
  {
    host_on_1 = 1;
    host_on_2 = 1;   
  }
  
  drv_1.visible =  host_on_1;
  drv_1_txt.visible = host_on_1;
  con_1_1.visible = host_on_1;
  con_1_2.visible = host_on_1;
  
  drv_2.visible = host_on_2;
  drv_2_txt.visible = host_on_2;
  con_2_1.visible = host_on_2;
  con_2_2.visible = host_on_2;
  
  
  
  int activeHostNum = VIDEO_OA_getActiveHostNum();
  
  drv_1.backCol = (activeHostNum == 1) ? \"Video_mode_active\" : \"\";
  drv_2.backCol = (activeHostNum == 2) ? \"Video_mode_active\" : \"\"; 
  
  if ( (dynContains(conn1, manNum1) > 0) && (host_on_1 == 1) )
  {
    drv_1.foreCol = \"Video_state_connection_ok\";
    con_1_1.visible = 1;
    con_1_2.visible = 1; 
    running_1 = 1; 
  }
  else
  {
    drv_1.foreCol = \"Video_state_connection_offline\";
    con_1_1.visible = 0;
    con_1_2.visible = 0;
    running_1 = 0; 
  }
 
  if ( (dynContains(conn2, manNum2) > 0) && (host_on_2 == 1) )
  {
    drv_2.foreCol = \"Video_state_connection_ok\";
    con_2_1.visible = 1;
    con_2_2.visible = 1;
    running_2 = 1; 
  }
  else
  {
    drv_2.foreCol = \"Video_state_connection_offline\";
    con_2_1.visible = 0;
    con_2_2.visible = 0;
    running_2 = 0;
  }
  
  
  
   VIDEO_OA_setActiveColForControlInterface(\"interface_1\", \"interface_2\", conn_1_1, conn_1_2, conn_2_1, conn_2_2, 
                                                                          running_1, running_2, 
                                                                          host_on_1, host_on_2);
}


VIDEO_OA_setConnectionCol(string widget, string conn)
{

  string col;
  switch( conn )
  {
    case \"OFFLINE\":   col=\"Video_state_connection_offline\"; break;
    case \"LOGGED_IN\": col=\"Video_state_connection_logged_in\"; break;
    case \"ONLINE\":    col=\"Video_state_connection_ok\"; break;
    default:          col=\"Video_panel\";
  }
   setValue( widget, \"color\", col);
}

VIDEO_OA_setActiveColForControlInterface(string widget_1, string widget_2 , string conn_1_1, 
                                                                            string conn_1_2, 
                                                                            string conn_2_1, 
                                                                            string conn_2_2,
                                                                            bool running_1,
                                                                            bool running_2,
                                                                            bool host_on_1,
                                                                            bool host_on_2)
{

    if ( (conn_1_1 == \"ONLINE\") && (running_1 == 1) && (host_on_1 == 1) ) 
    {
     setValue(widget_1, \"backCol\", \"Video_mode_active\");
     setValue(widget_2, \"backCol\", \"\"); 
    }
    else if ( (conn_1_2 == \"ONLINE\") && (running_1 == 1) && (host_on_1 == 1) )
    {
     setValue(widget_1, \"backCol\", \"\");
     setValue(widget_2, \"backCol\", \"Video_mode_active\"); 
    }
    else if ( (conn_2_1 == \"ONLINE\") && (running_2 == 1) && (host_on_2 == 1) )
    {
     setValue(widget_1, \"backCol\", \"Video_mode_active\");
     setValue(widget_2, \"backCol\", \"\"); 
    }
    else if ( (conn_2_2 == \"ONLINE\") && (running_2 == 1) && (host_on_2 == 1) )
