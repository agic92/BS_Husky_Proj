VIDEO_OA_work_frontControlInterfaceCover(string dp1, dyn_string sLink1,
                                         string dp2, dyn_string sLink2)                              
{
  dyn_string modes;
  VIDEO_OA_formatDeviceLink(sLink1);
  VIDEO_OA_formatDeviceLink(sLink2);
  modes[1] = VIDEO_OA_filterKeywordValue(sLink1[1], \"mode\");
  modes[2] = VIDEO_OA_filterKeywordValue(sLink2[1], \"mode\");

  for (int i = 1; i <= 2; i++)
  {
    if (modes[i] == \"1\")
      setValue(\"cover_server\" + i, \"visible\", 0);
    else
      setValue(\"cover_server\" + i, \"visible\", 1);
  }
}

