"main()
{ 
  string systemName;

  systemName = $SYSTEM;
   
  string sQuery = \"SELECT '_online.._value,_online.._stime' FROM '*.object.device.alarm' REMOTE '\" + systemName + \"' WHERE _DPT = \\\"VIDEO_OA_CAMERA\\\" OR _DPT = \\\"VIDEO_OA_ENCODER\\\"\"; 
  state.text = \"1\";
  state_show.text = getCatStr(\"voa_general\",\"deviceAlertScreen_running\");
  state_show.backCol = \"Video_seq_play\";
  
  // query for alarm para  
  dpQueryConnectSingle(\"VIDEO_OA_alarmShowAlarmTable\", 0, \"VIDEO_OA_alarmShowAlarmTable\", sQuery); 
  
  this.deleteAllLines();
  this.sortOnClick(0);
  VIDEO_OA_alternatingRowColorsForTable();
}

VIDEO_OA_alarmShowAlarmTable(string ident, dyn_dyn_anytype val) 
{
  if (state.text == \"1\")
  {
    for (int i = 2; i <= dynlen(val); i++) 
    {
       string device =  dpSubStr(val[i][1], DPSUB_SYS_DP);
       string alertTime = formatTime(\"%c\", val[i][3], \".%03d\");
       dyn_string values = strsplit(val[i][2], \";\");
     
       for (int j = 1; j <= dynlen(values); j++)
       {
         this.appendLine(\"#1\", alertTime, \"#2\", device, \"#4\", VIDEO_OA_getFormatedObjectNumber(device) + \" \"+ dpGetComment(device + \".\"), \"#3\", values[j]);  
       }
    }
  } 
}" 0
