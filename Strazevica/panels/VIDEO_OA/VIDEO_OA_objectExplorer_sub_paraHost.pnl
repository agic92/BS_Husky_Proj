E "bool updateLock;

bool VIDEO_OA_objExpApplyParaChangesHost() 
{   
  string sType = txt_definition.userData(txt_definition.selectedPos);
  dyn_string asDefinition = makeDynString( \"type=\"+sType+\";para=1\" );
  string sLink;
 
    // check permission
  if (!VIDEO_OA_getUserPermission(\"perm_16\"))
  {
    VIDEO_OA_openNoPermission(\"perm_16\");
    return 1;
  }
  
  if (VIDEO_OA_objExpCheckHostname(txt_key_hostname.text) != 1)
  {    
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_hostname\"));
    return 1;  
  }
  
  if (strtolower(txt_key_hostname.text) == \"localhost\")
  {    
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_hostname_5\"));
    return 1;  
  }
  
  string multipleHostname = VIDEO_OA_objExpCheckMultipleHostname(txt_key_hostname.text, $OBJECT); 
  if (multipleHostname != \"\")
  {
    string hostnameErrorText = getCatStr(\"voa_general\", \"para_error_hostname_2\");
    
    strreplace(hostnameErrorText, \"$HOSTNAME\", multipleHostname);
    VIDEO_OA_openMessageWarning(hostnameErrorText);
    return 1;  
  }
 
  updateLock = 1;  // lock backCol update for input fields
  
 
  sLink = VIDEO_OA_objExpGetLinkInfosFromTextFields();
  
  sLink = sLink + \";\" + VIDEO_OA_objExpGetLinkInfosFromCheckBox(\"chk_2\", makeDynString(\"isDisplayServer\", \"isExportServer\",\"isRecordingServer\",\"isVideoInterface\",\"isRtspServer\",\"isFTPAlarmReceiver\"));

  
  strreplace(sLink,\";;\",\";\");
  
  dpSet( $OBJECT + \".object.device.link\", makeDynString(sLink),
         $OBJECT + \".object.device.definition\", asDefinition);
  
  
  txt_definition.backCol = \"\";  
  VIDEO_OA_objExpUnlockTree( $MAINMODULE, $MAINPANEL);  
  delay(0,200);
  updateLock = 0;  // unlock backCol update for input fields
  return 0;
}
//----------------------------------------------------------------


VIDEO_OA_objExpUpdateParaHost() 
{
  dyn_string asLink, asDefinition;
  
  updateLock = 1;  // lock backCol update for input fields
  
  dpGet( $OBJECT + \".object.device.link\", asLink,
         $OBJECT + \".object.device.definition\", asDefinition);
  
  VIDEO_OA_formatDeviceDefinition(asDefinition);  
  VIDEO_OA_formatDeviceDefinition(asLink);   // only [1] is in use for this type 
  
  mapping objectInfo =  VIDEO_OA_objExpObjectInfoToMapping(asLink[1]);

  // text fields 
  VIDEO_OA_objExpFillKeyValueFields(VIDEO_OA_objExpGetKeyValueFields(), objectInfo);
 
  // combo box
  string type = VIDEO_OA_filterKeywordValue(asDefinition[1], \"type\");
  string ipAddress = VIDEO_OA_filterKeywordValue(asDefinition[1], \"type\");

  if ( (type == \"HOST\") || (type == \"CONTROLLER\") )
  {
    txt_definition.text     = getCatStr(\"voa_general\",\"type_\"+type);
  }
  else
    txt_definition.text     = getCatStr(\"voa_general\",\"type_HOST\");
  
  // check box
  VIDEO_OA_objExpFillCheckBox(\"chk_2\", objectInfo, makeDynString(\"isDisplayServer\", \"isExportServer\",\"isRecordingServer\",\"isVideoInterface\",\"isRtspServer\"));
  
  txt_definition.backCol = \"\";  
  VIDEO_OA_objExpUnlockTree( $MAINMODULE, $MAINPANEL);  
  delay(0,200);
  updateLock = 0;  // unlock backCol update for input fields
}



