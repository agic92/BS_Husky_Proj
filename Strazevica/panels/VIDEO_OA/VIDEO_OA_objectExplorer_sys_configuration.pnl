VIDEO_OA_objExpUpdateParaSysConf() 
{
  dyn_string asLink, asDefinition, asLink_2, asDefinition_2, asLink_interface_1, asLink_interface_2;
  bool svr1active, svr2active;
   
  int createError;  
  
  if (!dpExists(\"_VIDEO_SERVER_1\"))
    dpCopy(\"_mp_VIDEO_OA_HOST\",\"_VIDEO_SERVER_1\",createError);
  if (!dpExists(\"_VIDEO_SERVER_2\"))
    dpCopy(\"_mp_VIDEO_OA_HOST\",\"_VIDEO_SERVER_2\",createError);
  
  if (!dpExists(\"Control_Interface_00001\"))
    dpCopy(\"_mp_VIDEO_OA_CONTROL_INTERFACE.\",\"Control_Interface_00001\",createError);
  if (!dpExists(\"Control_Interface_00002\"))
    dpCopy(\"_mp_VIDEO_OA_CONTROL_INTERFACE.\",\"Control_Interface_00002\",createError);
  
  updateLock = 1;  // lock backCol update for input fields
  
  dpGet($SYSTEM + \"_VIDEO_SERVER_1\" + \".object.device.link\", asLink,
        $SYSTEM + \"_VIDEO_SERVER_1\" + \".object.device.definition\", asDefinition,
        $SYSTEM + \"_VIDEO_SERVER_2\" + \".object.device.link\", asLink_2,
        $SYSTEM + \"_VIDEO_SERVER_2\" + \".object.device.definition\", asDefinition_2,
        $SYSTEM + \"Control_Interface_00001\" + \".object.device.link\", asLink_interface_1,
        $SYSTEM + \"Control_Interface_00002\" + \".object.device.link\", asLink_interface_2);
  
  VIDEO_OA_formatDeviceDefinition(asDefinition);  
  VIDEO_OA_formatDeviceLink(asLink);   // only [1] is in use for this type 
  
  VIDEO_OA_formatDeviceDefinition(asDefinition_2);  
  VIDEO_OA_formatDeviceLink(asLink_2);   // only [1] is in use for this type 
  
  VIDEO_OA_formatDeviceLink(asLink_interface_1);
  VIDEO_OA_formatDeviceLink(asLink_interface_2);
  
  mapping objectInfo =  VIDEO_OA_objExpObjectInfoToMapping(asLink[1]);
  mapping objectInfo_2 =  VIDEO_OA_objExpObjectInfoToMapping(asLink_2[1]);
  mapping objectInfo_interface_1 = VIDEO_OA_objExpObjectInfoToMapping(asLink_interface_1[1]);
  mapping objectInfo_interface_2 = VIDEO_OA_objExpObjectInfoToMapping(asLink_interface_2[1]);
  
  svr1active = VIDEO_OA_objExpGetValueForKey(objectInfo_interface_1, \"mode\");
  svr2active = VIDEO_OA_objExpGetValueForKey(objectInfo_interface_2, \"mode\");
  chk_server_1_active.state(0) = svr1active;
  chk_server_2_active.state(0) = svr2active;
  cover_server1.visible = !svr1active;
  cover_server2.visible = !svr2active;
  
  chk_server_1_active.enabled = !svr2active;
  
  txt_key_port.text       = 4227;
  txt_key_port_2.text     = 4227;

  txt_key_ip.text         = VIDEO_OA_objExpGetValueForKey(objectInfo, \"ip\");
  txt_key_ip_2.text       = VIDEO_OA_objExpGetValueForKey(objectInfo_2, \"ip\");
  txt_key_hostname.text   = VIDEO_OA_objExpGetValueForKey(objectInfo, \"hostname\");
  txt_key_hostname_2.text = VIDEO_OA_objExpGetValueForKey(objectInfo_2, \"hostname\");
  
  // check box
  //VIDEO_OA_objExpFillCheckBox(\"chk_1\", objectInfo, makeDynString(\"isControlInterface\",\"isVideoInterface\"));
  VIDEO_OA_objExpFillCheckBox(\"chk_2\", objectInfo, makeDynString(\"isDisplayServer\", \"isExportServer\",\"isRecordingServer\",\"isRtspServer\",\"isFTPAlarmReceiver\"));
  
  //VIDEO_OA_objExpFillCheckBox(\"chk_1_2\", objectInfo_2, makeDynString(\"isControlInterface\",\"isVideoInterface\"));
  VIDEO_OA_objExpFillCheckBox(\"chk_2_2\", objectInfo_2, makeDynString(\"isDisplayServer\", \"isExportServer\",\"isRecordingServer\",\"isRtspServer\",\"isFTPAlarmReceiver\"));
  
  
  VIDEO_OA_objExpUnlockTree( $MAINMODULE, $MAINPANEL);  
  delay(0,100);
  updateLock = 0;  // unlock backCol update for input fields
}


