bool VIDEO_OA_objExpApplyParaChangesSysConf()
{   
  string sType = \"CONTROLLER\";
  dyn_string asDefinition = makeDynString( \"type=\"+sType+\";para=1\" );
  string sLink, sLink_2, sLinkInterface1, sLinkInterface2;
  string ip, ip_2, host, host_2;
  bool server1, server2;
  string multipleHostname;
  bool bEncryptIpcState;
  
  ip      = txt_key_ip.text;
  ip_2    = txt_key_ip_2.text;
  host    = txt_key_hostname.text;
  host_2  = txt_key_hostname_2.text;
  
  server1 = cover_server1.visible;
  server2 = cover_server2.visible;

 
    // check permission
  if (!VIDEO_OA_getUserPermission(\"perm_16\"))
  {
    VIDEO_OA_openNoPermission(\"perm_16\");
    return 1;
  }
  
  
  if ((VIDEO_OA_objExpCheckHostname(host) != 1) && !server1)
  {    
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_hostname\"));
    return 1;  
  }
  
  
  if ((VIDEO_OA_objExpCheckHostname(host_2) != 1) && !server2)
  {    
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_hostname\"));
    return 1;  
  }
  
  
  if ((strtolower(host) == strtolower(host_2)) && (!server1 && !server2))
  {    
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_hostname_3\"));
    return 1;  
  } 
  
  
  if ( ((strtolower(host) == \"localhost\") || (strtolower(host_2) == \"localhost\")) && (!server1 && !server2) )
  {    
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_hostname_4\"));
    return 1;  
  }
  
  multipleHostname = VIDEO_OA_objExpCheckMultipleHostname(host, \"_VIDEO_SERVER_1\"); 
  if (multipleHostname != \"\")
  {
    string hostnameErrorText = getCatStr(\"voa_general\", \"para_error_hostname_2\");
    
    strreplace(hostnameErrorText, \"$HOSTNAME\", multipleHostname);
    VIDEO_OA_openMessageWarning(hostnameErrorText);
    return 1;  
  }

  multipleHostname = VIDEO_OA_objExpCheckMultipleHostname(host_2, \"_VIDEO_SERVER_2\"); 
  if (multipleHostname != \"\")
  {
    string hostnameErrorText = getCatStr(\"voa_general\", \"para_error_hostname_2\");
    
    strreplace(hostnameErrorText, \"$HOSTNAME\", multipleHostname);
    VIDEO_OA_openMessageWarning(hostnameErrorText);
    return 1;  
  }
  
  if ((VIDEO_OA_objExpCheckPort(txt_key_port.text) != 1) && !server1)
  {    
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_port\"));
    return 1;  
  }
  
  
  if ((VIDEO_OA_objExpCheckPort(txt_key_port_2.text) != 1) && !server2)
  {    
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_port\"));
    return 1;  
  }
  

  updateLock = 1;  // lock backCol update for input fields

  
  sLink =   \"hostname=\" + host + \";\";
  sLink_2 = \"hostname=\" + host_2 + \";\"; 
  sLink = sLink + \";\" + VIDEO_OA_objExpGetLinkInfosFromCheckBox( \"chk_2\", makeDynString(\"isDisplayServer\", \"isExportServer\",\"isRecordingServer\",\"isRtspServer\",\"isFTPAlarmReceiver\"));
  sLink = sLink + \";\" + VIDEO_OA_objExpGetLinkInfosFromCheckBox(\"chk_1\", makeDynString(\"isControlInterface\",\"isVideoInterface\"));
  sLink_2 = sLink_2 + \";\" + VIDEO_OA_objExpGetLinkInfosFromCheckBox(\"chk_2_2\", makeDynString(\"isDisplayServer\", \"isExportServer\",\"isRecordingServer\",\"isRtspServer\",\"isFTPAlarmReceiver\"));
  sLink_2 = sLink_2 + \";\" + VIDEO_OA_objExpGetLinkInfosFromCheckBox(\"chk_1_2\", makeDynString(\"isControlInterface\",\"isVideoInterface\"));
  
  strreplace(sLink,\";;\",\";\");
  strreplace(sLink_2,\";;\",\";\");
  
  sLink   = (chk_server_1_active.state(0)) ? sLink : \"\";
  sLink_2 = (chk_server_2_active.state(0)) ? sLink_2 : \"\";
  
  sLinkInterface1 = \"function=CMS,INTERFACE_VIMACC_CONTROL;port=\" + txt_key_port.text + \";mode=\" + (int)chk_server_1_active.state(0) + \"\";
  sLinkInterface2 = \"function=CMS,INTERFACE_VIMACC_CONTROL;port=\" + txt_key_port_2.text + \";mode=\" + (int)chk_server_2_active.state(0) + \"\";
  
  bEncryptIpcState = cbEncryptIpc.state(0);

  dpSet($SYSTEM + \"_VIDEO_SERVER_1\" + \".object.device.link\", makeDynString(sLink),
        $SYSTEM + \"_VIDEO_SERVER_1\" + \".object.device.definition\", asDefinition,
        $SYSTEM + \"_VIDEO_SERVER_2\" + \".object.device.link\", makeDynString(sLink_2),
        $SYSTEM + \"_VIDEO_SERVER_2\" + \".object.device.definition\", asDefinition,
        $SYSTEM + \"Control_Interface_00001\" + \".object.device.definition\", makeDynString(\"type=CONTROL_INTERFACE;para=1\"),
        $SYSTEM + \"Control_Interface_00002\" + \".object.device.definition\", makeDynString(\"type=CONTROL_INTERFACE;para=1\"),
        $SYSTEM + \"Control_Interface_00001\" + \".object.device.link\", makeDynString(sLinkInterface1, sLinkInterface1, sLinkInterface1),
        $SYSTEM + \"Control_Interface_00002\" + \".object.device.link\", makeDynString(sLinkInterface2, sLinkInterface2, sLinkInterface2),
        
        $SYSTEM + \"Control_Interface_00001\" + \".object.device.controller\", makeDynString(\"_VIDEO_SERVER_1\", \"_VIDEO_SERVER_1\", \"_VIDEO_SERVER_1\"),
        $SYSTEM + \"Control_Interface_00002\" + \".object.device.controller\", makeDynString(\"_VIDEO_SERVER_2\", \"_VIDEO_SERVER_2\", \"_VIDEO_SERVER_2\"),
        
        $SYSTEM + \"_VIDEO_OA_MAIN.settings.encryptIpcSockets\", bEncryptIpcState);
        

  
  txt_key_ip.backCol         = \"\"; 
  txt_key_ip_2.backCol       = \"\";
  txt_key_hostname.backCol   = \"\";
  txt_key_hostname_2.backCol = \"\";
  
  txt_key_port.backCol       = \"\";
  txt_key_port_2.backCol     = \"\";
  
  
  chk_server_1_active.backCol = \"\";
  chk_server_2_active.backCol = \"\";
  chk_1.backCol   = \"\";
  chk_2.backCol   = \"\";
  chk_1_2.backCol = \"\";
  chk_2_2.backCol = \"\";
  
  VIDEO_OA_objExpUnlockTree( $MAINMODULE, $MAINPANEL);  
  delay(0,100);
  updateLock = 0;  // unlock backCol update for input fields
  return 0;
}
