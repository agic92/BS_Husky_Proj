"main()
{
  int max = maxMonitor.text;
  int actCount = tab.lineCount;
  int newLines = max - actCount;
  int iAnswer;
  
  if(newLines != 0)
    VIDEO_OA_openMessageInfoAnswer(getCatStr(\"voa_general\", \"changeNumberOfMonitors\"), 1, iAnswer);
  
  if(iAnswer == 1)
  {
    tab.updatesEnabled(0);
    if(newLines > 0)
    {
      for(int i = 1; i <= newLines; i ++)
       VIDEO_OA_monitorFieldPara_appendLine();
    }
    else // <= 0
    {
      for(int i = 1; i <= abs(newLines) ; i ++)
       VIDEO_OA_monitorFieldPara_deleteLine();
    }    
    tab.updatesEnabled(1);
  }
}


