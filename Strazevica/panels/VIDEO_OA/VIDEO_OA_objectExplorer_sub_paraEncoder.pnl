/*
  VIDEO_OA_objExpUpdateParaCamera() 
  funtion to show the actual camera parameter in the parameter panel
  
*/
VIDEO_OA_objExpUpdateParaCamera() 
{
  dyn_string asLink, asDefinition, asController;
  dyn_string encapControl, encapStream, codec, dsDelete, deleteControl, parameter, parameterControl, urlControl, urlStream, fieldlayout;
  string port,streamPort;
  bool valuesFound;
  
  updateLock = 1;  // lock backCol update for input fields
  RC0_updateLock.text = 1;
  
  dpGet( $OBJECT + \".object.device.link\", asLink,
         $OBJECT + \".object.device.definition\", asDefinition,
         $OBJECT + \".object.device.controller\", asController);
  
  VIDEO_OA_formatDeviceDefinition(asDefinition);  
  VIDEO_OA_formatDeviceLink(asLink);   
  VIDEO_OA_formatControllerLink(asController);
  
  // streamsettings RC0 -RC2  
  mapping objectInfo_RC0 =  VIDEO_OA_objExpObjectInfoToMapping(asLink[1],\"_RC0\");
//  mapping objectInfo_RC1 =  VIDEO_OA_objExpObjectInfoToMapping(asLink[2],\"_RC1\"); 
//  mapping objectInfo_RC2 =  VIDEO_OA_objExpObjectInfoToMapping(asLink[3],\"_RC2\");

  dyn_string valueFields_RC0 = VIDEO_OA_objExpGetKeyValueFields(\"txt_key_*\",myModuleName(),\"VIDEO_OA\\\\VIDEO_OA_objectExplorer_sub_paraCameraIP.pnl\", \"_RC0\");
//  dyn_string valueFields_RC1 = VIDEO_OA_objExpGetKeyValueFields(\"txt_key_*\",myModuleName(),\"VIDEO_OA\\\\VIDEO_OA_objectExplorer_sub_paraCameraIP.pnl\", \"_RC1\");
//  dyn_string valueFields_RC2 = VIDEO_OA_objExpGetKeyValueFields(\"txt_key_*\",myModuleName(),\"VIDEO_OA\\\\VIDEO_OA_objectExplorer_sub_paraCameraIP.pnl\", \"_RC2\");
  
  // controlsettings RC0 -RC2 
  mapping objectInfo =  VIDEO_OA_objExpObjectInfoToMapping(asLink[1]);  // take control settings only from [1] - RCO because is always equal to
                                                                        // RC 1 und RC 2
  
  dyn_string items = VIDEO_OA_objExpGetVideoHosts(dpSubStr($OBJECT, DPSUB_SYS), \"\", makeDynString(\"isVideoInterface\"));
  txt_controller.items = items;
  txt_controller_2.items = items;
  txt_controller_3.items = items;
  // combo box
  asController[1] = (asController[1] == \"\") ? \"_VIDEO_SERVER_1\" : asController[1];
  txt_controller.text = asController[1];
  
  asController[2] = (asController[2] == \"\") ? \"_VIDEO_SERVER_1\" : asController[2];
  txt_controller_2.text = asController[2];
  
  asController[3] = (asController[3] == \"\") ? \"_VIDEO_SERVER_2\" : asController[3];
  txt_controller_3.text = asController[3];

  // check box
  VIDEO_OA_objExpFillCheckBox(\"chk_1\", objectInfo, makeDynString(\"mode\"));
  VIDEO_OA_objExpFillCheckBox(\"chk_2\", objectInfo, makeDynString(\"flipx\", \"flipy\", \"flipz\"));
  VIDEO_OA_objExpFillCheckBox(\"chk_permConn\", objectInfo, makeDynString(\"permanentConnection\"));
  
  
  // combo box for camera type
  string control = VIDEO_OA_filterKeywordValue(asLink[1], \"ctrlProtocol\");
  string ptzPort = VIDEO_OA_filterKeywordValue(asLink[1], \"ptzPort\");
  
  if (control != \"\")
  {
    if (dpTypeName($OBJECT) == \"VIDEO_OA_CAMERA\")
      txt_control.text = getCatStr(\"voa_general\", \"camType_\"+control);
    else
    {
      txt_control.text = getCatStr(\"voa_general\", \"encoderType_\"+control);
      ptz_port.text = ptzPort;
      if (control == \"RCPP_VIDEO_DEVICE\")
         ptz_port.enabled = 0; 
      else
         ptz_port.enabled = 1;   
       
    }
    VIDEO_OA_objExpGetEncapsulationForCameraType(control, encapControl, encapStream, codec, port, streamPort, dsDelete, deleteControl, parameter, parameterControl, urlControl, urlStream, fieldlayout);
    VIDEO_OA_objExpFillItemsForCamera(control, encapControl, encapStream, codec, port, streamPort, dsDelete, deleteControl, parameter, parameterControl, urlControl, urlStream, fieldlayout);
  }
  else
    txt_control.text = \"\";
    
  
  
  // text fields control 
  valuesFound = VIDEO_OA_objExpFillKeyValueFields(VIDEO_OA_objExpGetKeyValueFields(), objectInfo);
  
  // text fields stream 
  
  valuesFound = VIDEO_OA_objExpFillKeyValueFields( valueFields_RC0 ,objectInfo_RC0);
   
  VIDEO_OA_objExpShowStream (\"stream1\", \"RC0\", objectInfo_RC0);
  VIDEO_OA_objExpShowStream (\"stream2\", \"RC0\", objectInfo_RC0);
  VIDEO_OA_objExpShowStream (\"stream3\", \"RC0\", objectInfo_RC0);
  
//  valuesFound = VIDEO_OA_objExpFillKeyValueFields( valueFields_RC1 ,objectInfo_RC1);
  
//   VIDEO_OA_objExpShowStream (\"stream1\", \"RC1\", objectInfo_RC1);
//   VIDEO_OA_objExpShowStream (\"stream2\", \"RC1\", objectInfo_RC1);
//   VIDEO_OA_objExpShowStream (\"stream3\", \"RC1\", objectInfo_RC1);
  
//  valuesFound = VIDEO_OA_objExpFillKeyValueFields( valueFields_RC2 ,objectInfo_RC2);
 
//   VIDEO_OA_objExpShowStream (\"stream1\", \"RC2\", objectInfo_RC1);
//   VIDEO_OA_objExpShowStream (\"stream2\", \"RC2\", objectInfo_RC1);
//   VIDEO_OA_objExpShowStream (\"stream3\", \"RC2\", objectInfo_RC1); 

  // fill user defined fields RC0
  dyn_string UserDefinedFields             = VIDEO_OA_objExpFindUserDefinedFields(objectInfo_RC0, \"\", \"RC0\", \"control\");
  dyn_string UserDefinedFields_RC0_stream1 = VIDEO_OA_objExpFindUserDefinedFields(objectInfo_RC0, \"stream1\", \"RC0\", \"stream\");
  dyn_string UserDefinedFields_RC0_stream2 = VIDEO_OA_objExpFindUserDefinedFields(objectInfo_RC0, \"stream2\", \"RC0\", \"stream\");
  dyn_string UserDefinedFields_RC0_stream3 = VIDEO_OA_objExpFindUserDefinedFields(objectInfo_RC0, \"stream3\", \"RC0\", \"stream\");

  string convertParameter =  UserDefinedFields;
  strreplace(convertParameter,\" | \",\";\");  
  
  txt_key_userDef.text = convertParameter;

  convertParameter =  UserDefinedFields_RC0_stream1;
  strreplace(convertParameter,\" | \",\";\"); 
  
  txt_key_stream1_userDef_RC0.text = convertParameter;

  convertParameter =  UserDefinedFields_RC0_stream2;
  strreplace(convertParameter,\" | \",\";\"); 

  txt_key_stream2_userDef_RC0.text = convertParameter;

  convertParameter =  UserDefinedFields_RC0_stream3;
  strreplace(convertParameter,\" | \",\";\");   
  
  txt_key_stream3_userDef_RC0.text = convertParameter;
  // end of fill user defined fields RC0
  
  
  // function fix or ptz
  string function = VIDEO_OA_filterKeywordValue(asLink[1], \"function\");
  
  // encoder ptz mode
  string ptzMode = VIDEO_OA_filterKeywordValue(asLink[1], \"ptzMode\");
  
  
  if (ptzMode == \"transparent\") // -> transparent PTZ mode
  {
    rb_ptz_mode.number = 1; 
    ptz_port.visible = 1;      // only for transparent mode
    txt_ptz_port.visible = 1;
  }
  else
  {
    rb_ptz_mode.number = 0;
    ptz_port.visible = 0;
    txt_ptz_port.visible = 0;
  }  
  
  
  if (function == \"PTZ\") // -> PTZ Kamera
  {
    chk_3.state(0, 1);
    coverPTZ.visible = 0;
  }
  else
  {
    chk_3.state(0, 0);
    coverPTZ.visible = 1;
  }

  if (dpTypeName($OBJECT) == \"VIDEO_OA_ENCODER\")
  {
    chk_3.visible = 0; // no PTZ for Encoder 
    chk_permConn.visible = 0; // no permanent connection option for encoder 
  }
  
  txt_preset.text = VIDEO_OA_getMaxPreset($OBJECT);
  
  tab_edit.text = \"\"; // reset register icons

  VIDEO_OA_objExpUnlockTree( $MAINMODULE, $MAINPANEL);  
  delay(0,200);
  updateLock = 0;  // unlock backCol update for input fields
  RC0_updateLock.text = 0;
  
}

