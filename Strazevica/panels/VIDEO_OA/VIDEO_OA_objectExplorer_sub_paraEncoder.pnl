/*
 VIDEO_OA_objExpApplyParaChangesCamera() 
 funtion to update a camera datapoint after using the aplly or ok button
 in a parameter panel
  
*/
bool VIDEO_OA_objExpApplyParaChangesCamera() 
{   

  string sLink, sLink_RC0, sLink_RC1, sLink_RC2;
  string sController = txt_controller.text;
  string sFunction;
  string presetDef;
  string ptzMode,ptzPort;
  dyn_string definition;
  
  
  
    // check permission
  if (!VIDEO_OA_getUserPermission(\"perm_16\"))
  {
    VIDEO_OA_openNoPermission(\"perm_16\");
    return 1;
  }
  
  if ( (VIDEO_OA_objExpCheckPort(txt_key_ctrlPort.text) != 1) && (txt_key_ctrlPort.visible == 1) ) 
  {    
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_port\"));
    return 1;  
  }
  
  if (txt_control.text == \"\")
  {    
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_camType\"));
    return 1;  
  }
  
  if ( (VIDEO_OA_objExpCheckIpAddress(txt_key_ip.text) != 1) && (txt_key_ip.visible == 1) ) 
  {    
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_ip\"));
    return 1;  
  } 
 
  if ((ptz_port.visible == 1) && (VIDEO_OA_objExpCheckPort(ptz_port.text ,0) != 1) )
  {    
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_ptzPort\"));
    return 1;  
  }
  
  updateLock = 1;  // lock backCol update for input fields
  
  
  sLink = VIDEO_OA_objExpGetLinkInfosFromTextFields();
  
  // Link Info for RC0
  
  dyn_string valueFields_RC0 = VIDEO_OA_objExpGetKeyValueFields(\"txt_key_*\",myModuleName(),\"VIDEO_OA\\\\VIDEO_OA_objectExplorer_sub_paraCameraIP.pnl\",\"_RC0\");  
  
  if (chk_RC0_stream1.state(0))
    sLink_RC0 = sLink_RC0 + VIDEO_OA_objExpGetLinkInfosFromTextFields(valueFields_RC0, \"_RC0\", \"stream1*\");
  if (chk_RC0_stream2.state(0))
    sLink_RC0 = sLink_RC0 + VIDEO_OA_objExpGetLinkInfosFromTextFields(valueFields_RC0, \"_RC0\", \"stream2*\");
  if (chk_RC0_stream3.state(0))
    sLink_RC0 = sLink_RC0 + VIDEO_OA_objExpGetLinkInfosFromTextFields(valueFields_RC0, \"_RC0\", \"stream3*\");
  
  
    
  sLink = sLink + \";\" + VIDEO_OA_objExpGetLinkInfosFromCheckBox(\"chk_1\", makeDynString(\"mode\"));
  
  if (dpTypeName($OBJECT) == \"VIDEO_OA_CAMERA\")
  {
    sLink = sLink + \";\" + VIDEO_OA_objExpGetLinkInfosFromCheckBox(\"chk_2\", makeDynString(\"flipx\",\"flipy\",\"flipz\"));
  
    sFunction = (chk_3.state(0)) ? \"function=PTZ\" : \"function=FIX\";                   // function=
  
    sLink = sLink + \";\" + sFunction;
    
    sLink = sLink + \";\" + VIDEO_OA_objExpGetLinkInfosFromCheckBox(\"chk_permConn\", makeDynString(\"permanentConnection\"));     
  }
  else if (dpTypeName($OBJECT) == \"VIDEO_OA_ENCODER\")
  {
    ptzMode = (rb_ptz_mode.number == 1) ? \"ptzMode=transparent\" : \"ptzMode=internal\";
    
    sLink = sLink + \";\" + ptzMode;
    
    if (ptzMode == \"ptzMode=transparent\")
      sLink = sLink + \";ptzPort=\" + ptz_port.text;
  } 
  
  sLink = sLink + \";\" +\"ctrlProtocol=\" + txt_control.userData(txt_control.selectedPos); // control =
  
  if (txt_preset.text > 0) // preset definintion
  {
    presetDef=\";presets=[1-\" + txt_preset.text + \"]\";
  }
  
//   DebugN(\"---------------------\"); 
//   DebugN($OBJECT);
//   DebugN(sLink);  
//   DebugN(sLink_RC0);
//   DebugN(\"---------------------\");
   
  sLink_RC0   = sLink + \";\" + sLink_RC0;
  
  strreplace(sLink_RC0,\";;;\",\";\"); 
  strreplace(sLink_RC0,\";;\",\";\");  // :-) clean link
  
  if (dpTypeName($OBJECT) == \"VIDEO_OA_CAMERA\")  
    definition = makeDynString(\"type=CAMERA_IP\"+presetDef);
  else if (dpTypeName($OBJECT) == \"VIDEO_OA_ENCODER\")  
    definition = makeDynString(\"type=ENCODER\");
  else
     definition = makeDynString(\"\");
  
  dpSet( $OBJECT + \".object.device.link\",       makeDynString(sLink_RC0,sLink_RC0,sLink_RC0 ),
         $OBJECT + \".object.device.definition\", definition,
         $OBJECT + \".object.device.controller\", makeDynString(txt_controller.text, txt_controller_2.text, txt_controller_3.text));
  
  chk_3.backCol = \"\";
  txt_preset.backCol = \"\";
  
  tab_edit.text = \"\"; // reset register icons
  
  chk_RC0_stream1.backCol = \"\";
  chk_RC0_stream2.backCol = \"\";
  chk_RC0_stream3.backCol = \"\";
  ptz_port.backCol = \"\";
      
  VIDEO_OA_objExpUnlockTree( $MAINMODULE, $MAINPANEL);  
  delay(0,200);
  updateLock = 0;  // unlock backCol update for input fields
  RC0_updateLock.text = 0;
  return 0;
}
