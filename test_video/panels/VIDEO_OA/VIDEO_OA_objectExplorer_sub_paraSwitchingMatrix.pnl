//----------------------------------------------------------------
/*
  VIDEO_OA_objExpUpdateParaSwitchingMatrix() 
  funtion to show the actual switching matrix parameter in the parameter panel
  
*/
VIDEO_OA_objExpUpdateParaSwitchingMatrix() 
{
  dyn_string asLink, asDefinition, asController;
  
  updateLock = 1;  // lock backCol update for input fields

  dpGet( $OBJECT + \".object.device.link\", asLink,
         $OBJECT + \".object.device.definition\", asDefinition,
         $OBJECT + \".object.device.controller\", asController);
  
  VIDEO_OA_formatDeviceDefinition(asDefinition);  
  VIDEO_OA_formatDeviceLink(asLink);   
  VIDEO_OA_formatControllerLink(asController);
  
  mapping objectInfo =  VIDEO_OA_objExpObjectInfoToMapping(asLink[1]);  // take control settings only from [1]
  
  dyn_string items = VIDEO_OA_objExpGetVideoHosts(dpSubStr($OBJECT, DPSUB_SYS), \"\", makeDynString(\"isVideoInterface\"));
  txt_controller.items = items;
  txt_controller_2.items = items;
  txt_controller_3.items = items;
  // combo box
  asController[1] = (asController[1] == \"\") ? \"_VIDEO_SERVER_1\" : asController[1];
  txt_controller.text = asController[1];
  
  asController[2] = (asController[2] == \"\") ? \"_VIDEO_SERVER_1\" : asController[2];
  txt_controller_2.text = asController[2];
  
  asController[3] = (asController[3] == \"\") ? \"_VIDEO_SERVER_2\" : asController[3];
  txt_controller_3.text = asController[3];
 


  // check box
  VIDEO_OA_objExpFillCheckBox(\"chk_1\", objectInfo, makeDynString(\"mode\"));
  
  VIDEO_OA_objExpFillKeyValueFields(VIDEO_OA_objExpGetKeyValueFields(), objectInfo);
  
  
  // encoder ptz mode
  string ptzMode = VIDEO_OA_filterKeywordValue(asLink[1], \"ptzMode\");
  
  // ptz protocol
  string protocol = VIDEO_OA_filterKeywordValue(asLink[1], \"ctrlProtocol\");
  string protocolName;

  if (protocol != \"\")
  {
    protocolName = getCatStr(\"voa_general\", \"switchingMatrixType_\" + protocol);
    txt_ctrlProtocol.text = protocolName;

    switch(protocol)
    {
      case \"GEVISOFT_SWITCHING_MATRIX\" : txt_key_encapsulation.editable = 0; 
                                         txt_key_encapsulation.enabled = 0; 
                                         txt_key_encapsulation.text = \"gsc\"; 
                                         
                                         break;
                                         
      case \"TEST_SWITCHING_MATRIX\" :     txt_key_encapsulation.editable = 0;                                           
                                         txt_key_encapsulation.text = \"tsm\";                                         
                                         txt_key_encapsulation.enabled = 0;
                                         
                                         break;
     
      default: txt_key_encapsulation.editable = 1; 
               txt_key_encapsulation.enabled = 0; 
               
    }
    

  }
  else
  {
    txt_ctrlProtocol.text = \"\";
  }  
  
  
  
  txt_ctrlProtocol.backCol = \"\";
 
  rb_ptzMode.backCol = \"\"; // reset backCol
  
  if (ptzMode == \"transparent\") // -> transparent PTZ mode
  {
    rb_ptzMode.number = 1; 
  }
  else
  {
    rb_ptzMode.number = 0;
  }    
  
  VIDEO_OA_objExpUnlockTree( $MAINMODULE, $MAINPANEL);  
  delay(0.200);
  updateLock = 0;  // unlock backCol update for input fields
}



//----------------------------------------------------------------
/*
 VIDEO_OA_objExpApplyParaChangesCameraAnalog() 
 funtion to update a camera datapoint after using the aplly or ok button
 in a parameter panel
  
*/
bool VIDEO_OA_objExpApplyParaChangesSwitchingMatrix() 
{   

  string sLink;
  string sController = txt_controller.text;
  string sFunction;
  string presetDef;
  string ptzMode;
  dyn_string definition;
  
  definition[1] = \"type=VIDEO_SWITCHING_MATRIX;para=1\";
  

  // check permission
  if (!VIDEO_OA_getUserPermission(\"perm_16\"))
  {
    VIDEO_OA_openNoPermission(\"perm_16\");
    return 1;
  }
 
  if (txt_ctrlProtocol.text == \"\") 
  {    
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_protocol\"));
    return 1;  
  }   

  if ( (VIDEO_OA_objExpCheckPort(txt_key_ctrlPort.text) != 1) && (txt_key_ctrlPort.visible == 1) ) 
  {    
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_port\"));
    return 1;  
  }
  
  if ( (VIDEO_OA_objExpCheckIpAddress(txt_key_ip.text) != 1) && (txt_key_ip.visible == 1) ) 
  {    
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_ip\"));
    return 1;  
  }
 

  
  updateLock = 1;  // lock backCol update for input fields
  
  sLink = sLink + VIDEO_OA_objExpGetLinkInfosFromCheckBox(\"chk_1\", makeDynString(\"mode\"));

  sLink = sLink + VIDEO_OA_objExpGetLinkInfosFromTextFields();
  
  sLink = sLink +  \"ctrlProtocol=\" + txt_ctrlProtocol.userData(txt_ctrlProtocol.selectedPos());
  
  ptzMode = (rb_ptzMode.number == 1) ? \"ptzMode=transparent\" : \"ptzMode=internal\";
    
  sLink = sLink + \";\" + ptzMode;
 
  strreplace(sLink,\";;;\",\";\"); 
  strreplace(sLink,\";;\",\";\");  // :-) clean link
 
  dpSet( $OBJECT + \".object.device.link\",       makeDynString(sLink, sLink, sLink),
         $OBJECT + \".object.device.definition\", definition,
         $OBJECT + \".object.device.controller\", makeDynString(txt_controller.text, txt_controller_2.text, txt_controller_3.text));

  VIDEO_OA_objExpUnlockTree( $MAINMODULE, $MAINPANEL);  
  delay(0,200);
  updateLock = 0;  // unlock backCol update for input fields
  //DebugN(definition,sLink); 
  return 0;
}" 0
