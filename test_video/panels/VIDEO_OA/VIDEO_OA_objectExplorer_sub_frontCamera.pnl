"main()
{
  
  dpConnect(\"VIDEO_OA_work_frontCamera\", $OBJECT + \".object.device.link\",
                                         $OBJECT + \".object.device.definition\",
                                         $OBJECT + \".object.device.name\");
}

VIDEO_OA_work_frontCamera(string dp1, dyn_string asLink,
                          string dp2, dyn_string asDefinition,
                          string dp3, string sName)
{
  VIDEO_OA_formatDeviceLink(asLink); 
  VIDEO_OA_formatDeviceDefinition(asDefinition); 
  string protocol;
  
 
  mapping objectInfo =  VIDEO_OA_objExpObjectInfoToMapping(asLink[1]);

  txt_name.text = sName;  
  
  VIDEO_OA_objExpFillKeyValueFields(VIDEO_OA_objExpGetKeyValueFields(), objectInfo);
 
  protocol = VIDEO_OA_objExpGetValueForKey(objectInfo,\"ctrlProtocol\");
  if (protocol != \"\")
    txt_field_ctrlProtocol.text = getCatStr(\"voa_general\", \"camType_\" + protocol);
  
  txt_definition.text = asDefinition[1];
  
  
  string type = VIDEO_OA_filterKeywordValue(asDefinition[1], \"type\");
  txt_definition.text = getCatStr(\"voa_general\",\"camType_\" + type);
  
  string protocolType = VIDEO_OA_filterKeywordValue(asLink[1], \"ptzProtocol\");
  txt_ptzProtocol.text = getCatStr(\"voa_general\", \"protocolType_\" + protocolType);
  
  string ptzMode = VIDEO_OA_filterKeywordValue(asLink[1], \"ptzMode\");
  txt_ptzMode.text = getCatStr(\"voa_general\", \"ptzMode_\" + ptzMode);
  
  if (type == \"CAMERA_IP\")
  {
    cover_ip.visible = 0;
    cover_analog.visible = 1;
  }
  else if (type == \"CAMERA_ANALOG\")
  {
    cover_ip.visible = 1;
    cover_analog.visible = 0;
  }
  else
  {
    cover_ip.visible = 1;
    cover_analog.visible = 1;
  }
}


