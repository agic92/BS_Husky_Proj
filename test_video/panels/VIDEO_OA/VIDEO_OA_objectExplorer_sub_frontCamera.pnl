"main()
{
  string cmd_1 = \"cmd=setpriorityvalues;streamIndex=1\";
  string cmd_2 = \"cmd=setpriorityvalues;streamIndex=2\";
  string cmd_3 = \"cmd=setpriorityvalues;streamIndex=3\";
  string currentStreamInfo;
  dyn_string streams;
  
  if(VIDEO_OA_getUserPermission(\"perm_16\"))
  { 
    int iAnswer;
    VIDEO_OA_openMessageInfoAnswer(getCatStr(\"voa_general\", \"priorities_reset_camera\"), 1, iAnswer);
    if(iAnswer == 1)
    {   
      dpGet($OBJECT + \".object.device.currentStreamInfo\",currentStreamInfo);
  
      streams[1] = VIDEO_OA_filterKeywordValue(currentStreamInfo,\"stream_1\");
      streams[2] = VIDEO_OA_filterKeywordValue(currentStreamInfo,\"stream_2\");
      streams[3] = VIDEO_OA_filterKeywordValue(currentStreamInfo,\"stream_3\");
  
  
      if(streams[1] != \"\")
        dpSet($OBJECT + \".object.command.request\", cmd_1);
  
      if(streams[2] != \"\")
        dpSet($OBJECT + \".object.command.request\", cmd_2);
  
      if(streams[3] != \"\")
        dpSet($OBJECT + \".object.command.request\", cmd_3);
      
      VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\", \"priorities_reseted_camera\"));
    }  
  }
  else
    VIDEO_OA_openNoPermission(\"perm_16\");
      
}


