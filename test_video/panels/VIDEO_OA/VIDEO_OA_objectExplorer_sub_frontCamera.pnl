VIDEO_OA_cameraFrontShowEncoderPoolNames(string dp, string streamInfo,
                                         string dp, dyn_string link)
{
  dyn_string streams,poolNames;
  dyn_bool activeStreams;
  
  VIDEO_OA_formatDeviceLink(link);
  poolNames = VIDEO_OA_getEncoderPoolNamesFromLink(link[1]);
  
  streams[1] = VIDEO_OA_filterKeywordValue(streamInfo,\"stream_1\");
  streams[2] = VIDEO_OA_filterKeywordValue(streamInfo,\"stream_2\");
  streams[3] = VIDEO_OA_filterKeywordValue(streamInfo,\"stream_3\");
  
  activeStreams[1] = (streams[1] == \"\") ? 0 : 1;
  activeStreams[2] = (streams[2] == \"\") ? 0 : 1;
  activeStreams[3] = (streams[3] == \"\") ? 0 : 1;  
 
//   for test 
//   activeStreams[1] = 1;
//   activeStreams[2] = 1;
//   activeStreams[3] = 1;  
  
  tab_pool.deleteAllLines();
  
  for (int i = 1; i <= 3; i++)
  {
    if (activeStreams[i])
    {
      
      for (int ii=1; ii <= dynlen(poolNames); ii++)
      {
        string prioMatrix = VIDEO_OA_createEncoderPoolPrioMatrix(ii, dynlen(poolNames));
        string cmd = \"cmd=setpriorityvalues;streamIndex=\" + i + \";camera_priority_value=\" + prioMatrix;
        tab_pool.appendLine(\"#1\", getCatStr(\"voa_general\",\"stream\") + \" \" + i + \" \" + getCatStr(\"voa_general\",\"poolName\") +\": \" + poolNames[ii], \"#3\", cmd, \"#4\", poolNames[ii]);
        tab_pool.cellWidgetRC((tab_pool.lineCount)-1, \"#2\", \"PushButton\", \"PRIO 1\");     
      }
    }

  }  
}

