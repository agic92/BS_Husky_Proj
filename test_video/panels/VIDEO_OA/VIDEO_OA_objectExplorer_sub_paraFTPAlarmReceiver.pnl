//----------------------------------------------------------------
/*
  VIDEO_OA_objExpUpdateParaFTPAlarmReceiver() 
  funtion to show the actual FTP alarm receiver parameter in the parameter panel
  
*/
VIDEO_OA_objExpUpdateParaFTPAlarmReceiver() 
{
  dyn_string asLink, asDefinition, asController;
  
  updateLock = 1;  // lock backCol update for input fields

  dpGet( $OBJECT + \".object.device.link\", asLink,
         $OBJECT + \".object.device.definition\", asDefinition,
         $OBJECT + \".object.device.controller\", asController);
  
  VIDEO_OA_formatDeviceDefinition(asDefinition);  
  VIDEO_OA_formatDeviceLink(asLink);   
  VIDEO_OA_formatControllerLink(asController);
  
  mapping objectInfo =  VIDEO_OA_objExpObjectInfoToMapping(asLink[1]);  // take control settings only from [1]
  
  string ftpWatchDirPath = VIDEO_OA_filterKeywordValue(asLink[1], \"ftpWatchDirPath\");
  string ftpexcludedsubdirlist = VIDEO_OA_filterKeywordValue(asLink[1], \"ftpexcludedsubdirlist\");
  
  bool ftpdeletefilesafterprocess = VIDEO_OA_filterKeywordValue(asLink[1], \"deletefilesafterprocess\");
  
  txt_ftpWatchDirPath.text = ftpWatchDirPath;
  txt_ftpexcludedsubdirlist.text = ftpexcludedsubdirlist;
  
  txt_key_ftpdeletefilesafterprocess.state(0) = ftpdeletefilesafterprocess;
  
  

  txt_controller.text = asController[1];  
  
  if (dpExists(asController[1]))
  {
    dyn_string controllerLink;
    dpGet(dpSubStr($OBJECT, DPSUB_SYS) + asController[1] + \".object.device.link\", controllerLink);
    VIDEO_OA_formatControllerLink(controllerLink); 
    txt_ctrl_hostname.text = VIDEO_OA_filterKeywordValue(controllerLink[1], \"hostname\");      
  }
  else
  {
    txt_ctrl_hostname.text = \"------------\";;
  }
  

  // check box
  VIDEO_OA_objExpFillCheckBox(\"chk_1\", objectInfo, makeDynString(\"mode\"));
  
  VIDEO_OA_objExpFillKeyValueFields(VIDEO_OA_objExpGetKeyValueFields(), objectInfo);
  
    
  
  VIDEO_OA_objExpUnlockTree( $MAINMODULE, $MAINPANEL);  
  delay(0.200);
  updateLock = 0;  // unlock backCol update for input fields
}



//----------------------------------------------------------------
/*
 VIDEO_OA_objExpApplyParaChangesFTPAlarmReceiver() 
 funtion to update a FTP alarm receiver datapoint after using the aplly or ok button
 in a parameter panel
  
*/
bool VIDEO_OA_objExpApplyParaChangesFTPAlarmReceiver() 
{   

  string sLink;
  string sController = txt_controller.text;
  string sFunction;
  string presetDef;
  string ptzMode;
  dyn_string definition;
  
  definition[1] = \"type=FTP_ALARM_RECEIVER;para=1\";
  

  // check permission
  if (!VIDEO_OA_getUserPermission(\"perm_16\"))
  {
    VIDEO_OA_openNoPermission(\"perm_16\");
    return 1;
  }
  

  if ( (VIDEO_OA_objExpCheckPort(txt_key_ftpport.text) != 1) && (txt_key_ftpport.visible == 1) ) 
  {    
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_port\"));
    return 1;  
  }
   

  
  if ( (VIDEO_OA_objExpCheckIpAddress(txt_key_ftpserver.text) != 1) && (txt_key_ftpserver.visible == 1) ) 
  {    
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"para_error_ip\"));
    return 1;  
  } 
  
  string ftpWatchDirPath = txt_ftpWatchDirPath.text; 
  string ftpexcludedsubdirlist = txt_ftpexcludedsubdirlist.text;
  
  bool ftpdeletefilesafterprocess = txt_key_ftpdeletefilesafterprocess.state(0);
  
  strreplace(ftpWatchDirPath, \"\\\\\", \"/\");
  strreplace(ftpexcludedsubdirlist, \"\\\\\", \"/\");
  
  updateLock = 1;  // lock backCol update for input fields
  
  sLink = sLink + \";ftpWatchDirPath=\" + ftpWatchDirPath + \";ftpexcludedsubdirlist=\" + ftpexcludedsubdirlist + \";\";
  
  sLink = sLink + \";deletefilesafterprocess=\" + (int)ftpdeletefilesafterprocess + \";\";
  
  sLink = sLink + VIDEO_OA_objExpGetLinkInfosFromCheckBox(\"chk_1\", makeDynString(\"mode\")) + \";\";

  sLink = sLink + VIDEO_OA_objExpGetLinkInfosFromTextFields();
  
  sLink = sLink + \";function=FTP_ALARM_RECEIVER;\";
 
  strreplace(sLink,\";;;\",\";\"); 
  strreplace(sLink,\";;\",\";\");  // :-) clean link
 
  dpSet( $OBJECT + \".object.device.link\",       makeDynString(sLink, sLink, sLink),
         $OBJECT + \".object.device.definition\", definition,
         $OBJECT + \".object.device.controller\", makeDynString(txt_controller.text, txt_controller.text, txt_controller.text));

  VIDEO_OA_objExpUnlockTree( $MAINMODULE, $MAINPANEL);  
  delay(0,200);
  updateLock = 0;  // unlock backCol update for input fields
  //DebugN(definition,sLink); 
  return 0;
}



