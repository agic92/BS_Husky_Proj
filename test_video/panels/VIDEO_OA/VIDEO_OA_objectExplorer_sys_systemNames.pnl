E "void VIDEO_OA_objExpUpdateParaSystemNames()
{
  string names;
  dyn_string showResultKey_1, showResultValue_1;

  dpGet($SYSTEM + \"_VIDEO_OA_MAIN.systemName\", names);

  dyn_dyn_string result = VIDEO_OA_objExpObjectInfoToDynString(names);
 
  for(int i = 1 ; i <= dynlen(result); i ++)
  {           
      dynAppend(showResultKey_1, result[i][1]);
      dynAppend(showResultValue_1, result[i][2]);            
  }
  
  tab_param.deleteAllLines();
  tab_param.columnFormat(\"#1\", \"[0s,,,ALIGNMENT_BEGINNING]\");
  tab_param.columnFormat(\"#2\", \"[0s,,,ALIGNMENT_CENTER]\");
  tab_param.appendLines(dynlen(showResultKey_1), \"#1\", showResultKey_1, \"#2\", showResultValue_1);
  tab_param.sort(\"#2\");
  tab_param.lineVisible(1);
    
}




bool VIDEO_OA_objExpApplyParaSystemNames()
{
  dyn_anytype lineResult;
  string  resultForDp;
  bool returnValue = 1;
  int count;
  
  dyn_string systemName;
  dyn_string interfaceName;
  
  tab_param.deleteLines(2, \"#1\", makeDynString(\"\"), \"#2\", makeDynString(\"\"));  // delete empty lines  
  
  count = tab_param.lineCount();
      
  for (int i = 0; i <= (count - 1); i++)
  {
    lineResult = tab_param.getLineN(i);

    if ((lineResult[1] == \"\") || (lineResult[2] == \"\"))
    {
      VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"externalAlerts_KeywordEmtyLine\") + \" \" + (i + 1)); 
      return 1; 
    }
    else
    { 
      strreplace(lineResult[2], \":\", \"\");
      lineResult[2] = lineResult[2] + \":\";
       
      lineResult[1] = strtolower(lineResult[1]);     
      
      
      resultForDp = resultForDp + \";\" + lineResult[1] + \"=\" + lineResult[2];
      dynAppend(interfaceName, lineResult[1]);
    }
  }
      
  if((dynlen(interfaceName)) != dynUnique(interfaceName)) 
  {
    DebugN(\"doppelte Keywords\");
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\", \"systemName_multipleInterface\") );    
    return 1;  
  }
  else
  {
    dpSet($SYSTEM + \"_VIDEO_OA_MAIN.systemName\", resultForDp);
    returnValue = 0;
  }
 
  return returnValue;
}



