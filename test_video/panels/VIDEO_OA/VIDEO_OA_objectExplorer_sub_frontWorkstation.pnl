VIDEO_OA_objExpShowWorkstationParameter(string dp1, int  defaultLive,
                                        string dp2, int  defaultPlayback,
                                        string dp3, int  manualSpeed,
                                        string dp4, int  activeCam,
                                        string dp5, int  activeMon,
                                        string dp6, bool keyboard,
                                        string dp7, dyn_string definition)
{
  
  VIDEO_OA_formatDeviceDefinition(definition);
  string isExportServer = VIDEO_OA_filterKeywordValue(definition[1], \"isExportServer\");
  
  cover_exportServer.visible = (isExportServer == \"1\") ? 0 : 1;
  
  defaultLive      = (defaultLive == 0)      ? 1 : defaultLive;
  defaultPlayback = (defaultPlayback == 0)   ? 1 : defaultPlayback;
  manualSpeed     = (manualSpeed == 0)       ? 20 : manualSpeed;
  
  txt_defaultLive.text     = \"Stream \" + defaultLive;
  txt_defaultPlayback.text = \"Stream \" + defaultPlayback;
  txt_manualSpeed.text     = manualSpeed + \" %\";
  
  txt_activeCam.text = activeCam;
  txt_activeMon.text = activeMon;   
}
                                        
