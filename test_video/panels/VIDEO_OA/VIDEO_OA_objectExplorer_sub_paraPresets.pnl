VIDEO_showPresetpara(string dp, string sNewValue)
{
  string sError    = VIDEO_OA_filterKeywordValue(sNewValue, \"error\");
  string sAnswer   = VIDEO_OA_filterKeywordValue(sNewValue, \"answer\");
  string sUserdata = VIDEO_OA_filterKeywordValue(sNewValue, \"userdata\");
  
  
  if(sUserdata==\"PRESETPARA\")
  {
   if (sError != \"\")
    setValue(\"\", \"text\", sError, \"backCol\", \"Video_state_alarm\");
   else if (sAnswer != \"\")
    setValue(\"\", \"text\", sAnswer, \"backCol\", \"Video_state_logged_in\");
   else
    setValue(\"\", \"text\", sNewValue, \"backCol\", \"\");
  }
  else
     setValue(\"\", \"text\", sNewValue,\"backCol\",\"\");
}
