"main(int row, string column, string value)
{
  // Camera Name 
  string sCameraCmd =$OBJECT+\".object.command.request\";
  string sCommand;
  bool permAdmin;
  
  bool permCmd=VIDEO_OA_getUserPermission(\"perm_9\");
  
  permAdmin=(row<=9) ? VIDEO_OA_getUserPermission(\"perm_7\"):VIDEO_OA_getUserPermission(\"perm_8\");
    
  if ((column==\"#2\")&& permCmd)// \"goto\" Button
    sCommand=\"cmd=move;presetno=\"+(string)(row+1);
  else if(column==\"#2\") // no permission \"goto\" Button
    VIDEO_OA_openNoPermission(\"perm_9\");
  
  else if((column==\"#3\")&&permAdmin) // \"set\" Button
   sCommand=\"cmd=setpreset;presetno=\"+(string)(row+1);
  else if((column==\"#4\")&&permAdmin) // \"delete\" Button 
    sCommand=\"cmd=deletepreset;presetno=\"+(string)(row+1);
  else if (((column==\"#3\")||(column==\"#4\"))&&(row<=9)) 
   VIDEO_OA_openNoPermission(\"perm_7\");
  else if ((column==\"#3\")||(column==\"#4\")) 
   VIDEO_OA_openNoPermission(\"perm_8\");
    
  if(sCommand!=\"\")
    dpSet(sCameraCmd,sCommand+\";userdata=PRESETPARA\");


}
