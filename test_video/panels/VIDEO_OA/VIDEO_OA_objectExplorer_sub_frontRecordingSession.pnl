"main()
{
  VIDEO_OA_alternatingRowColorsForTable();
  string recording = dpSubStr($OBJECT, DPSUB_DP);
  string sSystem = dpSubStr($OBJECT, DPSUB_SYS);
  string query = \"SELECT '_online.._value' FROM '*.object.device.link' REMOTE '\" + sSystem + \"' WHERE '_online.._value' LIKE \\\"*recording=\" + recording + \";*\\\"\" ;
  dyn_dyn_anytype tab, tab2;

  string comment;
  string num;  
  
  dpQuery(query, tab); 
  this.deleteAllLines();  


  for(int i = 2; i <= dynlen(tab); i++)
  {
    string dp = dpSubStr(tab[i][1], DPSUB_SYS_DP);

    if (dpTypeName(dp) == \"VIDEO_OA_CAMERA\")
    {
      comment =  dpGetDescription(dp + \".\");
      num = VIDEO_OA_getFormatedObjectNumber(dp);                     
      this.appendLine(\"num\", num, \"cam\", comment); 
    }
    else if(dpTypeName(dp) == \"VIDEO_OA_ENCODER\")
    {     
      query = \"SELECT '_online.._value' FROM '*.object.device.link' REMOTE '\" + sSystem + \"'  WHERE '_online.._value' LIKE \\\"*videoEncoder=\" + dpSubStr(dp, DPSUB_DP) + \"*\\\"\";
      dpQuery(query, tab2);
      
      for (int j = 2; j <= dynlen(tab2); j++)
      {  
        dp = dpSubStr(tab2[j][1], DPSUB_SYS_DP);
        if(dpTypeName(dp) == \"VIDEO_OA_CAMERA\")
        {
          comment =  dpGetDescription(dp + \".\");
          num = VIDEO_OA_getFormatedObjectNumber(dp);                     
          this.appendLine(\"num\", num, \"cam\", comment);
        }
      }     
    }     
  }
  this.sort(\"num\");
