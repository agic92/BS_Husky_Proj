VIDEO_OA_objExpFillStateTabRecordingServer(string object, 
                                           string state = \".object.device.state\", 
                                           string link  = \".object.device.link\")
{
  VIDEO_OA_alternatingRowColorsForTable(\"tab_server_1\");
  VIDEO_OA_alternatingRowColorsForTable(\"tab_server_2\");
  tab_server_1.sortOnClick = 0;
  tab_server_2.sortOnClick = 0;
  dpConnect(\"VIDEO_OA_objExpFillStateTabRecSession_work\", object + state, object + link);
}

VIDEO_OA_objExpFillStateTabRecSession_work(string dp1, string sState, string dp2, dyn_string aLink)
{
  dyn_dyn_string result = VIDEO_OA_objExpObjectInfoToDynString(sState);
  dyn_string showResultKey_1, showResultValue_1, showResultKey_2, showResultValue_2;
  int serverNum;
  dyn_errClass err;
  
  VIDEO_OA_formatDeviceLink(aLink);
  server_txt_1.text = VIDEO_OA_filterKeywordValue(aLink[1], \"recordingServer1\");
  server_txt_2.text = VIDEO_OA_filterKeywordValue(aLink[1], \"recordingServer2\");
  
  mode_txt.text           = getCatStr(\"voa_general\", \"mode_\" + VIDEO_OA_filterKeywordValue(aLink[1], \"mode\") );
  recordingMode_txt.text  = getCatStr(\"voa_general\", \"recordingMode_\" + VIDEO_OA_filterKeywordValue(aLink[1], \"recordingMode\") );
  redundancyMode_txt.text = getCatStr(\"voa_general\", \"redundancyMode_\" + VIDEO_OA_filterKeywordValue(aLink[1], \"redundancyMode\") ); 
  
  for(int i = 1 ; i <= dynlen(result); i ++)
  {  
    if (strpos(result[i][1], \"recordingServer1_\" ) == 0)
    {
      serverNum = 1;
      cover_server_1.visible = 0;
    }
    else if (strpos(result[i][1], \"recordingServer2_\" ) == 0)
    {
      serverNum = 2;
      cover_server_2.visible = 0;
    }
    else
      serverNum = 0;
    

    strreplace(result[i][1], \"recordingServer1_\", \"recordingServer_\");
    strreplace(result[i][1], \"recordingServer2_\", \"recordingServer_\"); 
    
    if(result[i][1] == \"recordingServer_state\")
    {     
      setValue(\"state_server_\" + serverNum, \"text\", getCatStr(\"voa_general\", \"state_txt_value_\" + result[i][2]));
      setValue(\"state_server_\" + serverNum, \"backCol\", VIDEO_OA_stateColor(\"state=\" + result[i][2]));
    }
    else if(result[i][1] == \"recordingServer_partitionUsage\")
    {     
      setValue(\"usage_percent_\" + serverNum, \"text\", result[i][2]);
      strreplace(result[i][2], \"%\", \"\");
      
      setValue(\"usage_\" + serverNum, \"scale\", 1, (float)result[i][2]/100.0);
    }
    else
    {      
      string sText = getCatStr(\"voa_general\", \"state_txt_\" + result[i][1]);
      err = getLastError();
      if (dynlen(err) == 0) //no error-> cat string found
        result[i][1] = sText;
      else
        result[i][1] = result[i][1];
    
      if (serverNum == 1)
      {
        dynAppend(showResultKey_1, result[i][1]);
        dynAppend(showResultValue_1, result[i][2]);
      }
      else if (serverNum == 2)
      {
        dynAppend(showResultKey_2, result[i][1]);
        dynAppend(showResultValue_2, result[i][2]);
      }
    }    
  } 
  
  tab_server_1.deleteAllLines();
  tab_server_1.columnFormat(\"#1\", \"[0s,,,ALIGNMENT_BEGINNING]\");
  tab_server_1.columnFormat(\"#2\", \"[0s,,,ALIGNMENT_CENTER]\");
  tab_server_1.appendLines(dynlen(showResultKey_1), \"#1\", showResultKey_1, \"#2\", showResultValue_1);
  tab_server_1.sort(\"#1\");
  tab_server_1.lineVisible(1);
   
  tab_server_2.deleteAllLines();
  tab_server_2.columnFormat(\"#1\", \"[0s,,,ALIGNMENT_BEGINNING]\");
  tab_server_2.columnFormat(\"#2\", \"[0s,,,ALIGNMENT_CENTER]\");
  tab_server_2.appendLines(dynlen(showResultKey_2), \"#1\", showResultKey_2, \"#2\", showResultValue_2);
  tab_server_2.sort(\"#1\");
  tab_server_2.lineVisible(1);   
}


VIDEO_OA_objExpShowObjectStateField_work(string dp, string state)
{
  string realState = VIDEO_OA_filterKeywordValue(state, \"state\");
  
  this.backCol = VIDEO_OA_stateColor(state, dpSubStr(dp, DPSUB_SYS_DP));
  
  this.text = getCatStr(\"voa_general\", \"state_txt_value_\" + realState);
             
