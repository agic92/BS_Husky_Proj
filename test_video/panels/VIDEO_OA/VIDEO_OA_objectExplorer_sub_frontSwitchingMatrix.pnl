/*
  VIDEO_OA_objExpFillStateTabSwitchingMatrix(string object, string element = \".object.device.state\")
  function to fill function table for  a switchingMatrix
  @author MN

  @param object     : object Name
  @param element    : element name (default:.object.device.state) 
*/
VIDEO_OA_objExpFillStateTabSwitchingMatrix(string object, string element = \".object.device.state\")
{
  VIDEO_OA_alternatingRowColorsForTable();
  this.sortOnClick = 0;
  this.enableColumnResize(FALSE); 

  dpConnect(\"VIDEO_OA_objExpFillStateTabSwitchingMatrix_work\", object+element);
}
//----------------------------------------------------------------



//----------------------------------------------------------------
/*
  VIDEO_OA_objExpFillStateTabSwitchingMatrix_work(string dp1, string sState)
  work function for VIDEO_OA_objExpFillStateTabSwitchingMatrix()
  @author MN

*/
VIDEO_OA_objExpFillStateTabSwitchingMatrix_work(string dp1, string sState)
{
  
  dyn_dyn_string result = VIDEO_OA_objExpObjectInfoToDynString(sState);
  dyn_string showResultKey,showResultValue;
  dyn_errClass err;
  
 
  for(int i = 1 ; i <= tab_connect.lineCount(); i ++)
  {
    if ( tab_connect.lineCount() > 0 )
      tab_connect.cellValueRC(i - 1, \"#5\" , \"\"); 
  }
  
  for(int i = 1 ; i <= dynlen(result); i ++)
  {  
    if( ( strlen(result[i][1]) < 4 ) || ( substr(result[i][1],0,4) != \"out_\" ) )
    {        
      string sText = getCatStr(\"voa_general\", \"state_txt_\" + result[i][1]);
      err = getLastError();
      if (dynlen(err) == 0) //no error-> cat string found
        result[i][1] = sText;
      else
        result[i][1] = result[i][1];
      
      dynAppend(showResultKey, result[i][1]);
    
      string sText = getCatStr(\"voa_general\", \"state_txt_value_\" + result[i][2]);
      err = getLastError();
      if (dynlen(err) == 0) //no error-> cat string found
        result[i][2] = sText;
      else
        result[i][2] = result[i][2];
    
      dynAppend(showResultValue, result[i][2]);
    }
    else
    {
      strreplace(result[i][1], \"out_\", \"\");
      strreplace(result[i][2], \"in_\", \"\");
      tab_connect.updateLine (1, \"#3\", result[i][1], \"#3\", result[i][1], \"#5\", VIDEO_OA_objExpGetCameraNameFromMatrixInputPort(dpSubStr($OBJECT,DPSUB_DP) ,result[i][2], dpSubStr($OBJECT,DPSUB_SYS)) + \"  (\" + result[i][2] + \")\");   
    }
  }
                       
  tab_function.deleteAllLines();
  tab_function.columnFormat(\"#1\", \"[0s,,,ALIGNMENT_BEGINNING]\");
  tab_function.columnFormat(\"#2\", \"[0s,,,ALIGNMENT_CENTER]\");
  tab_function.appendLines(dynlen(showResultKey), \"#1\", showResultKey, \"#2\", showResultValue);
  tab_function.sort(\"#1\");
  tab_function.lineVisible(1);
  
}
