dyn_string VIDEO_OA_objExpGetCameraNameFromMatrixInputPort(string matrixInputPort, string sSystem=getSystemName())
{
  dyn_string result;
  string query = \"SELECT '_online.._value' FROM 'Camera_*.object.device.link' REMOTE '\" + sSystem + \"' WHERE '_online.._value' LIKE \\\"*switchingMatrix=\" + dpSubStr($OBJECT, DPSUB_DP) + \";*\\\" AND  _DPT = \\\"VIDEO_OA_CAMERA\\\"\";
  dyn_dyn_anytype tab;
  dpQuery(query, tab);
  
  for( int i = 2; i <= dynlen(tab); i++)
  {
    string matrixInputPort = VIDEO_OA_filterKeywordValue(tab[i][2],\"matrixInputPort\");
    string cameraInfo = dpSubStr((tab[i][1]), DPSUB_DP) + \" (\" + matrixInputPort +\")\";
    string comment = dpGetDescription(dpSubStr((tab[i][1]), DPSUB_SYS_DP)+\".\");
    
    if( (comment != dpSubStr((tab[i][1]), DPSUB_SYS_DP)+\".\") && (comment != dpSubStr((tab[i][1]), DPSUB_DP)+\".\") )
      cameraInfo = cameraInfo + \" - \" + comment;
    dynAppend(result, cameraInfo);
  }  
  return result;
}
