/*
  VIDEO_OA_objExpGetCameraNameFromMatrixInputPort(string switchingMatrix, string matrixInputPort, string sSystem=getSystemName())
  function to get the camera datapoint name for a switching matrix output address
  
  @author MN
  @param object     : switchingMatrix
  @param element    : matrixInputPort
  @param element    : matrixInputPort
  @param element    : sSystem
  
  @return : one camera found -> camera datapoint
            unknown camera found -> ?
            more than on camera found -> \",\" seperate list with all cameras
  

*/
string VIDEO_OA_objExpGetCameraNameFromMatrixInputPort(string switchingMatrix, string matrixInputPort, string sSystem=getSystemName())
{
  string result;
  switchingMatrix = dpSubStr(switchingMatrix, DPSUB_DP);
  string query = \"SELECT '_online.._value' FROM 'Camera_*.object.device.link' REMOTE '\" + sSystem + \"' WHERE '_online.._value' LIKE \\\"*matrixInputPort=\"+ matrixInputPort +\";*\\\" AND '_online.._value' LIKE \\\"*switchingMatrix=\"+ switchingMatrix +\";*\\\" AND _DPT = \\\"VIDEO_OA_CAMERA\\\"\";
  dyn_dyn_anytype tab;
  dpQuery(query, tab);
   
  if(dynlen (tab) == 2) // correct result. one camera found
    result = dpSubStr(tab[2][1], DPSUB_DP);
  
  else if (dynlen (tab) == 1)  // no camera for this addresse
    result = \"?\";
  
  else
  {
    for( int i = 2; i <= dynlen(tab); i++)
      result = result + \",\" + dpSubStr(tab[i][1], DPSUB_DP); // more than on camera for this address
  }
  
  return result;
}
