dyn_dyn_string VIDEO_OA_objExpGetConnectedChannelsForSwitchingMatrix (string encoderDp = \"\", string shapeName)
{
  dyn_dyn_anytype tab;

  string query;
  
  setValue(shapeName, \"sortOnClick\", 0);
  
  if( dpExists(encoderDp))
  {
    string sSystem = dpSubStr(encoderDp, DPSUB_SYS);

    query = \"SELECT '_online.._value' FROM '*.object.device.link' REMOTE '\" + sSystem + \"'  WHERE '_online.._value' LIKE \\\"*switchingMatrix=\" + dpSubStr(encoderDp, DPSUB_DP) + \"*\\\" AND  _DPT = \\\"VIDEO_OA_ENCODER_POOL_CHANNEL\\\"\";
    
    dpQueryConnectAll(\"VIDEO_OA_objExpGetConnectedCamerasForEncoderWork\", 1, shapeName, query);
  }


}
VIDEO_OA_objExpGetConnectedCamerasForEncoderWork(anytype userData, dyn_dyn_anytype tab) 
{     
  dyn_dyn_string result;
  
  shape widget = getShape(userData);
  widget.deleteAllLines();  
    
  for (int i = 2; i <= dynlen(tab); i++)
  {
    string channel = dpSubStr( tab[i][1], DPSUB_SYS_DP);
    string videoEncoder = VIDEO_OA_filterKeywordValue(tab[i][2], \"videoEncoder\");
    string videoPort = VIDEO_OA_filterKeywordValue(tab[i][2], \"videoPort\");
    string encoderPool = VIDEO_OA_filterKeywordValue(tab[i][2], \"encoderPool\");
    string matrixOutputPort = VIDEO_OA_filterKeywordValue(tab[i][2], \"matrixOutputPort\");
      
    result[dynlen(result) + 1][1] = channel;
    result[dynlen(result)    ][2] = videoEncoder;
    sprintf(videoPort, \"%03d\", (int)videoPort);     // format for sort
    result[dynlen(result)    ][3] =  videoPort;   
    
    result[dynlen(result)    ][4] =  matrixOutputPort;
    result[dynlen(result)    ][5] =  encoderPool;  
     
  }    

  dynDynSort(result,1); // sort result 

  for (int i = 1; i <= dynlen(result); i++)
  { 
    widget.updateLine( 1, \"#3\", result[i][4], \"#1\", dpGetComment(result[i][1] + \".\"), \"#2\", result[i][2] + \"  (\" + result[i][3] + \")\", \"#3\", result[i][4], \"#4\", result[i][5] );  
  }
  
  
  // upgdate camera column
  string sState;
  dpGet($OBJECT + \".object.device.state\", sState);
  VIDEO_OA_objExpFillStateTabSwitchingMatrix_work($OBJECT, sState);
}





