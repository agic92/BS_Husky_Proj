                                                 , $OBJECT + \".object.device.controller\" );
} 
VIDEO_OA_objExpShowLinkTypeDefault(string dp1, dyn_string asLink,
                                   string dp2, dyn_string asDefinition,
                                   string dp3, dyn_string asController)
{
  
  VIDEO_OA_formatControllerLink(asController);
  VIDEO_OA_formatDeviceDefinition(asDefinition);  
  VIDEO_OA_formatDeviceLink(asLink);
 
  txt_controller.text   =  asController[1];
  txt_controller_2.text =  asController[2];
  txt_controller_3.text =  asController[3];
      
  txt_definition.text      = asDefinition[1];  
  txt_link.text      = asLink[1]; 
  txt_link_2.text    = asLink[2];
  txt_link_3.text    = asLink[3]; 
}
