bool VIDEO_OA_applyParaChanges()
{
  dyn_string asType = makeDynString( txt_definition.text);
  dyn_string asLink = makeDynString( txt_link.text, txt_link_2.text, txt_link_3.text);
  dyn_string asController = makeDynString( txt_controller.text, txt_controller_2.text, txt_controller_3.text);
  
  updateLock = 1;

  // check permission
  if (!VIDEO_OA_getUserPermission(\"perm_16\"))
   {
     VIDEO_OA_openNoPermission(\"perm_16\");
     return 1;
   }

  dpSet($OBJECT + \".object.device.definition\", asType,
        $OBJECT + \".object.device.link\", asLink,
        $OBJECT + \".object.device.controller\", asController);
  
  txt_controller.backCol   = \"\";
  txt_controller_2.backCol = \"\";
  txt_controller_3.backCol = \"\";
      
  txt_definition.backCol = \"\";
  txt_link.backCol       = \"\";
  txt_link_2.backCol     = \"\";
  txt_link_3.backCol     = \"\";
  
  VIDEO_OA_objExpUnlockTree($MAINMODULE, $MAINPANEL);
  delay(0,200);
  updateLock = 0;
  return 0;
}



