"main()
{
  dyn_string camList;
  dyn_int lines=tab.getSelectedLines();
  int iAnswer;
  dyn_bool streamSetting, abStreams;
 
  streamSetting[1] = chk_streamSetting.state(0);
  streamSetting[2] = chk_streamSetting.state(1);
  streamSetting[3] = chk_streamSetting.state(2); 
  
  VIDEO_OA_openMessageInfoAnswer(getCatStr(\"voa_general\",\"export_changeStreamSettings\"), 1, iAnswer);
  if(iAnswer != 1)
    return;
  
  if(dynlen(lines) >= 1)
  {
   for(int i = 1; i <= dynlen(lines); i++)
    {
     dynAppend(camList, tab.cellValueRC(lines[i], \"#3\"));
     
          abStreams = VIDEO_OA_getStreamNumbers(camList[i], \"playback\");
          string comment = dpGetDescription(camList[i] + \".\");
           tab.updateLine(1, \"#3\", camList[i], \"#3\", camList[i], 
                         \"#2\", comment, 
                         \"#1\", VIDEO_OA_getFormatedObjectNumber(camList[i]),
                         \"#4\", (abStreams[1] == 1) ? streamSetting[1] : \"--\",
                         \"#5\", (abStreams[2] == 1) ? streamSetting[2] : \"--\",
                         \"#6\", (abStreams[3] == 1) ? streamSetting[3] : \"--\");     
     
     
     
     
     
    }
  }
