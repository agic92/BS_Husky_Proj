"main()
{
  string sSystem = \"\"; 
  
  dyn_string dpsBasic=dpNames(\"\" + \"Camera_*\", \"VIDEO_OA_CAMERA\");
  dyn_string dps;
  dyn_string dps_comment;
  dyn_string dps_name;
  dyn_string number;
  
  dyn_bool abStreams;
  
  VIDEO_OA_alternatingRowColorsForTable();
  
  this.selectByClick(TABLE_SELECT_LINE);
  this.tableMode(TABLE_SELECT_MULTIPLE);
  
  for(int i = 1; i <= dynlen(dpsBasic); i++)
  {
    abStreams = VIDEO_OA_getStreamNumbers(dpsBasic[i], \"playback\");
    if(dynContains(abStreams,1) > 0)
    {
      dynAppend(dps, dpSubStr(dpsBasic[i], DPSUB_DP));
      dynAppend(dps_name, dpSubStr(dpsBasic[i], DPSUB_SYS_DP));
      dynAppend(dps_comment, dpGetComment(dpsBasic[i]+\".\"));
      dynAppend(number, VIDEO_OA_getFormatedObjectNumber(dpsBasic[i])) ; 
    }
  }
  this.deleteAllLines(); 
  this.appendLines(dynlen(dps), \"#1\", dps, \"#2\", dps_comment, \"#3\", dps_name, \"#4\", number);
  this.lineVisible(0);
  if(dynlen(dps) < 1)
    VIDEO_OA_openMessageWarning(getCatStr(\"voa_general\",\"export_noPlaybackCamsFound\"));
}
