work(string dp, string response)
{
  string userdata=VIDEO_OA_filterKeywordValue(response,\"userdata\");
  string answer=VIDEO_OA_filterKeywordValue(response,\"answer\");
  string error=VIDEO_OA_filterKeywordValue(response,\"error\");  
  string message;
  string backColor;
  
  
  
  if(userdata == VIDEO_OA_createUserdataForCmd())
  {
    if (answer == \"duplicate export name\")
    {
      VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\", \"export_duplicateExport\"));
      backColor = \"Video_export_error\";
      message = getCatStr(\"voa_general\",\"export_error\");
