"DragDrop" "main(string information, int dragType, int row, string column)
{
  dyn_string camList;
  dyn_bool abStreams;
  
  strreplace(information,\" | \",\"|\" );
  camList = strsplit(information, \" | \");

  if ((dynlen(camList) > 1) && (camList[1] == \"cam_list\"))
  {
    dynRemove(camList,1); // delete idendifier
         
    for (int i = 1; i <= dynlen(camList); i++)
    {
      abStreams = VIDEO_OA_getStreamNumbers(camList[i], \"playback\");
      string comment = dpGetDescription(camList[i] + \".\");
     if(dynContains(abStreams,1) > 0)
      this.updateLine(1, \"#3\", camList[i], \"#3\", camList[i], 
                         \"#2\", comment, 
                         \"#1\", VIDEO_OA_getFormatedObjectNumber(camList[i]),
                         \"#4\", (abStreams[1] == 1) ? true : \"--\",
                         \"#5\", (abStreams[2] == 1) ? true : \"--\",
                         \"#6\", (abStreams[3] == 1) ? true : \"--\");
    }    
  }                                                               
}


