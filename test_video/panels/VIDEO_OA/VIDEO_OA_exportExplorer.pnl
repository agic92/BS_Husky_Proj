"main()
{
  dyn_string ds;
  dyn_float  df;
  dyn_anytype cameras;
  dyn_bool protectOption;
  dyn_string protectPuffer;
  string selectetCameras = \";source=\";
  string exportCmd;
  string exportCmdComplete;
  string dp = VIDEO_OA_getWorkstationDp();
  string cameraComment;
  string cameraNum;
  string camPrefix = getCatStr(\"video\",\"camera\")+\"_\";
  
  string userdata=VIDEO_OA_createUserdataForCmd();
  
  getValue(\"tab\",\"getColumnN\",0,cameras);


  ds[1]=filenameTxt.text;
  ds[2]=txt_export.text;
  ds[3]=1;
  df[1]=expStartPos.text;
  df[2]=expEndPos.text;
  
  // check permission
  if(!VIDEO_OA_getUserPermission(\"perm_21\"))
  {
    VIDEO_OA_openNoPermission(\"perm_21\");
    return;
  }
  
  if(dynlen(cameras)<=0)
  {
    VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"noCameraSelection\"));
    return;
  }
  
  if(!dpNameCheck(ds[1])||(ds[1]==\"\"))
  {
    VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"invalidFilename\"));
    return;
  }
  else
    ds[1] = \"export_\" + ds[1];
  
 
  
  if((ds[2]==\"\"))
  {
    VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"invalidFolder\"));
    return;
  }
  
  if(access(ds[2]  ,W_OK) != 0)
  {
    VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"invalidFolder\"));
    return;
  }
  
  
  
  if(df[1]>=df[2])
  {
    VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"invalidTime\"));
    return;
  }
  
  if(df[2]>=getCurrentTime())
  {
    VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"export_time_in_future\"));
    return;
  }
  
  string password = txt_password.text;
  string repeatPassword = txt_repeatPassword.text;
  string user = txt_user.text;
  
  if((password != \"\") && (user == \"\"))
  {
    VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"export_no_user\"));
    return;
  }
  
  if((password == \"\") && (user != \"\"))
  {
    VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"export_no_password\"));
    return;
  }
  
  if(password != repeatPassword)
  {
    VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"export_password_not_equal\"));
    return;
  }
    
  int answer; 

  if(password != \"\")
  {
  VIDEO_OA_openMessageInfoAnswer(getCatStr(\"voa_general\", \"really_export_with_password\"), 2, answer);
  if(answer != 1)
    return;
  }
  
  if(password == \"\")
  {
  VIDEO_OA_openMessageInfoAnswer(getCatStr(\"voa_general\", \"really_export_without_password\"), 2, answer);
  if(answer != 1)
    return;
  }
   
  VIDEO_OA_setMyModuleEnabled(0);
  
  
  for(int i=1;i<=dynlen(cameras);i++)
  {
   cameras[i]=dpSubStr(cameras[i],DPSUB_DP); 
   
   cameraComment = dpGetDescription(cameras[i]+\".\");
                           
   if(i<dynlen(cameras))
    selectetCameras=selectetCameras+cameras[i]+\",\";
   else // last
    selectetCameras=selectetCameras+cameras[i];
   
  }

  string sFile=ds[2] + \"/\" + +ds[1];
  
  exportCmd=\"cmd=export;posupdateinterval=2000;begin=\"+df[1]+\";end=\"+df[2]+\";name=\"+sFile;
  
  if((user != \"\") && (password != \"\"))
  {
    exportCmd = exportCmd + \";password=\" + password + \";username=\" + user;
  }
  
  // alles in einem Befehl
  exportCmdComplete=exportCmd+selectetCameras;
  
  exportCmdComplete=exportCmdComplete+\";userdata=\"+userdata;
  
  //DebugTN(exportCmdComplete);
 
  dpSet(dp + \".exportServer.object.command.request\", exportCmdComplete);
   
  if(pathMode.number == 0)  
    dpSet(dp + \".exportServer.lastExportPath\", ds[2]);
  else
    dpSet(dp + \".exportServer.lastExportPath\", \"\");
    
  
  VIDEO_OA_exportNewLog(dp, \"exportLogTxt_start\", sFile, getUserName());
  VIDEO_OA_setMyModuleEnabled(1);
}

