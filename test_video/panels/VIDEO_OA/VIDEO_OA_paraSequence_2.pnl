E "VIDEO_OA_paraList_updateSequenceList (string widget)
{
  dyn_dyn_string list = VIDEO_OA_paraList_getAllSequences();
  dyn_string items;
  shape target = getShape(widget);
  
  for(int i = 1; i <= dynlen(list); i++)
    dynAppend(items, list[i][2]);
  
  target.items = items;  
  
  for(int i = 1; i <= dynlen(list); i++)
    target.userData(i, list[i][1]);
}


dyn_dyn_string VIDEO_OA_paraList_getAllSequences()
{  

  dyn_string tab = dpNames(\"*\",\"_VIDEO_OA_SEQUENCE\");  
  dyn_dyn_string result;
  string name;
  string dp;

  for (int i = 1; i <= dynlen(tab); i++)
  {
    dp = dpSubStr(tab[i], DPSUB_SYS_DP);
    dpGet( dp + \".name\", name);
    result[i][1] = dp;
    result[i][2] = name;      
  }
  
  dynDynSort(result, 2);
  return result;  
}

VIDEO_OA_paraList_updateSequenceConfig(string dp)
{
  string name;
  dyn_string paraListName;
  int periodTime;
  int mode;
  dyn_string target;
  
  dyn_string items;
  dyn_dyn_string list = VIDEO_OA_paraList_getAll(\"cam_list\");

  dyn_string displayDp = dpNames(dpSubStr(dp, DPSUB_SYS) + \"*\",\"VIDEO_OA_DISPLAY\");
  
  
  for(int i=1; i <= dynlen(displayDp); i++)
  {
    if (strpos(displayDp[i],\"_mp_VIDEO_OA_DISPLAY\") < 0)
      dynAppend(items,dpSubStr(displayDp[i],DPSUB_DP));
  }  

  cb_display.items = items;

  dynClear(items);
  
  for( int i = 1; i <= 255; i++)
  {
    items[i] = i;
  }
  cb_manNum.items = items;
  
  dynClear(items);
    
  for( int i = 1; i <= 60; i++)
  {
    items[i] = i;
  }
  cb_time.items = items;
  
  dynClear(items);
  
  for( int i = 1; i <= 24; i++)
  {
    items[i] = i;
  }
  cb_dialog.items = items;

  dynClear(items);
  
  for(int i = 1; i <= dynlen(list); i++)
    dynAppend(items, list[i][2]);
  
  cb_config.items = items;  
  
  for(int i = 1; i <= dynlen(list); i++)
    cb_config.userData(i, list[i][1]);
  
  dynClear(items);
  
  cb_switchingMatrix.items = VIDEO_OA_getAllDpsOfType(\"VIDEO_OA_SWITCHING_MATRIX\");
  
  dyn_string dsCams = VIDEO_OA_getAllDpsOfType(\"VIDEO_OA_CAMERA\");
  dyn_string dsDevLink, dsDevLinkSplit, dsSwitchingMatrix, dsAssignedCams;
  
  for ( int i = 1; i <= dynlen(dsCams); i++ )
  {
    dpGet(dsCams[i] + \".object.device.link\", dsDevLink);
    dsDevLinkSplit = strsplit(dsDevLink[1], \";\");
    
    for ( int j = 1; j <= dynlen(dsDevLinkSplit); j++ )
    {
      if ( strpos(dsDevLinkSplit[j], \"switchingMatrix=\") != -1 )
      {
        dsDevLinkSplit[j] = substr(dsDevLinkSplit[j], strlen(\"switchingMatrix=\"));
        dynAppend(dsSwitchingMatrix, dsDevLinkSplit[j] + \"|\" + dsCams[i]);
      }
    }
  }
  
  for ( int i = 1; i <= dynlen(dsSwitchingMatrix); i++ )
    if ( strpos(dsSwitchingMatrix[i], cb_switchingMatrix.text+\"|\") != -1 )
      dynAppend(dsAssignedCams, strsplit(dsSwitchingMatrix[i], \"|\")[2]);
  
  cb_camera.items = dsAssignedCams;
  //cb_camera.items = VIDEO_OA_getAllDpsOfType(\"VIDEO_OA_CAMERA\");

  
  dpGet( dp + \".name\", name,
         dp + \".paraListName\", paraListName,
         dp + \".time\", periodTime,
         dp + \".mode\", mode,
         dp + \".target\", target);
 

  string configName;
  

  
  if ((dynlen(target) != 4 ) && (dynlen(target) != 8 ))// invalid parameter
   target = makeDynString( \"display\",\"\",\"0\",\"\");
  
  if(target[1] == \"ewo\")
  {
    rb_mode.number = 0;
    txt_ewo.text = target[2];
    txt_ewo.visible = 1;
    cb_display.visible = 0;
    cb_manNum.visible = 1;
    txt_manNum.visible = 1;
  }
  else
  {
    rb_mode.number = 1;
    cb_display.text = target[2];
    txt_ewo.visible = 0;
    cb_display.visible = 1;
    cb_manNum.visible = 0;
    txt_manNum.visible = 0;
  } 
  
  cb_time.text = (periodTime == 0) ? 1 : periodTime;
  cb_dialog.text = (target[3] == 0) ? 1 : target[3];
  cb_manNum.text = (target[4] == \"\") ? myManNum() : target[4];
  
  if(dpExists(paraListName))
  {
    dpGet(paraListName + \".name\", configName);
  }
  cb_config.text = configName;
  
  if ((dynlen(target) == 8 ) && (target[5] == \"matrixsequence\" )) // matrixsequence
  {
    cb_matrix.state(0) = 1;
    cb_camera.text = target[6];
    cb_switchingMatrix.text = target[7];
    txt_address.text = target[8];
    coverMatrix.visible = 0;
  }
  else
  { 
    cb_matrix.state(0) = 0;
    coverMatrix.visible = 1;
  }    
}
 

