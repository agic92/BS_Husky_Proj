"main()
{ 
  string query = \"SELECT 'mode:_online.._value', 'lastPosition:_online.._value', 'paraListName:_online.._value', 'time:_online.._value', 'name:_online.._value', 'target:_online.._value' FROM '*' WHERE _DPT = \\\"_VIDEO_OA_SEQUENCE\\\"\";
  dyn_dyn_anytype tab;
  
  table.columnFormat(\"#3\",\"[0s,,,ALIGNMENT_CENTER]\");
  VIDEO_OA_alternatingRowColorsForTable();
  
  dpQueryConnectSingle(\"VIDEO_OA_showSequences\", 1, \"VIDEO_OA_showSequences\", query);  
}

VIDEO_OA_showSequences(anytype userData, dyn_dyn_anytype val) 
{  
  for (int i = 2; i <= dynlen(val); i++) 
  {    
    string name = val[i][6];
    int mode = val[i][2];
    string dp = dpSubStr(val[i][1], DPSUB_DP);
    
    string modePic;
    mapping keys;
    dyn_int lines;
    
    table.updateLine(1, \"#1\", dp, \"#1\", dp, \"#2\", name);
    
    keys[\"#1\"] = makeDynString(dp);
    
    lines = this.lineNumbers(keys);
            
    table.cellWidgetRC(lines[1], \"#4\", \"PushButton\", \"...\");
    
    table.cellBackColRC(lines[1], \"#3\", VIDEO_seq_getModeCol(mode));
    
    table.cellFillRC(lines[1], \"#3\", \"[pattern,[fit,any,\" + VIDEO_seq_getModePic(mode) + \"]]\" );
  }   
}


