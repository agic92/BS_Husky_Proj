  }
  
  for ( int i = 1; i <= dynlen(dsSwitchingMatrix); i++ )
    if ( strpos(dsSwitchingMatrix[i], this.text+\"|\") != -1 )
      dynAppend(dsAssignedCams, strsplit(dsSwitchingMatrix[i], \"|\")[2]);
  
  cb_camera.items = dsAssignedCams;
}
