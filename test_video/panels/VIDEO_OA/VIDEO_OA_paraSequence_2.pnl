"main()
{
  string answer;
  dyn_dyn_string tab;
  dyn_string names;
  
  if (!VIDEO_OA_getUserPermission(\"perm_4\"))
  {
    VIDEO_OA_openNoPermission(\"perm_4\");
    return;
  }
  
  VIDEO_OA_openMessageInput(getCatStr(\"voa_general\",\"paraList_configName\"),\"%s\",\"\",answer);
  
  tab = VIDEO_OA_paraList_getAllSequences();
  
  for (int i=2; i<=dynlen(tab); i++)
  {
    dynAppend(names, tab[i][2]);        
  }

  if( dynContains(names, answer) > 0 )
    VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"paraList_configAlreadyExists\"));
  else if(answer != \"\")
  {
    string dp = VIDEO_OA_paraList_getNextName(\"_Sequence_%05d\");
    dpCreate(dp, \"_VIDEO_OA_SEQUENCE\");  
    
    dpSet(dp + \".paraListName\", \"\",
          dp + \".time\", 5,
          dp + \".name\", answer,
          dp + \".target\", makeDynString( \"display\",\"\",\"0\",\"\"),
          dp + \".mode\", -1);

    VIDEO_OA_paraList_updateSequenceList(\"txt_configs\");
    
    dyn_string items = txt_configs.items;
    
    int actPos;
    for (int i = 1; i <= dynlen(items); i++)
    {
      if ( items[i] == answer )
        actPos = i;
    }
    
    txt_configs.selectedPos = actPos;    
    
    VIDEO_OA_paraList_updateSequenceConfig(VIDEO_OA_paraList_getSequenceDpFromName(txt_configs.text));
    
    cover_config.visible = 0;
    cover_config_text.visible = 0;
  }
  else
    VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"paraList_configEmptyName\")); 
}


