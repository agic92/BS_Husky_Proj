  string dp = txt_configs.userData(txt_configs.selectedPos);
  string answer;
  dyn_string target;
  dyn_dyn_string tab = VIDEO_OA_paraList_getAllSequences();
  dyn_string names;
  
  if (!VIDEO_OA_getUserPermission(\"perm_5\"))
  {
    VIDEO_OA_openNoPermission(\"perm_5\");
    return;
  }
  
  if(dp != \"\")
  {  
    VIDEO_OA_openMessageInput(getCatStr(\"voa_general\",\"paraList_configNewName\"),\"%s\",\"\",answer);
  
    for (int i=2; i<=dynlen(tab); i++)
    {
      dynAppend(names, tab[i][2]);        
    }
  
    if( dynContains(names, answer) > 0 )
      VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"paraList_configAlreadyExists\"));
    else if(answer != \"\")
    {
      dpSet( dp + \".name\", answer);
    
      VIDEO_OA_paraList_updateSequenceList(\"txt_configs\");
    
      dyn_string items = txt_configs.items;
    
      int actPos;
      for (int i = 1; i <= dynlen(items); i++)
      {
        if ( items[i] == answer )
          actPos = i;
      }
    
      txt_configs.selectedPos = actPos;    
    
      VIDEO_OA_paraList_updateSequenceConfig(txt_configs.userData(txt_configs.selectedPos));
    } 
  }
  else
    VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"paraList_configEmptyName\"));  
