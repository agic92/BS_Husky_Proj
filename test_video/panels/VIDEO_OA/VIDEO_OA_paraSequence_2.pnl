"main()
{
  string dp = txt_configs.userData(txt_configs.selectedPos);
  dyn_string target;
  
  if (!VIDEO_OA_getUserPermission(\"perm_5\"))
  {
    VIDEO_OA_openNoPermission(\"perm_5\");
    return;
  }
  
  if (dp != \"\")
  {
    string camList = cb_config.userData(cb_config.selectedPos);
    
    if (camList != \"\")
    {
      string targetWidget = (rb_mode.number == 0) ? txt_ewo.text : cb_display.text;
      int mode;
      
      dpGet(dp + \".mode\", mode);
     
      if (targetWidget != \"\")
      {    
        bool matrixsequence = cb_matrix.state(0);
        
        if ((matrixsequence == 0) || ((matrixsequence == 1) && (txt_address.text != \"\")))
        {
          target[1] = (rb_mode.number == 0) ? \"ewo\" : \"display\";
          target[2] = targetWidget;
          target[3] = cb_dialog.text;
          target[4] = (cb_manNum.visible == 1) ? cb_manNum.text : \"\";
          target[5] = (matrixsequence == 1) ? \"matrixsequence\" : \"\";
          target[6] = (matrixsequence == 1) ? cb_camera.text : \"\";
          target[7] = (matrixsequence == 1) ? cb_switchingMatrix.text : \"\";
          target[8] = (matrixsequence == 1) ? txt_address.text : \"\";
  
          mode = (mode == -1) ?  0 : mode;         
        
          dpSet(  
                dp + \".paraListName\", camList,
                dp + \".time\", cb_time.text,
                dp + \".target\", target,
                dp + \".mode\", mode);
         }
         else
         VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"paraList_configEmptyTargetWidget\")); 
      }
      else
      VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"paraList_configEmptyTargetWidget\")); 
    }
    else
      VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"paraList_configEmptyCamList\")); 
  }
  else
    VIDEO_OA_openMessageInfo(getCatStr(\"voa_general\",\"paraList_configEmptyName\"));  
}

