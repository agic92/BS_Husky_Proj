VIDEO_OA_showLastDriverUpdate(string dp1, string infoConfig, string dp2, string infoLicense)
{
  string configUdateTime    = VIDEO_OA_filterKeywordValue(infoConfig, \"lastConfigUpdateTime\");
  string configUdateState   = VIDEO_OA_filterKeywordValue(infoConfig, \"lastConfigUpdateState\");
  string licenseUpdateTime  = VIDEO_OA_filterKeywordValue(infoLicense, \"lastLicenseUpdateTime\");
  string licenseUpdateState = VIDEO_OA_filterKeywordValue(infoLicense, \"lastLicenseUpdateState\");
  
  txt_infoConfig.backCol  = VIDEO_OA_stateColor(\"state=\" + configUdateState);
  txt_infoLicense.backCol = VIDEO_OA_stateColor(\"state=\" + licenseUpdateState);
  
  txt_infoConfig.text  = formatTime(\"%c\", (time) configUdateTime);
  txt_infoLicense.text = formatTime(\"%c\", (time) licenseUpdateTime);
}


