VIDEO_OA_showMainInfo (string dp1, string configValue,
                       string dp2, string stateValue)
{
  string operationMode, currentMaster, state;

  string hostname_1, hostname_2;
  dyn_string link_1, link_2;

  dpGet($SYSTEM + \"_VIDEO_SERVER_1.object.device.link\", link_1,
        $SYSTEM + \"_VIDEO_SERVER_2.object.device.link\", link_2);

  VIDEO_OA_formatControllerLink(link_1);
  VIDEO_OA_formatControllerLink(link_2);

  hostname_1 = VIDEO_OA_filterKeywordValue(link_1[1],\"hostname\");
  hostname_2 = VIDEO_OA_filterKeywordValue(link_2[1],\"hostname\");

  operationMode = VIDEO_OA_filterKeywordValue(configValue, \"operationMode\");
  currentMaster = VIDEO_OA_filterKeywordValue(configValue, \"currentMaster\");
  state = VIDEO_OA_filterKeywordValue(stateValue, \"state\");

  // get case sensitive name of host
  string currentMasterCS;

  if (strtolower(currentMaster) == strtolower(hostname_1))
  {
    setValue(\"Server_1.frame\",\"backCol\",\"Video_mode_active\");
    setValue(\"Server_2.frame\",\"backCol\",\"\");
    currentMasterCS = hostname_1;

  }
  else if (strtolower(currentMaster) == strtolower(hostname_2))
  {
    setValue(\"Server_1.frame\",\"backCol\",\"\");
    setValue(\"Server_2.frame\",\"backCol\",\"Video_mode_active\");
    currentMasterCS = hostname_2;
  }
  else
  {
    setValue(\"Server_1.frame\",\"backCol\",\"\");
    setValue(\"Server_2.frame\",\"backCol\",\"\");
    currentMasterCS = currentMaster;
  }


  if (operationMode == \"RC0\")
  {
    setValue( \"Server_1.frame\", \"foreCol\", \"Video_state_connection_ok\");
    setValue( \"Server_2.frame\", \"foreCol\", \"Video_state_connection_ok\");
  }
  else if (operationMode == \"RC1\")
  {
    setValue( \"Server_1.frame\", \"foreCol\", \"Video_state_connection_ok\");
    setValue( \"Server_2.frame\", \"foreCol\", \"Video_state_connection_offline\");
  }
  else if (operationMode == \"RC2\")
  {
    setValue( \"Server_1.frame\", \"foreCol\", \"Video_state_connection_offline\");
    setValue( \"Server_2.frame\", \"foreCol\", \"Video_state_connection_ok\");
  }
  else
  {
    setValue( \"Server_1.frame\", \"foreCol\", \"Video_state_connection_offline\");
    setValue( \"Server_2.frame\", \"foreCol\", \"Video_state_connection_offline\");
  }

  txt_operationMode.text = getCatStr(\"voa_general\", \"state_txt_value_\" + operationMode);
  txt_currentMaster.text = currentMasterCS;//currentMaster;
  txt_state.text         = getCatStr(\"voa_general\", \"state_txt_value_\" + state);

  dyn_string dsBatchFiles = getBatchFiles();
  setMultiValue(\"cmdStartServ\", \"visible\", _WIN32 && dynlen(dsBatchFiles) && access(dsBatchFiles[1], F_OK) == 0,
                \"cmdStopServ\",  \"visible\", _WIN32 && dynlen(dsBatchFiles) && access(dsBatchFiles[2], F_OK) == 0);
}



