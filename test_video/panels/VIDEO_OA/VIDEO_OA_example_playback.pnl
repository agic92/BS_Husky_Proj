"main()
{
  float fSliderTime;
  int beforeTime = beforeT.text;
  int afterTime = afterT.text;
  string messageText;
  int answer;
  
  dyn_string playbackCams = VIDEO_OA_ewoOnContentChangedGetPlaybackCams(\"ewo1\");
  
  dyn_bool streamSettings, streamSettings2;
  for(int i = 1; i <= dynlen(playbackCams); i++)
  {
    streamSettings[i] = 1; // protect all streams
    streamSettings2[i] = 0;
  }
  
  
  getValue(\"slider1.slider\", \"value\", fSliderTime);
  
  messageText = getCatStr(\"voa_general\", \"reallyProtecInterval\");
  strreplace (messageText, \"$1\", formatTime(\"%c\", (time)(fSliderTime - beforeTime)) );
  strreplace (messageText, \"$2\", formatTime(\"%c\", (time)(fSliderTime + afterTime)) );
  
  VIDEO_OA_openMessageInfoAnswer(messageText, 1, answer);
  if(answer == 1)
    VIDEO_OA_pbProtectCamList(playbackCams, fSliderTime - beforeTime, fSliderTime + afterTime, streamSettings, streamSettings, streamSettings, 1, 0, 0);  
}" 0
