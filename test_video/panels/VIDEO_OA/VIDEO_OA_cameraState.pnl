"main()
{ 
  dpConnect(\"VIDEO_OA_work_frontCamera\", $OBJECT + \".object.device.link\",
                                         $OBJECT + \".object.device.definition\",
                                         $OBJECT + \".object.device.currentStreamInfo\");
}

VIDEO_OA_work_frontCamera(string dp1, dyn_string asLink,
                          string dp2, dyn_string asDefinition,
                          string dp3, string sStreamInfo)
{
  VIDEO_OA_formatDeviceLink(asLink); 
  VIDEO_OA_formatDeviceDefinition(asDefinition); 
  string protocol;
  dyn_string asStreams = strsplit(sStreamInfo, \";\");
  
  tab1.deleteAllLines();
  tab1.appendLines(dynlen(asStreams),\"#1\",asStreams);
  
  mapping objectInfo =  VIDEO_OA_objExpObjectInfoToMapping(asLink[1]);

  VIDEO_OA_objExpFillKeyValueFields(VIDEO_OA_objExpGetKeyValueFields(), objectInfo);
  
  protocol = VIDEO_OA_objExpGetValueForKey(objectInfo,\"ctrlProtocol\");
  if (protocol != \"\")
    txt_field_ctrlProtocol.text = getCatStr(\"voa_general\", \"camType_\" + protocol);
  
  txt_definition.text = asDefinition[1];
}


