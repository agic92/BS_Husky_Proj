E "VIDEO_OA_objExpDisplayShowGrid(int gridCount, string gridlayout)
{
  dyn_bool coverVisible,displayVisible;
  
  displayVisible = VIDEO_OA_paraListGetVisibleDisplaysForGridname(gridlayout, 1);
      
    
  for(int i = 1;i <= 24; i++)
  {
    setValue(\"mon_display_\" + i, \"visible\", 0);
    setValue(\"back_\" + i, \"backCol\", (displayVisible[i] == 1) ? \"\" : \"{199,204,219,50}\");
  }
}

VIDEO_OA_objExpDisplayUpdateStreamNumbers()
{
  string currentStreamInfo;
  string info; 
  dyn_int streamsNumbers;
  
  if(dpExists(cbx_camera.text + \".object.device.currentStreamInfo\"))
    dpGet(cbx_camera.text + \".object.device.currentStreamInfo\", currentStreamInfo);
  
  for (int i = 1; i <= 3; i++)
  { 
    info = VIDEO_OA_filterKeywordValue(currentStreamInfo,\"stream_\" + i);
    if (info != \"\")
      dynAppend(streamsNumbers, i);
  }
  cbx_stream.items = streamsNumbers;
  cbx_stream.selectedPos(1);
}

string getBatchFile()
{
  string sStartDisplays = \"\\\\bin\\\\vimaccStartDisplays.bat\";
  
  string sBatchFile;
  dyn_string ds;
  
  if ( _WIN32 )
  {
    string sOut, sErr, sCmd = \"REG QUERY HKLM\\\\SOFTWARE\\\\Accellence\\\\vimacc\\\\General /v InstallationDirectory\";
    system(sCmd, sOut, sErr);
    ds = strsplit(sOut, \" \");
  
    for ( int i = dynlen(ds); i >= 1; i-- )
      if ( ds[i] == \"\" )
        dynRemove(ds, i);
  
    sOut = strltrim(strrtrim(ds[dynlen(ds)]));
  
    sBatchFile = sOut + sStartDisplays;
  }
  
  return sBatchFile;
}

