"main()
{
   dpConnect(\"VIDEO_OA_objExpDisplayShowState\", $OBJECT + \".object.device.state\",
                                                $OBJECT + \".object.device.controller\");
}

VIDEO_OA_objExpDisplayShowState(string dp, string sState, string dp2, dyn_string controller)
{
  dyn_string displayItems;
  int dialogcount = VIDEO_OA_filterKeywordValue(sState, \"dialogcount\");
  string gridlayout = VIDEO_OA_filterKeywordValue(sState, \"gridlayout\");
  string state    = VIDEO_OA_filterKeywordValue(sState, \"state\");
  dyn_string source = strsplit(VIDEO_OA_filterKeywordValue(sState, \"source\"), \",\");
  VIDEO_OA_formatControllerLink(controller);
  
  txt_dialogcount.text = dialogcount;
  txt_gridlayout.text = gridlayout;
  txt_dp.text          = dpSubStr(dp, DPSUB_SYS_DP);
  txt_comment.text     = dpGetDescription(dpSubStr(dp, DPSUB_SYS_DP) + \".\");
  txt_host.text        = controller[1];
  
  for(int i = 0; i <= dialogcount; i++)
    dynAppend(displayItems, i);
  
  cbx_display.items = displayItems;
  cbx_display.selectedPos = 1;
  
  VIDEO_OA_objExpDisplayShowGrid(dialogcount, gridlayout);
  VIDEO_OA_objExpDisplayShowSource(source,dialogcount, gridlayout);
  
}



//0  -> ID
//1  -> speed
//2  -> audiomute
//3  -> begin (NTP-Timestamp)
//4  -> end (NTP-Timestamp)
//5  -> pint (play breaks)
//6  -> autostop
//7  -> pos
//8  -> streaming status: streaming, pending, stopped, paused
//9  -> dialogInd
//10 -> mode: live, sequence, playback

// Only if mode == sequence
//11 -> sequenceId
//12 -> sequenceStep

 VIDEO_OA_objExpDisplayShowSource(dyn_string source, int dialogcount, string gridlayout)
{
  dyn_string actSource;
  
  for(int i = 1; i <= dialogcount; i++)
  {     
     setValue(\"mon_display_\" + VIDEO_OA_objExpDisplayGetIdForGrid(i, dialogcount, gridlayout) + \".gridNumber\", \"visible\", 1,
                                                                                                   \"text\", i);
  }
  
  for(int i = 1; i <= dynlen(source); i++)
  {
    actSource = strsplit( source[i] , \" \");
    if(dynlen(actSource) >= 11) // data set ok
    {
     
      int gridId = VIDEO_OA_objExpDisplayGetIdForGrid(actSource[10], dialogcount, gridlayout);
      dyn_int camId = VIDEO_OA_objExpDisplayGetCamIdFromSystemId(actSource[1]);
      setValue(\"mon_display_\" + gridId, \"visible\", 1); // source found -> show display
      setValue(\"mon_display_\" + gridId + \".displayBack\", \"backCol\", \"Video_displayMode_\" + actSource[11]);
      setValue(\"mon_display_\" + gridId + \".displayNumber\", \"text\", camId[1]);
      setValue(\"mon_display_\" + gridId + \".gridNumber\", \"text\", actSource[10]);
      
      //DebugN(camId,gridId);
    }
    else
      DebugTN(\"WARNING: Source data set for display server incorrect: \", source[i]);
  }
}

int VIDEO_OA_objExpDisplayGetIdForGrid(unsigned dialogID, unsigned dialogcount, string gridlayout)
{
  int gridId;
   
  gridId = VIDEO_OA_paraListGetGridId( dialogID,  gridlayout);
 
  return gridId;
}

dyn_int VIDEO_OA_objExpDisplayGetCamIdFromSystemId(string systemId)
{
  
  dyn_int returnValue;
  strreplace(systemId, \"Camera_\", \"\");
  dyn_string parts =strsplit(systemId, \"_\");
  if (dynlen(parts) == 1) // stream 1
    parts[2] = 1;
  
  returnValue[1] = (int) parts[1];
  returnValue[2] = (int) parts[2];

  return returnValue;
}


