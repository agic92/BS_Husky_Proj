"main()
{
  string sSystem = $SYSTEM;
  int myTableMode = $TABLEMODE; 
  
  dyn_string dps=dpNames(sSystem + \"Camera_*\", \"VIDEO_OA_CAMERA\");
  dyn_string dps_comment;
  dyn_string dps_name;
  dyn_string number;
  
  VIDEO_OA_alternatingRowColorsForTable();
  
  this.selectByClick(TABLE_SELECT_LINE);
  this.tableMode(myTableMode);  
  
  for(int i = 1; i <= dynlen(dps); i++)
  {   
    dps_name[i] = dpSubStr(dps[i], DPSUB_SYS_DP);
    dps_comment[i] = dpGetComment(dps[i]+\".\");
    dps[i] = dpSubStr(dps[i], DPSUB_DP);

    number[i] = VIDEO_OA_getFormatedObjectNumber(dps_name[i]);
  }
  this.deleteAllLines(); 
  this.appendLines(dynlen(dps), \"#1\", dps, \"#2\", dps_comment, \"#3\", dps_name, \"#4\", number);
  this.lineVisible(0);
}
