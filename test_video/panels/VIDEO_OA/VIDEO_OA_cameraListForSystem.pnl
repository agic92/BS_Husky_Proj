"DragStart" "main(int keys, int row, string column)
{
  dyn_int selectedLines = this.getSelectedLines;
  dyn_string cams; 
  dyn_anytype result;
  string information;
  bool possible;
  
  getValue(myModuleName()+ \".\" + $PANELNAME + \":cover_config\",\"visible\", possible); 

  cams[1] = \"cam_list\"; // idendifier for drop target  
  
  for (int i = 1; i <= dynlen(selectedLines); i++)
  {
    result = this.getLineN(selectedLines[i]);
    dynAppend(cams, result[4]);
  }
  
  information = cams; // dynString to string 
 
  if ((dynlen(cams) > 0) && !possible)
    dragStart(information, \"DragCamera.png\");
}"
