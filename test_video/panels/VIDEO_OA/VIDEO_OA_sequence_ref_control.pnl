"main()
{
  string dp = $SEQUENCE;
  dp = VIDEO_OA_paraList_getSequenceDpFromName(dp);
  dyn_string target;

  if (!VIDEO_OA_getUserPermission(\"perm_5\"))
  {
    VIDEO_OA_openNoPermission(\"perm_5\");
    return;
  }
  
  target[1] = (rb_mode.number == 0) ? \"ewo\" : \"display\";
  target[2] = (rb_mode.number == 0) ? txt_ewo.text : cb_display.text;
  target[3] = cb_dialog.text;
  target[4] = (cb_manNum.visible == 1) ? cb_manNum.text : \"\";
  
  dpSet(  dp + \".paraListName\", cb_config.userData(cb_config.selectedPos),
          dp + \".time\", cb_time.text);
        
  
  VIDEO_OA_seq_changeTargetPartDisplay(dp, target);
}

