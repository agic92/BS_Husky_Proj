"main()
{
  string dp = $SEQUENCE;
  
  dp = VIDEO_OA_paraList_getSequenceDpFromName(dp);
  
  dyn_string items;
  dyn_dyn_string list = VIDEO_OA_paraList_getAll(\"cam_list\");

  dyn_string displayDp = dpNames(dpSubStr(dp, DPSUB_SYS) + \"*\",\"VIDEO_OA_DISPLAY\");
  
  
  for(int i=1; i <= dynlen(displayDp); i++)
  {
    if (strpos(displayDp[i],\"_mp_VIDEO_OA_DISPLAY\") < 0)
      dynAppend(items,dpSubStr(displayDp[i],DPSUB_DP));
  }  

  cb_display.items = items;

  dynClear(items);
  
  for( int i = 1; i <= 255; i++)
  {
    items[i] = i;
  }
  cb_manNum.items = items;
  
  dynClear(items);
    
  for( int i = 1; i <= 60; i++)
  {
    items[i] = i;
  }
  cb_time.items = items;
  
  dynClear(items);
  
  for( int i = 1; i <= 24; i++)
  {
    items[i] = i;
  }
  cb_dialog.items = items;

  dynClear(items);
  
  for(int i = 1; i <= dynlen(list); i++)
    dynAppend(items, list[i][2]);
  
  cb_config.items = items;  
  
  for(int i = 1; i <= dynlen(list); i++)
    cb_config.userData(i, list[i][1]);
  
  
 dpConnect(\"work_1\", dp + \".name\",
                   dp + \".paraListName\",
                   dp + \".time\",
                   dp + \".mode\",
                   dp + \".target\");
 
}
    
work_1(string dp1, string name,
       string dp2, dyn_string paraListName,
       string dp3, int periodTime,
       string dp5, int mode,
       string dp6, dyn_string target)
{
  string configName;
  
  frame.text = name + \" (\"+dpSubStr(dp1,DPSUB_DP)+\")\";
  
  if (dynlen(target) != 4 )  // invalid parameter
   target = makeDynString( \"display\",\"\",\"0\",\"\");
  
  if(target[1] == \"ewo\")
  {
    rb_mode.number = 0;
    txt_ewo.text = target[2];
    txt_ewo.visible = 1;
    cb_display.visible = 0;
    cb_manNum.visible = 1;
    txt_manNum.visible = 1;
  }
  else
  {
    rb_mode.number = 1;
    cb_display.text = target[2];
    txt_ewo.visible = 0;
    cb_display.visible = 1;
    cb_manNum.visible = 0;
    txt_manNum.visible = 0;
  } 
  
  cb_time.text = (periodTime == 0) ? 1 : periodTime;
  cb_dialog.text = (target[3] == 0) ? 1 : target[3];
  cb_manNum.text = (target[4] == \"\") ? myManNum() : target[4];
  
  if(dpExists(paraListName))
  {
    dpGet(paraListName + \".name\", configName);
  }
  cb_config.text = configName;
   
  
