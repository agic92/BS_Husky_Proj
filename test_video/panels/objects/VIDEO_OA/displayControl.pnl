VIDEO_OA_displayControlshowGridSettings(string display)
{
  moduleName = \"displayModule\" + rand();
  this.ModuleName(moduleName);
  dyn_errClass err;
  
  dpConnect(\"VIDEO_OA_displayControlshowGridSettingsWork\", display + \".object.device.state\");
  err =getLastError();
  if(dynlen(err) > 0)
    border.backCol = \"_dpdoesnotexist\";
}

VIDEO_OA_displayControlshowGridSettingsWork(string dp, string state)
{
  string gridlayout = VIDEO_OA_filterKeywordValue(state, \"gridlayout\");
  strreplace(gridlayout, \"+\", \"p\");
   
  string path = getPath(PANELS_REL_PATH , \"VIDEO_OA/VIDEO_OA_displayControl_sub_grid_\" + gridlayout + \".pnl\");
  dp = dpSubStr(dp, DPSUB_SYS_DP);
   
 
  
  if(path != \"\")
    RootPanelOnModule(\"VIDEO_OA/VIDEO_OA_displayControl_sub_grid_\" + gridlayout + \".pnl\", gridlayout, moduleName, makeDynString(\"$OBJECT:\" + dp));
  else if(gridlayout != \"\")
    RootPanelOnModule(\"VIDEO_OA/VIDEO_OA_displayControl_sub_grid_\" + \"4x6\" + \".pnl\", \"default\", moduleName, makeDynString(\"$OBJECT:\" + dp));
  else 
    RootPanelOnModule(\"VIDEO_OA/VIDEO_OA_displayControl_sub_grid_\" + \"empty\" + \".pnl\", \"empty\", moduleName, makeDynString(\"$OBJECT:\" + dp));
  
}" 0
