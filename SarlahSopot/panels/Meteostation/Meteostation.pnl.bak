    PrecipitationType.text(latinToCyrillic(returnPrecipitationTypeText($SYSTEMNAME + $METEO_DP + \".measurements.precipitationType\")));
    PrecipitationIntensity.text(latinToCyrillic(returnMeasurementText($SYSTEMNAME + $METEO_DP + \".measurements.precipitationIntensity\", \"\") + \" mm/h\"));
    Visibility.text(latinToCyrillic(returnMeasurementText($SYSTEMNAME + $METEO_DP + \".measurements.visibility\", $SYSTEMNAME + $METEO_DP + \".settings.maxVisibility\") + \" m\"));
    WindSpeed.text(latinToCyrillic(returnMeasurementText($SYSTEMNAME + $METEO_DP + \".measurements.windSpeed\", \"\") + \" m/s\"));
    WaterFilmHeight.text(latinToCyrillic(returnMeasurementText($SYSTEMNAME + $METEO_DP + \".measurements.waterFilmHightOnPanel\", \"\") + \" mm\"));
    FreezingTemperatureNaCl.text(latinToCyrillic(returnMeasurementText($SYSTEMNAME + $METEO_DP + \".measurements.freezingTemperatureNaCl\", $SYSTEMNAME + $METEO_DP + \".settings.maxTemperature\") + \" °C\"));
