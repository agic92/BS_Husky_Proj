void showWeatherState(string datapointName, time lastRead)
{
    LastRead.text(latinToCyrillic(dpValToString(datapointName, lastRead, TRUE)));

    Temperature.text(latinToCyrillic(returnMeasurementText($SYSTEMNAME + $METEO_DP + \".measurements.temperature\", $SYSTEMNAME + $METEO_DP + \".settings.maxTemperature\") + \" °C\"));
    RoadTemperature.text(latinToCyrillic(returnMeasurementText($SYSTEMNAME + $METEO_DP + \".measurements.roadTemperature\", $SYSTEMNAME + $METEO_DP + \".settings.maxTemperature\") + \" °C\"));
    DewPoint.text(latinToCyrillic(returnMeasurementText($SYSTEMNAME + $METEO_DP + \".measurements.dewPoint\", $SYSTEMNAME + $METEO_DP + \".settings.maxTemperature\") + \" °C\"));
    RelativeHumidity.text(latinToCyrillic(returnMeasurementText($SYSTEMNAME + $METEO_DP + \".measurements.relativeHumidity\", $SYSTEMNAME + $METEO_DP + \".settings.maxHumidity\") + \" %\"));
    WindDirection.text(latinToCyrillic(returnMeasurementText($SYSTEMNAME + $METEO_DP + \".measurements.windDirection\", \"\") + \" °\"));
