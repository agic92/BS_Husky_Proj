}


string returnMeasurementText(string datapointNameOriginalValue, string datapointNameMaxValue)
{
    if (!isValueAllowed(datapointNameOriginalValue, datapointNameMaxValue))
    {
        return \"Nema podataka\";
    }

    anytype originalValue;
    getDP(datapointNameOriginalValue, originalValue);
    return fromFloatIntToString(originalValue);
}

bool isValueAllowed(string datapointNameOriginalValue, string datapointNameMaxValue)
{
    if (datapointNameMaxValue == \"\")
    {
        return true;
    }

    anytype maxValue, originalValue;
    getDP(datapointNameMaxValue, maxValue);
    getDP(datapointNameOriginalValue, originalValue);

    if (originalValue > maxValue)
    {
        return false;
    }

    return true;
}

string fromFloatIntToString(float floatIntValue)
{
    if (getTypeName(floatIntValue) != \"float\")
    {
        return (string)floatIntValue;
    }

    string stringValue;
    sprintf(stringValue, \"%2.2f\", floatIntValue);
    return stringValue;
}

string returnPrecipitationTypeText(string datapointName)
{
    string precipitationType;
    getDP(datapointName, precipitationType);
    mapping precipitationTypesMapped = getMapfromDatapoint($SYSTEMNAME + $METEO_DP + \".settings.mappedPreciptationTypes\");
    if (!mappingHasKey(precipitationTypesMapped, precipitationType))
    {
        return precipitationTypesMapped[\"other\"];
    }

    return precipitationTypesMapped[precipitationType];
}
