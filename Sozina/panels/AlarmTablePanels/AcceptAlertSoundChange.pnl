"main(mapping event)
{
  string fileName;
//   $ALERTTEXT
//   $NEWFILENAM
  dyn_string alarmSoundsMapping;
  dpGet(\"SysSozina:_mp_AlarmTableParameters.settings.alarmSoundFilesMapping\", alarmSoundsMapping);

  for (int i = 1; i <= dynlen(alarmSoundsMapping); i++)
  {
    dyn_string array = strsplit(alarmSoundsMapping[i], \"|\");
    if (array[1] == $ALERTTEXT)
    {
      dynRemove(alarmSoundsMapping, i);
    }
  }

  dynAppend(alarmSoundsMapping, $ALERTTEXT + \"|\" + $NEWFILENAME);
  dpSet(\"SysSozina:_mp_AlarmTableParameters.settings.alarmSoundFilesMapping\", alarmSoundsMapping);


  TxtQuestion.visible = false;
  TxtSuccessfulChange.visible = true;
  delay(1);
  PanelOff();
  PanelOffPanel(\"AlarmTablePanels/ChangeAlertSound.pnl\");
}
