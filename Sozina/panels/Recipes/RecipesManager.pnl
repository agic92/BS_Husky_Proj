main(mapping event)
{
    string messageSuccessful, err, messageText, insertTab, selectedText = \"\";
    dyn_string dynStringIsRecipeEditable, returnString, editableRecipes;
    dyn_float returnFloat;

    if (isSelectedElementRecipe(\"EditableRecipesList\"))
    {
         selectedText = getRecipeNameFromSelectedText(\"EditableRecipesList\");
    }

    messageText =  \"Da li ste sigurni da želite obrisati scenarij \\\"\" + selectedText + \"\\\" ?\";

    if (selectedText == \"\")
    {
        messageText = \"Niste odabrali scenarij koji ćete obrisati!\";
        err = \"-1\";
    }
    ChildPanelOnCentralReturn(\"objects/RecipeSigns/RecipeManagerInfoWarning.pnl\", \"Informacija\", makeDynString(\"$messageText:\" + messageText, \"$err:\" + err), returnFloat, returnString);
    if (returnFloat[1] == -1)
    {
        return;
    }
    string err = \"\";
    rcp_deleteRecipe(selectedText, err);
    getDP(systemName + \"recipesManager.settings.editableRecipes\", editableRecipes);
    dynRemove(editableRecipes, dynContains(editableRecipes, selectedText));
    setDP(systemName + \"recipesManager.settings.editableRecipes\", editableRecipes);
    showRecipesInLists();
    messageSuccessful = \"Scenarij je uspješno obrisan!\";
    ChildPanelOnCentralReturn(\"objects/RecipeSigns/ActionSuccessful.pnl\", \"\", makeDynString(\"$messageText:\" + messageSuccessful), returnFloat, returnString);
}
