E "string getRecipeNameFromSelectedText(string listName)
{
    string selectedText, insertTab;
    getValue(listName, \"selectedText\", selectedText);
    getDP(systemName + \"recipesManager.settings.insertTab\", insertTab);
    strreplace(selectedText, insertTab, \"\");
    return selectedText;
}

bool isSelectedElementRecipe(string listName)
{
    string insertTab, selectedText;
    dyn_string allRecipeNames;
    getDP(systemName + \"recipesManager.settings.insertTab\", insertTab);
    getValue(listName, \"selectedText\", selectedText);
    rcp_getAllRecipes(\"Sozina\", allRecipeNames);

    if (strreplace(selectedText, insertTab, \"\") > 0)
    {
        if (dynContains(allRecipeNames, selectedText) > 0)
        {
            return true;
        }
    }
    return false;
}

void showRecipesInLists()
{
    dyn_string allRecipesNames, allRecipeGroups, allListItems, editableRecipes, notSortedRecipes;
    string recipeName, insertTab, recipeGroup;

    EditableRecipesList.deleteAllItems();
    NotEditableRecipesList.deleteAllItems();

    rcp_getAllRecipes(\"Sozina\", allRecipesNames);

    getDP(systemName + \"recipesManager.settings.insertTab\", insertTab);
    getDP(systemName + \"recipesManager.settings.recipeGroups\", allRecipeGroups);
    getDP(systemName + \"recipesManager.settings.editableRecipes\", editableRecipes);

    for (int i = 1; i <= dynlen(allRecipesNames); i++)
    {
        getDP(systemName + allRecipesNames[i] + \".description\", recipeGroup);
        if (dynContains(allRecipeGroups, recipeGroup) < 1)
        {
            dynAppend(notSortedRecipes, allRecipesNames[i]);
        }
    }
    for (int j = 1; j <= dynlen(allRecipeGroups); j++)
    {
        EditableRecipesList.appendItem(allRecipeGroups[j]);
        NotEditableRecipesList.appendItem(allRecipeGroups[j]);
        for (int i = 1; i <= dynlen(allRecipesNames); i++)
        {
            recipeName = allRecipesNames[i];
            getDP(systemName + recipeName + \".description\", recipeGroup);
            if (recipeGroup != allRecipeGroups[j])
            {
                continue;
            }
            if (dynContains(editableRecipes, recipeName) > 0)
            {
                EditableRecipesList.appendItem(insertTab + recipeName);
                continue;
            }
            NotEditableRecipesList.appendItem(insertTab + recipeName);
        }
        allListItems = EditableRecipesList.items();
        if (allListItems[dynlen(allListItems)] == allRecipeGroups[j])
        {
            EditableRecipesList.appendItem(insertTab + \"/\");
        }
        allListItems = NotEditableRecipesList.items();
        if (allListItems[dynlen(allListItems)] == allRecipeGroups[j])
        {
            NotEditableRecipesList.appendItem(insertTab + \"/\");
        }
    }
    if (dynlen(notSortedRecipes) > 0)
    {
        NotEditableRecipesList.appendItem(\"Scenariji bez grupe\");
        for (int i = 1; i <= dynlen(notSortedRecipes); i++)
        {
            NotEditableRecipesList.appendItem(insertTab + notSortedRecipes[i]);
        }
    }
}

void showLastActivatedRecipe(string datapointName, string lastActivatedTime)
{
    string recipeName, recipeGroup, user, datapointNameGroup = systemName + recipeName + \".description\";
    getDP(systemName + \"Sozina.lastActivatedOfThisType\", recipeName);
    getDP(systemName + \"recipesManager.state.lastActivated.groupName\", recipeGroup);
    getDP(systemName + \"Sozina.lastActivatedUser\", user);
    LastActivatedRecipeName.text(recipeName);
    LastActivatedTime.text(lastActivatedTime);
    LastActivatedGroup.text(recipeGroup);
    LastActiveUser.text(user);
}
