            setValue(trafficLightStates[i][1] + \"_ref.redLight\", \"backCol\", mappedLightStateColors[colorState]);
            setValue(trafficLightStates[i][1] + \"_ref.yellowLight\", \"backCol\", mappedLightStateColors[colorState]);
            setValue(trafficLightStates[i][1] + \"_ref.greenLight\", \"backCol\", mappedLightStateColors[colorState]);
        }
    }
    this.enableColumnResize(false);
}
