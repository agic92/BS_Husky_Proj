"#uses \"basicMethods.ctl\"

main()
{
    this.gridColor (\"white\");
    dyn_string  splitStringFirst, signNames, splitDatapointName, recipeDatapoints, signDatapointNames = dpNames(\"**\", \"Sign\");
    string firstColumnTitle, secondColumnTitle, pictureName, refPanelName;
    dyn_anytype recipeValues;
    mapping mappedOrientation = getMapfromDatapoint(systemName + \"recipesManager.settings.mappedOrientation\");
    getValue(\"\", \"columnHeader\", 0, firstColumnTitle);
    getValue(\"\", \"columnHeader\", 1, secondColumnTitle);
    getDP(systemName + \"Sozina.elements\", recipeDatapoints);
    getDP(systemName + $recipeName + \".elementValues\", recipeValues);

    for (int i = 1; i <= dynlen(recipeDatapoints); i++)
    {
        splitStringFirst = strsplit(recipeDatapoints[i], \".\");
        dynAppend(signNames, splitStringFirst[1]);
    }

    for (int i = 1; i <= dynlen(signDatapointNames); i++)
    {
         splitDatapointName = strsplit(signDatapointNames[i], \":\");
         if (strpos(splitDatapointName[2],\"_mp_\", 0) == -1)
         {
             this.appendLine(firstColumnTitle, splitDatapointName[2], secondColumnTitle, \"\");
             if (recipeValues[dynContains(signNames, splitDatapointName[2])] != \"%ignore\")
             {
                 pictureName = recipeValues[dynContains(signNames, splitDatapointName[2])];
                 this.cellFillRC(this.lineCount()-1, secondColumnTitle, \"[pattern,[fit,png,RegularSigns/\" + pictureName + \".png]]\");
                 refPanelName = splitDatapointName[2] + \"_ref\";
                 setValue(refPanelName + \".Sign\", \"fill\", \"[pattern,[fit,png,RegularSigns/\" + pictureName + \"_\" + mappedOrientation[splitDatapointName[2]] + \".png]]\");
             }
             else
             {
                 this.cellValueRC(this.lineCount()-1, secondColumnTitle, \"/\");
             }
         }

    }
    this.enableColumnResize(false);
}
