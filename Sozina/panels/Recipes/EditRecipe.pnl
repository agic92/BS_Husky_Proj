            {
                color = stateName;
                break;
            }
        }
        if (recipeValues[i] == \"TRUE\")
        {
            dynAppend(trafficLightStates, makeDynString(splitStringFirst[1], color));
            dynAppend(notIgnoredLights, splitStringFirst[1]);
        }
    }

    for (int i = 1; i <= dynlen(trafficLightNames); i++)
    {
        if (dynContains(notIgnoredLights, trafficLightNames[i]) < 1)
