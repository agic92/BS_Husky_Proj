"#uses \"rcp.ctc\"
#uses \"basicMethods.ctl\"

main(mapping event)
{
    string messageSuccessful, title, messageText;
    dyn_float df = makeDynFloat(0);
    dyn_string ds;

    if (isRecipeNameEmpty(RecipeName.text()))
    {
        title = \"Upozorenje\";
        messageText = \"Niste unijeli ime scenarija!\";
        ChildPanelOnCentralModalReturn(\"objects/RecipeSigns/AddEditRecipeInfoWarning.pnl\",title,makeDynString(\"$messageText:\" + messageText, \"$Title:\" + title), df, ds);
    }
    else if (isRecipeNameTaken(RecipeName.text()) && (RecipeName.text() != $recipeName))
    {
        title = \"Upozorenje\";
        messageText = \"Scenarij sa željenim imenom već postoji. Izaberite drugi naziv.\";
        ChildPanelOnCentralModalReturn(\"objects/RecipeSigns/AddEditRecipeInfoWarning.pnl\",title,makeDynString(\"$messageText:\" + messageText, \"$Title:\" + title), df, ds);
    }
    else
    {
        title = \"\";
        messageText = \"Da li ste sigurni da želite izvršiti izmjenu scenarija?\";
        ChildPanelOnCentralModalReturn(\"objects/RecipeSigns/AddEditRecipeInfoWarning.pnl\",title,makeDynString(\"$messageText:\" + messageText, \"$Title:\" + title), df, ds);
    }



    if (df[1] == -1)
    {
        return;
    }

    string err, fillString, signName, pictureName, firstColumnTitle, secondColumnTitle;
    string backColor, trafficLightName, trafficBlinkerName, signLightName;
    dyn_string mappedSignLightNames, recipeDatapoints, splitStringFirst, splitStringSecond, signDatapointNames, splitDatapointNames, editableRecipes, blinkerDatapointNames, blinkerDpNamesNoSystemName;
    mapping mappedRecipeValues;
    dyn_int recipeDatapointsIndex;
    dyn_anytype recipeValues;
    int pictureName;
    dyn_string signDpNamesNoSystemName, dynStringIsRecipeEditable;

    if (RecipeName.text() != $recipeName)
    {
        rcp_deleteRecipe($recipeName, err);
        getDP(systemName + \"recipesManager.settings.editableRecipes\", editableRecipes);
        dynRemove(editableRecipes, dynContains(editableRecipes, $recipeName));
        dynAppend(editableRecipes, RecipeName.text());
        setDP(systemName + \"recipesManager.settings.editableRecipes\", editableRecipes);
        rcp_createRecipe(RecipeName.text(), \"Sozina\", err);
    }

    getDP(systemName + \"Sozina.elements\", recipeDatapoints);
    getDP(systemName + \"Sozina.elementIndex\", recipeDatapointsIndex);

    makeMappedRecipeValues(\"SignsTable\", mappedRecipeValues);
    makeMappedRecipeValues(\"TrafficLightsTable\", mappedRecipeValues);
    makeMappedRecipeValues(\"TrafficBlinkersTable\", mappedRecipeValues);

    signDatapointNames = dpNames(\"**\", \"Sign\");
    blinkerDatapointNames = dpNames(\"**\", \"TrafficBlinker\");
    for (int i = 1; i <= dynlen(signDatapointNames); i++)
    {
        splitDatapointNames = strsplit(signDatapointNames[i], \":\");
        dynAppend(signDpNamesNoSystemName, splitDatapointNames[2]);
    }

    for (int i = 1; i <= dynlen(blinkerDatapointNames); i++)
    {
        splitDatapointNames = strsplit(blinkerDatapointNames[i], \":\");
        dynAppend(blinkerDpNamesNoSystemName, splitDatapointNames[2]);
    }

    mappedSignLightNames = mappingKeys(mappedRecipeValues);
    for (int i = 1; i <= dynlen(recipeDatapoints); i++)
    {
        splitStringFirst = strsplit(recipeDatapoints[i], \".\");
        signLightName = splitStringFirst[1];

        if (dynContains(mappedSignLightNames, signLightName) < 1)
        {
            dynAppend(recipeValues, \"%ignore\");
            continue;
        }

        if (dynContains(signDpNamesNoSystemName, signLightName) > 0)
        {
            pictureName = mappedRecipeValues[signLightName];
            dynAppend(recipeValues, pictureName);
        }

        else if (strpos(recipeDatapoints[i], mappedRecipeValues[signLightName], 0) > -1)
        {
            dynAppend(recipeValues, TRUE);
        }
        else if (dynContains(blinkerDpNamesNoSystemName, signLightName, 0) > 0)
        {
            dynAppend(recipeValues, FALSE);
        }
        else
        {
            dynAppend(recipeValues, \"%ignore\");
        }

    }

    rcp_changeRecipe(RecipeName.text(), RecipeGroup.text(), recipeValues, err);
    messageSuccessful = \"Izmjene su uspješno spašene!\";
    ChildPanelOnCentralReturn(\"objects/RecipeSigns/ActionSuccessful.pnl\", \"\", makeDynString(\"$messageText:\" + messageSuccessful), df, ds);
    PanelOffReturn(makeDynFloat(), makeDynString());
}

bool isRecipeNameEmpty (string newRecipeName)
{
    if (newRecipeName == \"\")
    {
        return true;
    }
    return false;
}

bool isRecipeNameTaken(string newRecipeName)
{
    dyn_string recipeNames;
    rcp_getAllRecipes(\"Sozina\", recipeNames);
    if (dynContains(recipeNames, newRecipeName) > 0)
    {
         return true;
    }
    return false;
}

void makeMappedRecipeValues (string tableName, mapping &mappedRecipeValues)
{
    int numberOfRows;
    string firstColumnTitle, secondColumnTitle, pictureName, fillString, elementName, cellValue;
    dyn_string splitStringFirst, splitStringSecond;
    getValue(tableName, \"columnHeader\", 0, firstColumnTitle);
    getValue(tableName, \"columnHeader\", 1, secondColumnTitle);
    getValue(tableName, \"lineCount\", numberOfRows);
    for (int i = 0; i < numberOfRows; i++)
    {
        getValue(tableName, \"cellFillRC\", i, secondColumnTitle, fillString);
        getValue(tableName, \"cellValueRC\", i, firstColumnTitle, elementName);
        getValue(tableName, \"cellValueRC\", i, secondColumnTitle, cellValue);

        if (cellValue != \"/\")
        {
            splitStringFirst = strsplit(fillString, \"/\");
            splitStringSecond = strsplit(splitStringFirst[2], \".\");
            pictureName = splitStringSecond[1];
            mappedRecipeValues[elementName] = pictureName;

        }

    }

}
