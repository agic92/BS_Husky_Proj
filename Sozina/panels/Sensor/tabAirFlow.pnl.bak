"main(mapping event)
{
  dyn_float df;
  dyn_string ds;
  int changedField = 0;
  int minLevel = 0;
  int maxLevel = 300;

  for (int i=0; i<tableAirFlow.lineCount(); i++)
  {
      //DebugTN(tableAirFlow.cellValueRC(i, \"Dp\")); // CO1
      int alarmLevel1, alarmLevel2, alarmLevel3, alarmLevel4;

      dpGet(systemName + tableAirFlow.cellValueRC(i, \"Dp\") + \".settings.alarmLevel1\", alarmLevel1);
      dpGet(systemName + tableAirFlow.cellValueRC(i, \"Dp\") + \".settings.alarmLevel2\", alarmLevel2);
      dpGet(systemName + tableAirFlow.cellValueRC(i, \"Dp\") + \".settings.alarmLevel3\", alarmLevel3);
      dpGet(systemName + tableAirFlow.cellValueRC(i, \"Dp\") + \".settings.alarmLevel4\", alarmLevel4);

      if (alarmLevel1 != tableAirFlow.cellValueRC(i, \"AlarmLevel1\")){
         changedField++;
         tableAirFlow.cellBackColRC(i, \"AlarmLevel1\", \"yellow\");
      }
      if (alarmLevel2 != tableAirFlow.cellValueRC(i, \"AlarmLevel2\")){
         changedField++;
         tableAirFlow.cellBackColRC(i, \"AlarmLevel2\", \"yellow\");
      }
      if (alarmLevel3 != tableAirFlow.cellValueRC(i, \"AlarmLevel3\")){
         changedField++;
         tableAirFlow.cellBackColRC(i, \"AlarmLevel3\", \"yellow\");
      }
      if (alarmLevel4 != tableAirFlow.cellValueRC(i, \"AlarmLevel4\")){
         changedField++;
         tableAirFlow.cellBackColRC(i, \"AlarmLevel4\", \"yellow\");
      }

  }//end for
  if(changedField == 0)
  {
       ChildPanelOnReturn(\"Sensor/MessageInfo1\", \"Upozorenje\", makeDynString(\"$1:Nema promjena za alarmne nivoe!\"), 300, 200, df, ds);
       return;
  }

  ChildPanelOnReturn(\"Sensor/MessageInfo\", \"Upozorenje\", makeDynString(\"$1:Da li ste sigurni da želite promijeniti alarmne nivoe!\", \"$2:DA\", \"$3:NE\"), 300, 200, df, ds);

  if(df[1]==1)
  {
    for (int i=0; i<tableAirFlow.lineCount(); i++)
    {
      int alarmLevel1, alarmLevel2, alarmLevel3, alarmLevel4;

      dpGet(systemName + tableAirFlow.cellValueRC(i, \"Dp\") + \".settings.alarmLevel1\", alarmLevel1);
      dpGet(systemName + tableAirFlow.cellValueRC(i, \"Dp\") + \".settings.alarmLevel2\", alarmLevel2);
      dpGet(systemName + tableAirFlow.cellValueRC(i, \"Dp\") + \".settings.alarmLevel3\", alarmLevel3);
      dpGet(systemName + tableAirFlow.cellValueRC(i, \"Dp\") + \".settings.alarmLevel4\", alarmLevel4);

      if (alarmLevel1 != tableAirFlow.cellValueRC(i, \"AlarmLevel1\"))
      {
        if((tableAirFlow.cellValueRC(i, \"AlarmLevel1\")) > minLevel && (tableAirFlow.cellValueRC(i, \"AlarmLevel1\")) < maxLevel && (tableAirFlow.cellValueRC(i, \"AlarmLevel1\") < tableAirFlow.cellValueRC(i, \"AlarmLevel2\"))){
           dpSet(systemName + tableAirFlow.cellValueRC(i, \"Dp\") + \".settings.alarmLevel1\", tableAirFlow.cellValueRC(i, \"AlarmLevel1\"));
           tableAirFlow.cellBackColRC(i, \"AlarmLevel1\", \"green\");
        }
        else{
           tableAirFlow.cellBackColRC(i, \"AlarmLevel1\", \"red\");
           tableAirFlow.cellValueRC(i, \"AlarmLevel1\", alarmLevel1);
        }
      }
      if (alarmLevel2 != tableAirFlow.cellValueRC(i, \"AlarmLevel2\"))
      {
         if((tableAirFlow.cellValueRC(i, \"AlarmLevel2\")) > minLevel && (tableAirFlow.cellValueRC(i, \"AlarmLevel2\")) < maxLevel &&
            (tableAirFlow.cellValueRC(i, \"AlarmLevel2\") < tableAirFlow.cellValueRC(i, \"AlarmLevel3\")) && (tableAirFlow.cellValueRC(i, \"AlarmLevel2\") > tableAirFlow.cellValueRC(i, \"AlarmLevel1\"))){
           dpSet(systemName + tableAirFlow.cellValueRC(i, \"Dp\") + \".settings.alarmLevel2\", tableAirFlow.cellValueRC(i, \"AlarmLevel2\"));
           tableAirFlow.cellBackColRC(i, \"AlarmLevel2\", \"green\");
         }
         else {
           tableAirFlow.cellBackColRC(i, \"AlarmLevel2\", \"red\");
           tableAirFlow.cellValueRC(i, \"AlarmLevel2\", alarmLevel2);
         }
      }
      if (alarmLevel3 != tableAirFlow.cellValueRC(i, \"AlarmLevel3\"))
      {
         if((tableAirFlow.cellValueRC(i, \"AlarmLevel3\")) > minLevel && (tableAirFlow.cellValueRC(i, \"AlarmLevel3\")) < maxLevel &&
            (tableAirFlow.cellValueRC(i, \"AlarmLevel3\") < tableAirFlow.cellValueRC(i, \"AlarmLevel4\")) && (tableAirFlow.cellValueRC(i, \"AlarmLevel3\") > tableAirFlow.cellValueRC(i, \"AlarmLevel2\"))){
            dpSet(systemName + tableAirFlow.cellValueRC(i, \"Dp\") + \".settings.alarmLevel3\", tableAirFlow.cellValueRC(i, \"AlarmLevel3\"));
            tableAirFlow.cellBackColRC(i, \"AlarmLevel3\", \"green\");
         }
         else{
            tableAirFlow.cellBackColRC(i, \"AlarmLevel3\", \"red\");
            tableAirFlow.cellValueRC(i, \"AlarmLevel3\", alarmLevel3);
         }
      }
      if (alarmLevel4 != tableAirFlow.cellValueRC(i, \"AlarmLevel4\"))
      {
         if((tableAirFlow.cellValueRC(i, \"AlarmLevel4\")) > minLevel && (tableAirFlow.cellValueRC(i, \"AlarmLevel4\")) < maxLevel &&
            (tableAirFlow.cellValueRC(i, \"AlarmLevel4\") > tableAirFlow.cellValueRC(i, \"AlarmLevel3\"))){
            dpSet(systemName + tableAirFlow.cellValueRC(i, \"Dp\") + \".settings.alarmLevel4\", tableAirFlow.cellValueRC(i, \"AlarmLevel4\"));
            tableAirFlow.cellBackColRC(i, \"AlarmLevel4\", \"green\");
         }
         else{
           tableAirFlow.cellBackColRC(i, \"AlarmLevel4\", \"red\");
           tableAirFlow.cellValueRC(i, \"AlarmLevel4\", alarmLevel4);
         }
      }

    }//end for

  }//end if
}" 0
