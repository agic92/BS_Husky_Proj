"main(int row, string column, string value)
{
    string fillType, pictureName;
    dyn_string splitStringFirst, splitStringSecond;
    getValue(\"\", \"cellFillRC\", row, column, fillType);
    if (fillType != \"\")
    {
        splitStringFirst = strsplit(fillType, \"/\");
        splitStringSecond = strsplit(splitStringFirst[2], \".png\");
        pictureName = splitStringSecond[1];
    }
    else
    {
        pictureName = \"\";
    }
    PanelOffReturn(makeDynFloat(),makeDynString($SIGN_NAME, pictureName));
}
