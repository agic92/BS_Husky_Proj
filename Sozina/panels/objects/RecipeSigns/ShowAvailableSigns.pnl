"main()
{
    this.gridColor (\"white\");
    int signType, signArrayIndex = 1;
    dyn_int typeConfiguration;
    dyn_string columnTitle;
    getDP(systemName + $SIGN_NAME + \".settings.signType\", signType);
    getDP(systemName + \"Type\" + signType + \".settings.typeConfiguration\", typeConfiguration);
    int availableSignsNumber = dynlen(typeConfiguration);
    int tableRowsNumber = (availableSignsNumber/this.columnCount());
    if (tableRowsNumber*this.columnCount() < availableSignsNumber)
    {
        tableRowsNumber++;
    }
    for (int j = 0; j < this.columnCount(); j++)
    {
        getValue(\"\", \"columnHeader\", j, columnTitle[j+1]);
    }
    this.appendLines(tableRowsNumber);
    for (int i = 0; i < tableRowsNumber; i++)
    {
        for (int j = 0; j < this.columnCount(); j++)
        {
            if (signArrayIndex > availableSignsNumber)
            {
                this.cellValueRC(i, columnTitle[j+1], \"/\");
            }
            else
            {
                this.cellFillRC(i, columnTitle[j+1], \"[pattern,[fit,png,RegularSigns/\" + typeConfiguration[signArrayIndex] + \".png]]\");
                signArrayIndex++;
            }

        }
    }
    if (this.lineCount() <= 2)
    {
        this.size(this.rowHeight()*this.columnCount()+2, this.rowHeight()*this.lineCount()+2);
    }
    else
    {
        this.size(this.rowHeight()*this.columnCount()+20, this.rowHeight()*2+2);
    }
}

