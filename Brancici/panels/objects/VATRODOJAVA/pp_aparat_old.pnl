void EP_setBackColor()
{
  int rc;

  if( !dpExists( sys_name + $MAN_DP + \".Status.Fire:_online.._value\"))
  {
    setValue(\"\", \"color\", \"_dpdoesnotexist\");
    return;
  }

  dpConnect(\"EP_setBackColorCB\", 
                 sys_name + $MAN_DP + \".Status.Fire:_online.._value\");
  dpConnect(\"EP_setBackColorFAULT\", 
                 sys_name + $MAN_DP+\".Status.Fault:_online.._value\");
  
  if ( sdGetLastError() < 0)
    setValue(\"\", \"color\", \"_dpdoesnotexist\");
}


void EP_setBackColorCB(string dpSource, bool fire)
{
  delay(1);
  while(!fire){    
    
    if (!fire){
      setValue(\"\", \"backCol\", \"stoerKamQuit\");
      delay(1);
      setValue(\"\", \"backCol\", \"\");
      delay(3);
    }
    dpGet(sys_name + $MAN_DP + \".Status.Fire:_online.._value\", fire);
   
  }
  setValue(\"\", \"backCol\", \"\");
}


void EP_setBackColorFAULT(string dpSource, bool fault)
{
  delay(4);
  while(!fault){    
    
    if (!fault){
      setValue(\"\", \"backCol\", \"warnKamQuit\");
      delay(1);
      setValue(\"\", \"backCol\", \"\");
      delay(3);
    }
    dpGet(sys_name + $MAN_DP + \".Status.Fault:_online.._value\", fault);
   
  }
  setValue(\"\", \"backCol\", \"\"); 
}


void EP_play_sound()
{
  int rc;

  if( !dpExists(sys_name + $MAN_DP+\".Status.Fire:_online.._value\"))
  {
    setValue(\"\", \"color\", \"_dpdoesnotexist\");
    return;
  }

  rc = dpConnect(\"EP_play_soundCB\",
                 sys_name + $MAN_DP+\".Status.Fire:_online.._value\");

  if ( sdGetLastError() < 0 || rc != 0)
    setValue(\"\", \"color\", \"_dpdoesnotexist\");
}

void EP_play_soundCB(string dp, bool boNewValue)
{
  if (!boNewValue){
    startSound(\"C:/WinCC_OA_Proj/Remote_Tunel/data/sounds/police.wav\", TRUE);
    delay(5);
    stopSound();
}
}
