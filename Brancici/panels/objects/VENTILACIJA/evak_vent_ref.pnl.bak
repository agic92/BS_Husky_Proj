  else if (!otvorena && zatvorena) {
    k1.visible = true;
    k2.visible = false;
    k3.visible = false;
  }
  if (!otvorena && !zatvorena) {
    k1.visible = false;
    k2.visible = true;
    k3.visible = false;
  }
}


