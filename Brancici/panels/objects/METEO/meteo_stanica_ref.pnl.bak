EP_setColor()
{
  dyn_errClass err;
  
  dpConnect(\"EP_setColorCB\", 
            sys_name + $METEO_DP + \".response.alarm.Status_Meteoroloska_Stanica_Prisutna\",
            sys_name + $METEO_DP + \".response.alarm.Status_Prisutan_Senzor_Ceste\",
            sys_name + $METEO_DP + \".response.alarm.Status_Senzor_Vidljivosti_Prisutan\",
            sys_name + $METEO_DP + \".response.alarm.Poledica\",
            sys_name + $METEO_DP + \".response.alarm.Magla\",
            sys_name + $METEO_DP + \".response.alarm.Sklizak_Kolovoz\",
            sys_name + $METEO_DP + \".response.alarm.Vjetar\",
            sys_name + $METEO_DP + \".response.alarm.Snijeg\",
            sys_name + $METEO_DP + \".response.alarm.Snijeg:_online.._bad\");
                 
  err = getLastError();
  if (dynlen(err) > 0)
    setValue(\"\", \"color\", \"_dpdoesnotexist\");

}
              
EP_setColorCB(string dp2, bool meteo_stanica_prisutna, string dp3, bool senzor_ceste,
              string dp4, bool senzor_vidljivosti, string dp5, bool poledica,
              string dp6, int magla, string dp7, int sklizak_kolovoz,
              string dp8, int vjetar, string dp9, int snijeg, 
              string dp10, bool bInvalid) {
  if(bInvalid)
    setValue(\"\", \"color\", \"_dpdoesnotexist\");
  else
    setValue(\"\", \"color\", \"\");
  

  if(!meteo_stanica_prisutna || !senzor_ceste || !senzor_vidljivosti) {
    setValue(\"\", \"color\", \"disturbanceWentUna\");
  }
  else if (poledica==1 || (magla<=60 && magla!=0) || vjetar>=22 || snijeg==1  || sklizak_kolovoz>=3000) {
    setValue(\"\", \"color\", \"stoerKamGingUnq\");
  }
  else if ((magla>60 && magla <=100) || (vjetar >= 18 && vjetar < 22)  || (sklizak_kolovoz >= 1500 && sklizak_kolovoz < 3000)) {
    setValue(\"\", \"color\", \"SiemensNaturalYellowLight\");
  }
  else {
    setValue(\"\", \"color\", \"\");
  }

}
