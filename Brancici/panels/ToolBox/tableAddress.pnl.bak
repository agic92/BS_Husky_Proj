      string address;
      bool isActive;

      dpGet(dps[i] + \":_address.._reference\", address,
            dps[i] + \":_address.._active\", isActive);

      this.appendLine(\"Dp\", dps[i] + \":_address.._reference\", \"Value\", address, \"IsActive\", isActive);

    }


  }
}
