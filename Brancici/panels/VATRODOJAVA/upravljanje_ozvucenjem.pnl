    value = list[i][2];
    dpe = list[i][1];
    dpe = dpSubStr(dpe, DPSUB_DP);  //DPSUB_DP - 
    DebugTN(\"Govor: \" + govor_state + \", value: \" + value[1]);
    if (value[1] == 10010 && govor_state) {
      aktivan_govor = true;
      break;
    }
    else if (value[1] == 10010 && !govor_state) {
      aktivna_poruka=true;
      break;
    }
  } 
  
  if (aktivan_govor) {
     status_poruke.visible = false;
     btn_start.visible = false;
     btn_end.visible = true;
  }
  else if (aktivna_poruka) {
     status_poruke.visible = false;
     btn_start.visible = false;
     btn_end.visible = false;
  }
  
  else {
    status_poruke.visible = false;
    btn_start.visible = true;
    btn_end.visible = false;
  }
}

