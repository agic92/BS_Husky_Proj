"main()
{
  string zona = zone2.selectedText;
  dyn_string poruke;
  for (int i=1; i<= odabrane_poruke2.itemCount; i++) {
    poruke += odabrane_poruke2.items[i];
    
    if (i != odabrane_poruke2.itemCount) {
      poruke += \", \";
    }
  }
  
  string live_speech = govor2.selectedText;
  
  string repeats = ponavljanja2.selectedText;
  
  posaljiKomanduNaOPC(zona, poruke, live_speech, repeats);
}

posaljiKomanduNaOPC(string zona, string poruke, string govor2, string ponavljanja) {
  string pravi_naziv_zone = mapirajNazivZone(zona);
  string speech = (govor2==\"Da\") ? \"1\" : \"0\";
  string call = \"Call_0\";
  
  string s = \"<nsPV:Command Name=\\\"Start\\\" Description=\\\"Starts a call\\\" Adresse=\\\"\" + call + \"\\\" xmlns:nsPV=\\\"file:///S3K/Proxyverwalter\\\" OPCServerKlasse=\\\"BoschPraesideoProxy30\\\" \" + \"Anzeigename=\\\"Start\\\">\" + 
               \"<nsPV:Parameters>\" + 
                 \"<nsPV:Parameter Name=\\\"Routing\\\" ValueType=\\\"string\\\" Description=\\\"Routing\\\" Anzeigename=\\\"Routing\\\">\" + pravi_naziv_zone + \"</nsPV:Parameter>\" + 
                 \"<nsPV:Parameter Name=\\\"Priority\\\" ValueType=\\\"long\\\" Description=\\\"Priority\\\" Anzeigename=\\\"Priority\\\">\" + 220 + \"</nsPV:Parameter>\" + 
                 \"<nsPV:Parameter Name=\\\"Partial\\\" ValueType=\\\"bool\\\" Description=\\\"Partial:1, not Partial:0\\\" Anzeigename=\\\"Partial\\\"></nsPV:Parameter>\" + 
                 \"<nsPV:Parameter Name=\\\"StartChime\\\" ValueType=\\\"string\\\" Description=\\\"Start chime\\\" Anzeigename=\\\"StartChime\\\">1-tone chime</nsPV:Parameter>\" + 
                 \"<nsPV:Parameter Name=\\\"EndChime\\\" ValueType=\\\"string\\\" Description=\\\"End chime\\\" Anzeigename=\\\"EndChime\\\">1-tone chime</nsPV:Parameter>\" + 
                 \"<nsPV:Parameter Name=\\\"LiveSpeech\\\" ValueType=\\\"bool\\\" Description=\\\"LiveSpeech:1, not LiveSpeech:0\\\" Anzeigename=\\\"LiveSpeech\\\">\" + speech +\"</nsPV:Parameter>\" + 
                 \"<nsPV:Parameter Name=\\\"AudioInput\\\" ValueType=\\\"string\\\" Description=\\\"Audio input source of Praesideo system\\\" Anzeigename=\\\"AudioInput\\\">PozSt 1_AI1</nsPV:Parameter>\" + 
                 \"<nsPV:Parameter Name=\\\"Messages\\\" ValueType=\\\"string\\\" Description=\\\"Messages\\\" Anzeigename=\\\"Messages\\\">\" + poruke + \"</nsPV:Parameter>\" + 
                 \"<nsPV:Parameter Name=\\\"Repeat\\\" ValueType=\\\"long\\\" Description=\\\"Repeat\\\" Anzeigename=\\\"Repeat\\\">\" + ponavljanja + \"</nsPV:Parameter>\" + 
               \"</nsPV:Parameters>\" + 
             \"</nsPV:Command>\";
  
  
  dpSet(\"SysSarani:ozvucenje_desna_cijev.cmd\", s);
  
  dpSet(\"SysSarani:ozvucenje_desna_cijev.odabrana_zona\", pravi_naziv_zone);
 
  bool status_govor = (govor2==\"Da\") ? 1 : 0;
  
  dpSet(\"SysSarani:ozvucenje_desna_cijev.govor\", status_govor);
}

string mapirajNazivZone(string zona) {
  mapping m;
  
  m[\"Sve zone\"] = \"Sarani 1-Sve\";
  m[\"Prva trećina\"] = \"Sarani 1-1\";
  m[\"Druga trećina\"] = \"Sarani 1-2\";
  m[\"Treća trećina\"] = \"Sarani 1-3\";
  m[\"Kolski prolaz\"] = \"Sarani 1-Kolski\";
  m[\"ka Beogradu\"] = \"Sarani 1-Beograd\";
  m[\"ka Čačku\"] = \"Sarani 1-Cacak\";
  
  for (int i=1; i<=mappinglen(m); i++) {
    if (mappingGetKey(m, i) == zona) {
      return mappingGetValue(m, i);
    }
  }
  return \"\";
}
