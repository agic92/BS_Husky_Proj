work (string dp, int state, string dp2, int stanje_sistema) {
  string stanje;
  
  
  if (stanje_sistema == 10126) {
    stanje = \"Redundantni prsten prekinut\";
  }
  else {
    switch (state) {
      case 10100:
        stanje = \"OK\";
        break;
      case 10101:
        stanje = \"Jedinica diskonektovana\";
        break;
      case 10126:
        stanje = \"Redundantni prsten prekinut\";
        break;
      case 10113:
        stanje = \"Jedinica nije priključena\";
        break;
      case 10124:
        stanje = \"Greška sa memorijom\";
        break;
      case 10148:
        stanje = \"Stanica isključena od stane korisnika\";
        break;
      case 10149:
        stanje = \"StandBy\";
        break;
      case 10150:
        stanje = \"Aktivna\";
        break;
      default:
        stanje = \"Greška\";
        break;
    }
  }
  this.text = latinToCyrillic(\"Status pozivne stanice: \" + stanje);
}" 0
