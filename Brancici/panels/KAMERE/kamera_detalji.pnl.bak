"#uses \"latin_to_cyrillic.ctl\"
string sys_name = \"SysSarani:\";
main()
{
  bool cameraActive;
  dpGet(sys_name + $CAM_DP + \".active:_original.._value\", cameraActive);
  string txt;
  
  if (cameraActive) {
    dpSet(sys_name + $CAM_DP + \".active:_original.._value\", 0);
    dpSet(sys_name + $CAM_DP + \".Pedestrian\",0);
    dpSet(sys_name + $CAM_DP + \".Smoke\",0);
    dpSet(sys_name + $CAM_DP + \".Ghost_driver\",0);
    dpSet(sys_name + $CAM_DP + \".Stopped_vehicle\",0);
    dpSet(sys_name + $CAM_DP + \".Traffic_jam\",0);
    dpSet(sys_name + $CAM_DP + \".Lost_cargo\",0);
    dpSet(sys_name + $CAM_DP + \".False_driver\",0); 
    dpSet(sys_name + $CAM_DP + \".Movement_in_non-traffic_area_0\",0); 
    
    txt = latinToCyrillic(\"Aktiviraj kameru\");
    this.text = txt;
    this.backCol = \"STD_device_intermediate\";
  }
  else {
    dpSet(sys_name + $CAM_DP + \".active:_original.._value\", 1);
    txt = latinToCyrillic(\"Deaktiviraj kameru\");  
    this.text = txt;
    this.backCol = \"WF_Error\";
  }
}
