"main()
{
  string s_pmonUser, s_pmonPassword;
  string s_logDir, s_separator;
  dyn_string ds_separator;
  
  int i_interval;
  bool b_active;
  bool b_idleCPU;
  bool b_msgInfo;
  
  dyn_int di_interval;
  
  if(!dpExists(\"NFR_PMonInfo_CPU\"))
  {
    string s_error;
    
    // show error dialog
    s_error = getCatStr(\"_errors\",\"00007\");
    ChildPanelOnCentralModal(\"vision/MessageWarning\",\"Information\",makeDynString(\"NFR_messageSimulation \" + s_error));

    // set all objects inactive except the \"Cancel\" button
    setMultiValue(\"pmonUser\",\"enabled\",false,
                  \"pmonPassword\",\"enabled\",false,
                  \"active\",\"enabled\",false,
                  \"reportInterval\",\"enabled\",false,
                  \"logDir\",\"enabled\",false,
                  \"idleCPU\",\"enabled\",false,
                  \"separator\",\"enabled\",false,
                  \"msgInfo\",\"enabled\",false,
                  \"pathBrowser\",\"enabled\",false,
                  \"register\",\"enabled\",false);
    
    setMultiValue(\"ok\",\"enabled\",false,
                  \"apply\",\"enabled\",false);
  }
  else
  {
    dpGet(\"NFR_PMonInfo_CPU.pmonUser\",s_pmonUser,
          \"NFR_PMonInfo_CPU.pmonPassword\",s_pmonPassword,
          \"NFR_PMonInfo_CPU.active\",b_active,  
          \"NFR_PMonInfo_CPU.CPU_checkInterval\",i_interval,
          \"NFR_PMonInfo_CPU.logDirectory\",s_logDir,
          \"NFR_PMonInfo_CPU.getCPUIdle\",b_idleCPU,
          \"NFR_PMonInfo_CPU.separator\",s_separator,
          \"NFR_PMonInfo_CPU.recordMsgInfo\",b_msgInfo);
    
    // define default values if nothing was defined
    if(i_interval == 0)
      i_interval = 60;
    
    if(s_separator == \"\")
      s_separator = \"TAB\";
  
    di_interval = makeDynInt(10,20,30,60,120,300,600,1200);
    ds_separator = makeDynString(\",\",\";\",\"TAB\");

    // check if the current log-directory fits the operating system
    if(_UNIX && strpos(s_logDir,\":\",0) > 0)
      s_logDir = PROJ_PATH + \"data\";
    else if(_WIN32 && strpos(s_logDir,\":\",0) == -1)
      s_logDir = PROJ_PATH + \"data\";
    else if(s_logDir == \"\")
      s_logDir = PROJ_PATH + \"data\";
  
    if(_WIN32)
      strreplace(s_logDir,\"/\",\"\\\\\");

  
    setMultiValue(\"pmonUser\",\"text\",s_pmonUser,
                  \"pmonPassword\",\"text\",s_pmonPassword,
                  \"active\",\"state\",0,b_active,
                  \"reportInterval\",\"items\",di_interval,
                  \"reportInterval\",\"text\",i_interval,
                  \"logDir\",\"text\",s_logDir,
                  \"idleCPU\",\"state\",0,b_idleCPU,
                  \"separator\",\"items\",ds_separator,
                  \"separator\",\"text\",s_separator,
                  \"msgInfo\",\"state\",0,b_msgInfo);
  }
  if(!isRedundant())
    setValue(\"register\",\"registerVisible\",1,0);
    
  if(b_active)
  {
    // set all objects inactive except the \"Cancel\" button
    setMultiValue(\"pmonUser\",\"enabled\",false,
                  \"pmonPassword\",\"enabled\",false,
                  \"reportInterval\",\"enabled\",false,
                  \"logDir\",\"enabled\",false,
                  \"idleCPU\",\"enabled\",false,
                  \"separator\",\"enabled\",false,
                  \"msgInfo\",\"enabled\",false,
                  \"pathBrowser\",\"enabled\",false,
                  \"register\",\"enabled\",false);
    
    setMultiValue(\"ok\",\"enabled\",false,
                  \"apply\",\"enabled\",false);
  }
}" 0
