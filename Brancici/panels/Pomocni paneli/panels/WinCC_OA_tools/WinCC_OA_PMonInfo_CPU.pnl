E "NFR_saveSettings()
{
  dyn_bool db_selectedManagers1, db_selectedManagers2;
  string s_pmonUser, s_pmonPassword;
  int i_interval;
  bool b_active, b_idleCPU, b_msgInfo;
  string s_logDir, s_separator;
  string s_error;
  bool b_visible;

  getMultiValue(\"manTable\",\"getColumnN\",2,db_selectedManagers1,
                \"pmonUser\",\"text\",s_pmonUser,
                \"pmonPassword\",\"text\",s_pmonPassword,
                \"active\",\"state\",0,b_active,
                \"reportInterval\",\"text\",i_interval,
                \"logDir\",\"text\",s_logDir,
                \"idleCPU\",\"state\",0,b_idleCPU,
                \"separator\",\"text\",s_separator,
                \"msgInfo\",\"state\",0,b_msgInfo);

  // read the information for the selected managers in a redundant system
  if(isRedundant())
  {
    getMultiValue(\"manTable_2\",\"getColumnN\",2,db_selectedManagers2);
  }
  
  // check if the settings are correct and deactivate recording if necessary
  if(b_active == 1)
  {
    // check if the manager table is visible
    getValue(\"manTable\",\"visible\",b_visible);
    if(b_visible == false)
    {
      s_error = \"Host1: \" + getCatStr(\"WinCC_OA_tools\",\"noPMonInfo\");
      ChildPanelOnCentralModal(\"vision/MessageWarning\",\"Information\",makeDynString(s_error));
      b_active = 0;
      setValue(\"active\",\"state\",0,b_active);
    }
  }    
   
  if(isRedundant() && b_active == 1)
  {
    getValue(\"manTable_2\",\"visible\",b_visible);
    if(b_visible == false)
    {
      s_error = \"Host2: \" + getCatStr(\"WinCC_OA_tools\",\"noPMonInfo\");
      ChildPanelOnCentralModal(\"vision/MessageWarning\",\"Information\",makeDynString(s_error));
      b_active = 0;
      setValue(\"active\",\"state\",0,b_active);
    }
  }

  // check if a log-directory was configured
  if(s_logDir == \"\" && b_active == 1)
  {
    s_error = getCatStr(\"Console\",\"3551\");
    ChildPanelOnCentralModal(\"vision/MessageWarning\",\"Information\",makeDynString(s_error));
    b_active = 0;
    setValue(\"active\",\"state\",0,b_active);
  }
  
  if(!dynContains(db_selectedManagers1,true) && b_idleCPU == 0 && b_active == 1)
  {
    s_error = getCatStr(\"WinCC_OA_tools\",\"noOptionSelected\") + \"Host1\";
    ChildPanelOnCentralModal(\"vision/MessageWarning\",\"Information\",makeDynString(s_error));
    b_active = 0;
    setValue(\"active\",\"state\",0,b_active);
  }
  
  if(isRedundant() && b_active == 1)
  {
    if(!dynContains(db_selectedManagers2,true) && b_idleCPU == 0)
    {
      s_error = getCatStr(\"WinCC_OA_tools\",\"noOptionSelected\") + \"Host2\";
      ChildPanelOnCentralModal(\"vision/MessageWarning\",\"Information\",makeDynString(s_error));
    }
  }

  if(b_active == 1)
  {
    // set all objects inactive except the \"Cancel\" button
    setMultiValue(\"pmonUser\",\"enabled\",false,
                  \"pmonPassword\",\"enabled\",false,
                  \"reportInterval\",\"enabled\",false,
                  \"logDir\",\"enabled\",false,
                  \"idleCPU\",\"enabled\",false,
                  \"separator\",\"enabled\",false,
                  \"msgInfo\",\"enabled\",false,
                  \"pathBrowser\",\"enabled\",false,
                  \"register\",\"enabled\",false);
    
    setMultiValue(\"ok\",\"enabled\",false,
                  \"apply\",\"enabled\",false);
  }
  else
  {
    // set all objects active
    setMultiValue(\"pmonUser\",\"enabled\",true,
                  \"pmonPassword\",\"enabled\",true,
                  \"reportInterval\",\"enabled\",true,
                  \"logDir\",\"enabled\",true,
                  \"idleCPU\",\"enabled\",true,
                  \"separator\",\"enabled\",true,
                  \"msgInfo\",\"enabled\",true,
                  \"pathBrowser\",\"enabled\",true,
                  \"register\",\"enabled\",true);
    
    setMultiValue(\"ok\",\"enabled\",true,
                  \"apply\",\"enabled\",true);
  }
  if(isRedundant())
  {
    dpSetWait(\"NFR_PMonInfo_CPU.selectedManagers\",db_selectedManagers1,
              \"NFR_PMonInfo_CPU.pmonUser\",s_pmonUser,
              \"NFR_PMonInfo_CPU.pmonPassword\",s_pmonPassword,
              \"NFR_PMonInfo_CPU.CPU_checkInterval\",i_interval,
              \"NFR_PMonInfo_CPU.logDirectory\",s_logDir,
              \"NFR_PMonInfo_CPU.getCPUIdle\",b_idleCPU,
              \"NFR_PMonInfo_CPU.separator\",s_separator,
              \"NFR_PMonInfo_CPU.recordMsgInfo\",b_msgInfo,
              \"NFR_PMonInfo_CPU.active\",b_active,
              \"NFR_PMonInfo_CPU_2.selectedManagers\",db_selectedManagers2);
  }
  else
  {
    dpSetWait(\"NFR_PMonInfo_CPU.selectedManagers\",db_selectedManagers1,
              \"NFR_PMonInfo_CPU.pmonUser\",s_pmonUser,
              \"NFR_PMonInfo_CPU.pmonPassword\",s_pmonPassword,
              \"NFR_PMonInfo_CPU.CPU_checkInterval\",i_interval,
              \"NFR_PMonInfo_CPU.logDirectory\",s_logDir,
              \"NFR_PMonInfo_CPU.getCPUIdle\",b_idleCPU,
              \"NFR_PMonInfo_CPU.separator\",s_separator,
              \"NFR_PMonInfo_CPU.recordMsgInfo\",b_msgInfo,
              \"NFR_PMonInfo_CPU.active\",b_active);
  }
