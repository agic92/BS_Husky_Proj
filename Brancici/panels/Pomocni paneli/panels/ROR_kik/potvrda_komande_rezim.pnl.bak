"#uses \"rasvjeta_komande_kik.ctl\"

main()
{
  this.enabled = false;
  string upjesno_postavljanje = latinToCyrillic(\"Režim upravljanja uspešno promenjen\");
  string greska = latinToCyrillic(\"GREŠKA U KOMUNIKACIJI SA ORMANOM\\nRežim upravljanja nije promenjen\");  
  string greska_daljinsko = latinToCyrillic(\"GREŠKA\\nPrekidači na ormanima nisu \\npodešeni na daljinsko upravljanje\");  
  
  bool automatski, provjera;
  dpGet(\"SysKik:rasvjeta_desna_cijev.automatsko_upravljanje\", automatski);
  dyn_string greske = provjeriRezimSvihOrmaraKik();
  
  if (!automatski) {
    if (dynlen(greske) > 0) {
      txt_info.text = greska_daljinsko;
      txt_pitanje.visible = false;
      btn_ok.visible = false;
      btn_cancel.visible = false;
      txt_info.visible = true;
      delay(2);
      PanelOff();
      return;
    }
  }
  
  dpSet(\"SysKik:rasvjeta_desna_cijev.automatsko_upravljanje\", !automatski);
  delay(1);  
  dpGet(\"SysKik:rasvjeta_desna_cijev.automatsko_upravljanje\", provjera);
      
  if (provjera != automatski) {
     txt_info.text = upjesno_postavljanje;
  }
  else {
    txt_info.text = greska;
  }

  txt_pitanje.visible = false;
  btn_ok.visible = false;
  btn_cancel.visible = false;
  txt_info.visible = true;
  delay(2);
  PanelOff();
}

