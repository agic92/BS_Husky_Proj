void EP_textFieldInCB(string dp, int iNewValue)
{
  if(iNewValue == 10)
  {
    setValue(\"\", \"text\",\"Suho\");
    this.backCol(\"STD_grey_middle\");
  }  
  else if(iNewValue == 15)
  {
     setValue(\"\", \"text\",\"Vlazno\");
     this.backCol(\"warnKamQuit\");
  }
  else if(iNewValue == 20)
  {
     setValue(\"\", \"text\",\"Mokro\");
     this.backCol(\"{236,149,55}\");
  }
  else if(iNewValue == 25)
  {
     setValue(\"\", \"text\",\"Vlazno sa soli\");
     this.backCol(\"warnKamQuit\");
  }
  else if(iNewValue == 30)
  {
     setValue(\"\", \"text\",\"Mokro sa soli\");
     this.backCol(\"{236,149,55}\");
  }
   else if(iNewValue==35)
  {
     setValue(\"\", \"text\",\"Led\");
     this.backCol(\"STD_value_not_ok\");
  }
   else if(iNewValue==40)
  {
     setValue(\"\", \"text\",\"Snijeg\");
     this.backCol(\"STD_value_not_ok\");
  }
   else if(iNewValue==45)
  {
     setValue(\"\", \"text\",\"Mraz\");
     this.backCol(\"STD_value_not_ok\");
  }
   else 
   { 
   setValue(\"\", \" Kriticno\" );
   this.backCol(\"\");
   }
}
