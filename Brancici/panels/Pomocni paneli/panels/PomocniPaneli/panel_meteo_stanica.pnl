{
  
  if(iNewValue==0)
  {
  setValue(\"\", \"text\",\"Nema padavina\");
  }  
  else if(iNewValue==60 || iNewValue==1)
  {
     setValue(\"\", \"text\",\"Kiša\"); 
  }
