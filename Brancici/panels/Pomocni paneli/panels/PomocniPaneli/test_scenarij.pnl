string sys_name = \"SysSarani:\";

void setAllSign()
{
  dyn_dyn_anytype xx;
  
  dpQuery(\"SELECT '_online.._value' FROM '\" + sys_name + \"VMS1-PS2.command.izbor_znaka' WHERE _DPT = \\\"Sign\\\"\", xx);  
        
  dyn_int config;
    
  for(int i = 2; i <= dynlen(xx); i++){
     string nazivZnaka = dpSubStr(xx[i][1], DPSUB_DP);
     dpGet(sys_name + nazivZnaka + \".config\", config);
     if(dynlen(config) != 0){
         //DebugTN(\"DATAPOINT: \" + xx[i][1] + \"   SET: \" + config[1]);
         dpSet(xx[i][1], config[1]);
     }
  }

