//Popunjava RCT sa podacima
workCB(string ident, dyn_dyn_anytype tab)
{
  
  int list = 1;
  dyn_string ds;

  for(int i=2;i<=dynlen(tab);i++)
  { 
    if (dpTypeName(tab[i][1]) == \"_Rct\")
      dynAppend(ds, dpSubStr(tab[i][1], DPSUB_DP));
  }

  dynSortAsc(ds);
  //cmbRct.items = ds
 
  scenarij_izbor.items = ds;
  loadSelectedType();
  
}
