"#uses \"latin_to_cyrillic.ctl\"
#uses \"latin_to_cyrillic.ctl\"
#uses \"rasvjeta_komande.ctl\"

main()
{
  string ormar = \"RO-A1-1D\";
  dyn_string greske = provjeriRezimJednogOrmaraAdaptivne(ormar);
  if (dynlen(greske)==0) {
    EP_childPanelOn();
  }
  else {
    
    string greska = \"Sljedeće komande na ormanu \" + ormar + \" \\nnisu postavljene na daljinsko upravljanje: \\n\";
    for (int i=1;i<=dynlen(greske);i++) {
      greska += greske[i] + \"\\n\";
    }
    izbaciGresku(greska);
  }
}

void EP_childPanelOn()
{
  string adaptivna;
  getValue(\"adaptivnaD\", \"text\", adaptivna);
  float nivo;  
  switch(adaptivna) {
    case \"0%\":
      nivo = 0;
      break;
    case \"12.5%\":
      nivo = 12.5;
      break;
    case \"25%\":
      nivo = 25;
      break;
    case \"50%\":
      nivo = 50;
      break;
    case \"75%\":
      nivo = 75;
      break;
    case \"100%\":
      nivo = 100;
      break;
  }
  ChildPanelOnRelativ(\"ROR/potvrda_komande_adaptivna.pnl\", latinToCyrillic(\"Potvrda komande\"),makeDynString(\"$CIJEV:\" + \"D\", \"$NIVO_ADAPTIVNE:\" + nivo),50, -90);
}" 0
