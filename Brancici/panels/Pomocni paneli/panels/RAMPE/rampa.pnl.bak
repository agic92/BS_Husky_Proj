"#uses \"latin_to_cyrillic.ctl\"

main()
{
  string sys_name = \"SysSarani:\";
  int rampa_podignuta;
  
  dpGet(sys_name + $RAMPA + \".state.rampa_podignuta\", rampa_podignuta);
  
  if (rampa_podignuta) {
    dpSet (sys_name + $RAMPA + \".cmd.spusti_rampu\", 1);
    delay(1);
    dpSet (sys_name + $RAMPA + \".cmd.spusti_rampu\", 0); 
  }
  else {
    dpSet (sys_name + $RAMPA + \".cmd.podigni_rampu\", 1);
    delay(1);
    dpSet (sys_name + $RAMPA + \".cmd.podigni_rampu\", 0);
  }
  dpSet(sys_name + $RAMPA + \".state.rampa_podignuta\", !rampa_podignuta);
}
