{
  if(bInvalid)
  setValue(\"\", \"color\", \"_dpdoesnotexist\");
  else
  setValue(\"\", \"color\", \"\");
  
  DebugN(\"U funkciji\");
  setValue(\"\", \"color\", sNewColor);
}

