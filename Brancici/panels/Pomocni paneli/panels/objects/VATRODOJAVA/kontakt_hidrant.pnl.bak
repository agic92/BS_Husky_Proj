void EP_setBackColorCB(string dp1, bool alarm, string dp2, bool greska, 
                       string dp3, bool iskljucen)
{
  if(alarm){ 
    setValue(\"vatra1\", \"backCol\", \"S7_toleranceWentUnq\");
    setValue(\"vatra2\", \"backCol\", \"S7_toleranceWentUnq\");
    setValue(\"vatra3\", \"backCol\", \"S7_toleranceWentUnq\");
    setValue(\"vatra4\", \"backCol\", \"S7_toleranceWentUnq\");
  }
  
  else if (greska) {
    setValue(\"vatra1\", \"backCol\", \"warningCamWentUna\");
    setValue(\"vatra2\", \"backCol\", \"warningCamWentUna\");
    setValue(\"vatra3\", \"backCol\", \"warningCamWentUna\");
    setValue(\"vatra4\", \"backCol\", \"warningCamWentUna\");
  }
  else if (iskljucen) {
    setValue(\"vatra1\", \"backCol\", \"SiemensNaturalYellowLight\");
    setValue(\"vatra2\", \"backCol\", \"SiemensNaturalYellowLight\");
    setValue(\"vatra3\", \"backCol\", \"SiemensNaturalYellowLight\");
    setValue(\"vatra4\", \"backCol\", \"SiemensNaturalYellowLight\");
  }
  else{
    setValue(\"vatra1\", \"backCol\", \"\");
    setValue(\"vatra2\", \"backCol\", \"\");
    setValue(\"vatra3\", \"backCol\", \"\");
    setValue(\"vatra4\", \"backCol\", \"\");
  }
}




