void EP_setColor()
{
  int rc;

  if( !dpExists(sys_name + $ROV + \".alarm.kvar_magla:_alert_hdl.._act_state_color\"))
  {
    setValue(\"\", \"color\", \"_dpdoesnotexist\");
    return;
  }

  rc = dpConnect(\"EP_setColorCB\", 
                 sys_name + $ROV + \".alarm.kvar_magla\", sys_name + $ROV + \".mjerenja.magla\");

  if ( sdGetLastError() < 0 || rc != 0)
    setValue(\"\", \"color\", \"_dpdoesnotexist\");
}

void EP_setColorCB(string dpSource, bool kvar_magla,string dp2, float magla)
{
  int alarmMagla = (int)magla;
  setValue(\"\", \"color\", \"\");
  
  if(alarmMagla < 60) setValue(\"\", \"color\", \"stoerKamUnq\");
  else if(alarmMagla < 100) setValue(\"\", \"color\", \"stoerKamQuit\");  
  else if(alarmMagla < 170) setValue(\"\", \"color\", \"SiemensNaturalYellowLight\"); 
  else if(alarmMagla < 250) setValue(\"\", \"color\", \"warnKamQuit\");
  else setValue(\"\", \"color\", \"\");
  
  if(kvar_magla) setValue(\"\", \"color\", \"stoerKamUnq\");
}
