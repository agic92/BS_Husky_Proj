main()
{   
    dpConnect(\"postaviNaTabelu\", sys_name + $ZNAK_DP + \".command.izbor_znaka\");
    addGlobal(\"selektovani_znak\", INT_VAR); //addGlobal
}

bool initialized = false;

void postaviNaTabelu(string dp, int izbor_znaka)
{    
    dyn_int config_niz;
    dpGet(sys_name + $ZNAK_DP + \".config\", config_niz);

    this.adjustColumn(0);    
    for(int i = 1; i <= dynlen(config_niz); i++)
    {      
      if(!initialized)
      { 
        this.appendLine(\"Znak\", \"\", \"Objasnjenje\",\"\");       
      }
 
      this.cellFillRC(i-1, \"Znak\", \"[pattern,[fit,wmf,Uznakovi/Znak\" + config_niz[i] + \"1.wmf]]\");
      this.cellValueRC(i-1, \"Objasnjenje\", vratiNazivZnaka(config_niz[i]));     
      this.cellFillRC(i-1, \"#1\", \"\");
      
      if(izbor_znaka == config_niz[i]){
          this.cellFillRC(i-1,\"#1\",\"[pattern,[fit,png,Correct_sign.png]]\");
          selektovani_znak = i;
      }
      if(izbor_znaka == 0) selektovani_znak = -1;
      
    }   
    
    initialized = true;
