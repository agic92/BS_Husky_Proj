    setValue(\"\", \"foreCol\", \"<stoerKamQuit,2,STD_undifined,2>\");
    string txt = latinToCyrillic(\"Stanje uređaja: Požar\");
    setValue(\"\", \"text\", txt); 
  }
  else{
    setValue(\"\", \"foreCol\", \"\");
    string txt2 = latinToCyrillic(\"Stanje uređaja: Uredno\");
    setValue(\"\", \"text\", txt2); 
  }
}



