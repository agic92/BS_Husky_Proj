EP_setColor()
{
dyn_errClass err;
  
  dpConnect(\"EP_setColorCB\", 
            sys_name + $METEO_DP + \".response.alarm.Status_Meteoroloska_Stanica_Prisutna\",
            sys_name + $METEO_DP + \".response.alarm.Status_Prisutan_Senzor_Ceste\",
            sys_name + $METEO_DP + \".response.alarm.Status_Senzor_Vidljivosti_Prisutan\",
            sys_name + $METEO_DP + \".response.alarm.Poledica\",
            sys_name + $METEO_DP + \".response.alarm.Magla\",
            sys_name + $METEO_DP + \".response.alarm.Sklizak_Kolovoz\",
            sys_name + $METEO_DP + \".response.alarm.Vjetar\",
            sys_name + $METEO_DP + \".response.alarm.Snijeg\",
            sys_name + $METEO_DP + \".response.alarm.Snijeg:_online.._bad\"
            );
                 
  err = getLastError();
  if (dynlen(err) > 0)
    setValue(\"\", \"color\", \"_dpdoesnotexist\");

}
              
EP_setColorCB(string dp2,bool meteo_stanica_prisutna,string dp3,bool senzor_ceste,string dp4,bool senzor_vidljivosti,string dp5,bool poledica,string dp6,int magla,string dp7,int sklizak_kolovoz,string dp8,int vjetar,string dp9,int snijeg, string dp10, bool bInvalid)
{
   if(bInvalid)
     setValue(\"\", \"color\", \"_dpdoesnotexist\");
   else
     setValue(\"\", \"color\", \"\");
   
   if(!meteo_stanica_prisutna || !senzor_ceste || !senzor_vidljivosti) setValue(\"\", \"color\", \"disturbanceWentUna\");
   else if(poledica>=4 || magla<60 || vjetar==5 || sklizak_kolovoz>3000) setValue(\"\", \"color\", \"stoerKamUnq\");  
   else if(poledica==3 || magla<100 || vjetar==4 || sklizak_kolovoz>1500) setValue(\"\", \"color\", \"stoerKamQuit\"); 
   else if(poledica==2 || magla<170 || vjetar==3  || sklizak_kolovoz>500) setValue(\"\", \"color\", \"SiemensNaturalYellowLight\");
   else if(poledica==1 || magla<250 || vjetar==2 || snijeg==1  || sklizak_kolovoz>200) setValue(\"\", \"color\", \"warnKamQuit\");
   else setValue(\"\", \"color\", \"\");
}
