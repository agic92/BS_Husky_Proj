"main()
{
  bool auto_upravljanje;
  dpGet(\"SysSarani:ventilacija_desna_cijev.state.automatsko_upravljanje\", auto_upravljanje);
  
  promijeniRezimUpravljanja(!auto_upravljanje);
}

promijeniRezimUpravljanja(bool rezim) {
  dpSet(\"SysSarani:ventilacija_desna_cijev.state.automatsko_upravljanje\", rezim);
  dpSet(\"SysSarani:ventilacija_lijeva_cijev.state.automatsko_upravljanje\", rezim);
    
    string query = \"SELECT '_original.._value' FROM '*.cmd.sdv.manuelno.rezim' WHERE _DPT = \\\"VENTILATOR\\\"\";
    dyn_dyn_anytype list;
    dpQuery(query, list);
  
    string sys_name = \"SysSarani:\";
    string value, dpe;
    
    for(int i = 2; i <= dynlen(list); i++) 
    {    
      value = list[i][2];
      dpe = list[i][1];
      dpe = dpSubStr(dpe, DPSUB_DP);  
      dpSet(sys_name + dpe + \".cmd.sdv.manuelno.rezim\", !rezim);
    }
    this.enabled = false;
    delay(1);
    
    PanelOff();
}" 0
