"sizePolicy" "Fixed Fixed"
"dashclr"N "_Transparent"
"antiAliased" "0"
"#uses \"latin_to_cyrillic.ctl\"

main()
{
  EP_setBackColor();
}

void EP_setBackColor()
{
  int rc;

  if( !dpExists(sys_name + $SOS_DP + \".response.state:_online.._value\"))
  {
    setValue(\"\", \"color\", \"_dpdoesnotexist\");
    return;
  }

  rc = dpConnect(\"EP_setBackColorCB\", 
                  sys_name + $SOS_DP + \".response.state:_online.._value\");

  if ( sdGetLastError() < 0 || rc != 0)
    setValue(\"\", \"color\", \"_dpdoesnotexist\");
}


void EP_setBackColorCB(string dpSource, int iNewValue)
{
  string txt;
  if(iNewValue == 0)
  {
      setValue(\"ELLIPSE4\", \"backCol\", \"\");
      txt = latinToCyrillic(\"Status: REGISTROVAN\");
      setValue(\"PRIMITIVE_TEXT11\", \"text\", txt);
  }
  else if (iNewValue == 1)
  {   
      setValue(\"ELLIPSE4\", \"backCol\", \"green\");
      txt = latinToCyrillic(\"Status: U RAZGOVORU\");
      setValue(\"PRIMITIVE_TEXT11\", \"text\",txt);
  }
  else if (iNewValue == 2)
  {
      setValue(\"ELLIPSE4\", \"backCol\", \"STD_valve_opening\");
      txt = latinToCyrillic(\"Status: ZVONI\");
      setValue(\"PRIMITIVE_TEXT11\", \"text\", txt);
  }
   else if (iNewValue == 3)
   {
      setValue(\"ELLIPSE4\", \"backCol\", \"stoerKamQuit\");
      txt = latinToCyrillic(\"Status: NEREGULARNO STANJE\");
      setValue(\"PRIMITIVE_TEXT11\", \"text\", txt);
   }
   else if (iNewValue == 4)
   {
      setValue(\"ELLIPSE4\", \"backCol\", \"yellow\");
      txt = latinToCyrillic(\"Status: KVAR\");
      setValue(\"PRIMITIVE_TEXT11\", \"text\", txt);
   }
   else 
   {
     setValue(\"PRIMITIVE_TEXT11\", \"backCol\", \"\");
     txt = latinToCyrillic(\"Status: NEPOZNATO\");    
     setValue(\"ELLIPSE4\", \"text\", txt);  
   }
}


