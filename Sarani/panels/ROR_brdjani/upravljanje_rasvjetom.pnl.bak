"#uses \"latin_to_cyrillic.ctl\"
#uses \"rasvjeta_komande.ctl\"
main()
{
  dpConnect(\"podesiPrikaz\", \"SysSarani:RO-A1-1D.value.aktivni_nivo\", \"SysSarani:RO-A1-2L.value.aktivni_nivo\", 
                            \"SysSarani:bazna.state.OS1.bazna\", \"SysSarani:bazna.state.OS1.bazna_redukcija\",
                            \"SysSarani:bazna.state.B-2L.bazna\", \"SysSarani:bazna.state.B-2L.bazna_redukcija\",
                            \"SysSarani:bazna.state.B-1D.bazna\", \"SysSarani:bazna.state.B-1D.bazna_redukcija\",
                            \"SysSarani:bazna.state.OS2.bazna\", \"SysSarani:bazna.state.OS2.bazna_redukcija\",
                            \"SysSarani:bazna.state.OS1.prilazna_zona1\", \"SysSarani:bazna.state.OS2.prilazna_zona2\");
  
}

void podesiPrikaz(string dp1, float nivoD, string dp2, float nivoL, 
                  string dp3, bool os1, string dp4, bool os1_p,
                  string dp5, bool b2l, string dp6, bool b2l_p,
                  string dp7, bool b1d, string dp8, bool b1d_p,
                  string dp9, bool os2, string dp10, bool os2_p,
                  string dp11, bool pz1, string dp12, bool pz2) {
  
  int transparentnostD, transparentnostL;
  int bazna_value1, bazna_value2, bazna_value3, bazna_value4;
  int prilazna1_value, prilazna2_value;
  
if (os1 && !os1_p)  
  bazna_value1 = 50;
else if (os1 && os1_p)  
  bazna_value1 = 25;
else if (!os1 && !os1_p)  
  bazna_value1 = 0;

if (b2l && !b2l_p)  
  bazna_value2 = 50;
else if (b2l && b2l_p)  
  bazna_value2 = 25;
else if (!b2l && !b2l_p)  
  bazna_value2 = 0;

if (b1d && !b1d_p)  
  bazna_value3 = 50;
else if (b1d && b1d_p)  
  bazna_value3 = 25;
else if (!b1d && !b1d_p)  
  bazna_value3 = 0;

if (os2 && !os2_p)  
  bazna_value4 = 50;
else if (os2 && os2_p)  
  bazna_value4 = 25;
else if (!os2 && !os2_p)  
  bazna_value4 = 0;

if (pz1)  
  prilazna1_value = 40;
else
  prilazna1_value = 0;

if (pz2)  
  prilazna2_value = 40;
else
  prilazna2_value = 0;

  switch(nivoD) {
    case 100:
      transparentnostD = 255;
      break;
    case 75:
      transparentnostD = 220;
      break; 
    case 50:
      transparentnostD = 183;
      break;
    case 25:
      transparentnostD = 150;
      break;
    case 12.5:
      transparentnostD = 110;
      break;
    default:
      transparentnostD = bazna_value1;
      break;
  }
  
  switch(nivoL) {
    case 100:
      transparentnostL = 255;
      break;
    case 75:
      transparentnostL = 220;
      break; 
    case 50:
      transparentnostL = 183;
      break;
    case 25:
      transparentnostL = 150;
      break;
    case 12.5:
      transparentnostL = 110;
      break;
    default:
      transparentnostL = bazna_value4;
      break;
  }
  
//   
//   
//   
  adaptivnarasvjeta1.fill = \"[gradient,[linear,pad,-0.675,0.446,0.94,0.446,0,{255,255,0,\" + transparentnostD + \"},1,{255,255,0,\" + bazna_value1 + \"}]]\";
  adaptivna2.fill = \"[gradient,[linear,pad,-0.675,0.446,0.94,0.446,0,{255,255,0,\" + transparentnostL + \"},1,{255,255,0,\" + bazna_value4 + \"}]]\";
//   pocetak2.fill = \"[gradient,[linear,pad,-0.675,0.446,0.94,0.446,0,{255,255,0,\" + transparentnost + \"},1,{255,255,0,\" + bazna_value + \"}]]\";  
  zona1_1.backCol = \"{255,255,0,\" + bazna_value1 + \"}\";       
  zona1_2.backCol = \"{255,255,0,\" + bazna_value1 + \"}\";  
  zona2.backCol = \"{255,255,0,\" + bazna_value2 + \"}\";
  zona3.backCol = \"{255,255,0,\" + bazna_value3 + \"}\";
  zona4_1.backCol = \"{255,255,0,\" + bazna_value4 + \"}\";
  zona4_2.backCol = \"{255,255,0,\" + bazna_value4 + \"}\";
  
  prilaznaZona1.backCol = \"{255,255,0,\" + prilazna1_value + \"}\";       
  prilaznaZona2.backCol = \"{255,255,0,\" + prilazna2_value + \"}\";  
  prilaznaZona1_1.backCol = \"{255,255,0,\" + prilazna1_value + \"}\";       
  prilaznaZona2_1.backCol = \"{255,255,0,\" + prilazna2_value + \"}\";  
//   ostatak2.backCol = \"{255,255,0,\" + bazna_value + \"}\";  
}

