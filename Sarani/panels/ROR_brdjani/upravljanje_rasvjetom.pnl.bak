work (string dp1, bool bazna, string dp2, bool redukcija,
      string dp3, bool bazna_ups, string dp4, bool redukcija_ups)
{
  string nivo;
  if (bazna && bazna_ups && !redukcija_ups && !redukcija)
    nivo = \"trenutni nivo rasvete: 100%\";
  else if (bazna && bazna_ups && redukcija_ups && redukcija) {
    nivo = \"trenutni nivo rasvete: 50%\";
  }
  else if (!bazna && bazna_ups && !redukcija_ups) {
    nivo = \"bazna rasveta na UPS-u\";
  }
  else if (!bazna && bazna_ups && redukcija_ups) {
    nivo = \"bazna rasveta na UPS-u, uključena redukcija\";
  }
  else {
    nivo = \"trenutni nivo rasvete: 0%\";
  }
    
  this.text = latinToCyrillic(\"ZONA 1 - \" + nivo);
