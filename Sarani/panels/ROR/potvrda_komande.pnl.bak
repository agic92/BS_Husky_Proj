#uses \"rasvjeta_komande.ctl\"

main()
{
  this.enabled = false;
  string upjesno_postavljanje = latinToCyrillic(\"Nivo rasvete uspešno promenjen\");
  string greska = latinToCyrillic(\"GREŠKA U KOMUNIKACIJI SA ORMANOM\\nNivo rasvete nije promenjen\");
  bool os1, os1_p, os1_ups, os1_ups_p, b1d, b1d_p, b1d_ups, b1d_ups_p, b2l, b2l_p, 
       b2l_ups, b2l_ups_p, os2, os2_p, os2_ups, os2_ups_p;
  switch($ZONA) {
    case 1:
      dpGet(\"SysSarani:bazna.state.OS1.bazna\", os1);
      dpGet(\"SysSarani:bazna.state.OS1.bazna_redukcija\", os1_p);
      
      dpGet(\"SysSarani:bazna.state.OS1/UPS.bazna\", os1_ups);
      dpGet(\"SysSarani:bazna.state.OS1/UPS.bazna_redukcija\", os1_ups_p);
      
      podesiNivoBazne(\"OS1\", $NIVO_BAZNE);
      podesiNivoBazne(\"OS1/UPS\", $NIVO_BAZNE);
      
      delay(1);
      if (!provjeriPostavljanjeBazne(\"OS1\",$NIVO_BAZNE) || !provjeriPostavljanjeBazne(\"OS1/UPS\",$NIVO_BAZNE)) {
        dpSet(\"SysSarani:bazna.cmd.run.OS1.bazna\", os1);
        dpSet(\"SysSarani:bazna.cmd.run.OS1.bazna_redukcija\", os1_p);
      
        dpSet(\"SysSarani:bazna.cmd.run.OS1/UPS.bazna\", os1_ups);
        dpSet(\"SysSarani:bazna.cmd.run.OS1/UPS.bazna_redukcija\", os1_ups_p);  
        
        txt_info.text = greska;
      }
      else {
        txt_info.text = upjesno_postavljanje;
      }
      break;
    case 2:
      dpGet(\"SysSarani:bazna.state.B-2L.bazna\", b2l);
      dpGet(\"SysSarani:bazna.state.B-2L.bazna_redukcija\", b2l_p);
      
      dpGet(\"SysSarani:bazna.state.B-2L/UPS.bazna\", b2l_ups);
      dpGet(\"SysSarani:bazna.state.B-2L/UPS.bazna_redukcija\", b2l_ups_p);
      
      podesiNivoBazne(\"B-2L\", $NIVO_BAZNE);
      podesiNivoBazne(\"B-2L/UPS\", $NIVO_BAZNE);
      
      delay(1);
      if (!provjeriPostavljanjeBazne(\"B-2L\",$NIVO_BAZNE) || !provjeriPostavljanjeBazne(\"B-2L/UPS\",$NIVO_BAZNE)) {
        dpSet(\"SysSarani:bazna.cmd.run.B-2L.bazna\", b2l);
        dpSet(\"SysSarani:bazna.cmd.run.B-2L.bazna_redukcija\", b2l_p);
        
        dpSet(\"SysSarani:bazna.state.B-2L/UPS.bazna\", b2l_ups);
        dpSet(\"SysSarani:bazna.state.B-2L/UPS.bazna_redukcija\", b2l_ups_p);
        
        txt_info.text = greska;
      }
      else {
        txt_info.text = upjesno_postavljanje;
      }
      break;
    case 3:
      dpGet(\"SysSarani:bazna.state.B-1D.bazna\", b1d);
      dpGet(\"SysSarani:bazna.state.B-1D.bazna_redukcija\", b1d_p);
      
      dpGet(\"SysSarani:bazna.state.B-1D/UPS.bazna\", b1d_ups);
      dpGet(\"SysSarani:bazna.state.B-1D/UPS.bazna_redukcija\", b1d_ups_p);
      
      podesiNivoBazne(\"B-1D\", $NIVO_BAZNE);
      podesiNivoBazne(\"B-1D/UPS\", $NIVO_BAZNE);
      
      delay(1);
      if (!provjeriPostavljanjeBazne(\"B-1D\",$NIVO_BAZNE) || !provjeriPostavljanjeBazne(\"B-1D/UPS\",$NIVO_BAZNE)) {
        dpSet(\"SysSarani:bazna.cmd.run.B-1D.bazna\", b1d);
        dpSet(\"SysSarani:bazna.cmd.run.B-1D.bazna_redukcija\", b1d_p);
        
        dpSet(\"SysSarani:bazna.state.B-1D/UPS.bazna\", b1d_ups);
        dpSet(\"SysSarani:bazna.state.B-1D/UPS.bazna_redukcija\", b1d_ups_p);
        
        txt_info.text = greska;
      }
      else {
        txt_info.text = upjesno_postavljanje;
      }
      
      break;
    case 4:
      dpGet(\"SysSarani:bazna.state.OS2.bazna\", os2);
      dpGet(\"SysSarani:bazna.state.OS2.bazna_redukcija\", os2_p);
      
      dpGet(\"SysSarani:bazna.state.OS2/UPS.bazna\", os2_ups);
      dpGet(\"SysSarani:bazna.state.OS2/UPS.bazna_redukcija\", os2_ups_p);
      
      podesiNivoBazne(\"OS2\", $NIVO_BAZNE);
      podesiNivoBazne(\"OS2/UPS\", $NIVO_BAZNE);
      
      delay(1);
      if (!provjeriPostavljanjeBazne(\"OS2\",$NIVO_BAZNE) || !provjeriPostavljanjeBazne(\"OS2/UPS\",$NIVO_BAZNE)) {
        dpSet(\"SysSarani:bazna.cmd.run.OS2.bazna\", os1);
        dpSet(\"SysSarani:bazna.cmd.run.OS2.bazna_redukcija\", os1_p);
      
        dpSet(\"SysSarani:bazna.cmd.run.OS2/UPS.bazna\", os1_ups);
        dpSet(\"SysSarani:bazna.cmd.run.OS2/UPS.bazna_redukcija\", os1_ups_p);  
        
        txt_info.text = greska;
      }
      else {
        txt_info.text = upjesno_postavljanje;
      }
     
      break;
  }
  txt_pitanje.visible = false;
  btn_ok.visible = false;
  btn_cancel.visible = false;
  txt_info.visible = true;
  delay(2);
  PanelOff();
}" 0
